            "language_name": "english"
          },
          "transliteration": {
            "text": "khāshiʿatun",
            "language_name": "english"
          }
        },
        {
          "id": 1777,
          "position": 4,
          "audio_url": null,
          "char_type_name": "end",
          "code_v1": "ﭬ",
          "page_number": 592,
          "line_number": 5,
          "text": "ﭬ",
          "translation": {
