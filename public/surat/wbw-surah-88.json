        {
          "id": 1776,
          "position": 3,
          "audio_url": "wbw/088_002_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﭫ",
          "page_number": 592,
          "line_number": 5,
          "text": "ﭫ",
          "translation": {
