          },
          "transliteration": {
            "text": null,
            "language_name": "english"
          }
        }
      ],
      "translations": [
        {
          "id": 95876,
          "resource_id": 19,
          "text": "Scorched by burning fire,"
        },
        {
          "id": 370258,
          "resource_id": 79,
          "text": "горят в Огне пылающем [в Аду],"
        },
        {
          "id": 256162,
          "resource_id": 45,
          "text": "Они войдут в Огонь жаркий."
        },
        {
          "id": 145005,
          "resource_id": 27,
          "text": "einem sehr heißen Feuer ausgesetzt,"
        }
      ]
    },
