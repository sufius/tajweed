          "char_type_name": "word",
          "code_v1": "ﭝ",
          "page_number": 585,
          "line_number": 2,
          "text": "ﭝ",
          "translation": {
            "text": "Or",
            "language_name": "english"
          },
          "transliteration": {
            "text": "aw",
            "language_name": "english"
