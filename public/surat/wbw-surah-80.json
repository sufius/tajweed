          },
          "transliteration": {
            "text": "watawallā",
            "language_name": "english"
          }
        },
        {
          "id": 694,
          "position": 3,
          "audio_url": null,
          "char_type_name": "end",
          "code_v1": "ﭓ",
          "page_number": 585,
          "line_number": 2,
          "text": "ﭓ",
          "translation": {
