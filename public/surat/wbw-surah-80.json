      "juz_number": 30,
      "words": [
        {
          "id": 5455,
          "position": 1,
          "audio_url": "wbw/080_005_001.mp3",
          "char_type_name": "word",
          "code_v1": "ﭢ",
          "page_number": 585,
          "line_number": 3,
          "text": "ﭢ",
          "translation": {
            "text": "As for",
            "language_name": "english"
          },
          "transliteration": {
            "text": "ammā",
            "language_name": "english"
          }
        },
        {
          "id": 5456,
          "position": 2,
          "audio_url": "wbw/080_005_002.mp3",
          "char_type_name": "word",
          "code_v1": "ﭣ",
          "page_number": 585,
          "line_number": 3,
          "text": "ﭣ",
          "translation": {
            "text": "(him) who",
            "language_name": "english"
          },
          "transliteration": {
            "text": "mani",
            "language_name": "english"
          }
        },
        {
          "id": 5457,
          "position": 3,
          "audio_url": "wbw/080_005_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﭤ",
          "page_number": 585,
          "line_number": 3,
          "text": "ﭤ",
          "translation": {
            "text": "considers himself free from need",
            "language_name": "english"
          },
          "transliteration": {
            "text": "is'taghnā",
            "language_name": "english"
          }
        },
        {
          "id": 5458,
          "position": 4,
          "audio_url": null,
          "char_type_name": "end",
          "code_v1": "ﭥ",
          "page_number": 585,
          "line_number": 3,
          "text": "ﭥ",
          "translation": {
            "text": "(5)",
            "language_name": "english"
          },
          "transliteration": {
            "text": null,
            "language_name": "english"
          }
        }
      ],
      "translations": [
        {
          "id": 95606,
          "resource_id": 19,
          "text": "As for him who thinketh himself independent,"
        },
        {
          "id": 370050,
          "resource_id": 79,
          "text": "Что же касается того, кто не нуждается (в Вере, Истинном Знании и в твоем призыве),"
        },
        {
          "id": 255912,
          "resource_id": 45,
          "text": "Тому, кто решил, что он ни в чем не нуждается,"
        },
        {
          "id": 144736,
          "resource_id": 27,
          "text": "Was nun jemanden angeht, der sich für unbedürftig hält,"
        }
      ],
      "text_uthmani_tajweed": "أَ[g[مّ]َا مَنِ [h:14302[ٱ]سْتَغْنَىٰ",
      "text_uthmani_tajweed_parsed": "أَ<tajweed class=\"ghn\" data-type=\"ghunnah\" data-description=\"Ghunnah: 2 Vowels\" data-tajweed=\"\">مّ</tajweed>َا مَنِ <tajweed class=\"ham_wasl\" data-type=\"hamza-wasl\" data-description=\"Hamzat ul Wasl\" data-tajweed=\":14302\">ٱ</tajweed>سْتَغْنَىٰ"
    },
    {
