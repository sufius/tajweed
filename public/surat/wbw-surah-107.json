          "char_type_name": "word",
          "code_v1": "ﮄ",
          "page_number": 602,
          "line_number": 11,
          "text": "ﮄ",
          "translation": {
            "text": "[the] small kindnesses",
            "language_name": "english"
          },
          "transliteration": {
            "text": "l-māʿūna",
            "language_name": "english"
          }
        },
        {
          "id": 7671,
          "position": 3,
          "audio_url": null,
          "char_type_name": "end",
          "code_v1": "ﮅ",
          "page_number": 602,
          "line_number": 11,
          "text": "ﮅ",
          "translation": {
