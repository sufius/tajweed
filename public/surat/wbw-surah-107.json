        {
          "id": 370490,
          "resource_id": 79,
          "text": "которые делают (свои благие дела) напоказ (перед людьми)"
        },
        {
          "id": 256555,
          "resource_id": 45,
          "text": "которые лицемерят"
        },
        {
          "id": 145367,
          "resource_id": 27,
          "text": "denjenigen, die dabei (nur) gesehen werden wollen;"
        }
      ]
    },
    {
      "id": 6204,
      "verse_number": 7,
      "verse_key": "107:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
