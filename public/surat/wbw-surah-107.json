      "page_number": 602,
      "juz_number": 30,
      "words": [
        {
          "id": 7669,
          "position": 1,
          "audio_url": "wbw/107_007_001.mp3",
          "char_type_name": "word",
          "code_v1": "ﮃ",
          "page_number": 602,
          "line_number": 11,
          "text": "ﮃ",
          "translation": {
            "text": "And they deny",
            "language_name": "english"
          },
          "transliteration": {
