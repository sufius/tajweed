      "juz_number": 30,
      "words": [
        {
          "id": 699,
          "position": 1,
          "audio_url": "wbw/082_001_001.mp3",
          "char_type_name": "word",
          "code_v1": "ﭑ",
          "page_number": 587,
          "line_number": 2,
          "text": "ﭑ",
          "translation": {
            "text": "When",
            "language_name": "english"
          },
          "transliteration": {
