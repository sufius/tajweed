      "text_uthmani": " وَٱلْعَصْرِ",
      "text_uthmani_tajweed": "وَ[h:9999[ٱ]لْعَصْرِ",
      "text_uthmani_tajweed_parsed": "وَ<tajweed class='ham_wasl' data-type='hamza-wasl' data-description='Hamzat ul Wasl' data-tajweed=':9999'>ٱ</tajweed>لْعَصْرِ",
      "text_uthmani_transcribed": "Wal-'aṣr(i).",
      "translations": [
        {
          "id": 89999,
          "resource_id": 19,
          "text": "By the declining day,"
        },
        {
          "id": 370464,
          "resource_id": 79,
          "text": "(Я, Аллах) клянусь временем,"
        },
        {
          "id": 257397,
          "resource_id": 45,
          "text": "Клянусь предвечерним временем (или временем)!"
        },
        {
          "id": 140601,
          "resource_id": 27,
          "text": "Beim Zeitalter!"
        },
        {
          "id": 10315150,
          "resource_id": 445,
          "text": "В знак времени (как Моего знаменья)"
        }
      ],
