          "translation": {
            "text": "your efforts",
            "language_name": "english"
          },
          "transliteration": {
            "text": "saʿyakum",
            "language_name": "english"
          }
        },
        {
          "id": 3406,
          "position": 3,
          "audio_url": "wbw/092_004_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﮥ",
