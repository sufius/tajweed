          "page_number": 595,
          "line_number": 12,
          "text": "ﮧ",
          "translation": {
            "text": "Then as for",
            "language_name": "english"
          },
          "transliteration": {
            "text": "fa-ammā",
            "language_name": "english"
          }
        },
        {
          "id": 4747,
          "position": 2,
          "audio_url": "wbw/092_005_002.mp3",
          "char_type_name": "word",
          "code_v1": "ﮨ",
          "page_number": 595,
          "line_number": 12,
          "text": "ﮨ",
          "translation": {
            "text": "(him) who",
            "language_name": "english"
          },
          "transliteration": {
