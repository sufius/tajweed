        {
          "id": 4748,
          "position": 3,
          "audio_url": "wbw/092_005_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﮩ",
          "page_number": 595,
          "line_number": 12,
          "text": "ﮩ",
