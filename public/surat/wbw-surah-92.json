            "language_name": "english"
          },
          "transliteration": {
            "text": "aʿṭā",
            "language_name": "english"
          }
        },
        {
          "id": 4749,
          "position": 4,
          "audio_url": "wbw/092_005_004.mp3",
          "char_type_name": "word",
          "code_v1": "ﮪ",
          "page_number": 595,
          "line_number": 12,
          "text": "ﮪ",
          "translation": {
