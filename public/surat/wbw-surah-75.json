          "line_number": 10,
          "text": "ﮧ",
          "translation": {
            "text": "He asks",
            "language_name": "english"
          },
          "transliteration": {
            "text": "yasalu",
            "language_name": "english"
          }
        },
        {
          "id": 6556,
          "position": 2,
          "audio_url": "wbw/075_006_002.mp3",
          "char_type_name": "word",
          "code_v1": "ﮨ",
          "page_number": 577,
          "line_number": 10,
          "text": "ﮨ",
          "translation": {
            "text": "When",
            "language_name": "english"
          },
          "transliteration": {
            "text": "ayyāna",
            "language_name": "english"
          }
        },
        {
          "id": 6557,
          "position": 3,
          "audio_url": "wbw/075_006_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﮩ",
          "page_number": 577,
          "line_number": 10,
          "text": "ﮩ",
          "translation": {
            "text": "(is the) Day",
            "language_name": "english"
          },
          "transliteration": {
            "text": "yawmu",
            "language_name": "english"
          }
        },
        {
          "id": 6558,
          "position": 4,
          "audio_url": "wbw/075_006_004.mp3",
          "char_type_name": "word",
          "code_v1": "ﮪ",
          "page_number": 577,
          "line_number": 10,
          "text": "ﮪ",
