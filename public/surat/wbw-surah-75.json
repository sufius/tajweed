          "id": 10119,
          "position": 3,
          "audio_url": "wbw/075_009_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﯖ",
          "page_number": 577,
          "line_number": 11,
          "text": "ﯖ",
          "translation": {
