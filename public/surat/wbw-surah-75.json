          },
          "transliteration": {
            "text": "bariqa",
            "language_name": "english"
          }
        },
        {
          "id": 7952,
          "position": 3,
          "audio_url": "wbw/075_007_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﮮ",
          "page_number": 577,
          "line_number": 10,
          "text": "ﮮ",
          "translation": {
