      "sajdah_number": null,
      "text_uthmani": "فَٱلْمُورِيَـٰتِ قَدْحًا",
      "text_uthmani_tajweed": "فَ[h:9999[ٱ]لْمُورِيَ[n[ـٰ]تِ قَ[q:14612[دْ]حًا",
      "text_uthmani_tajweed_parsed": "فَ<tajweed class='ham_wasl' data-type='hamza-wasl' data-description='Hamzat ul Wasl' data-tajweed=':9999'>ٱ</tajweed>لْمُورِيَ<tajweed class='madda_normal' data-type='madda-normal' data-description='Normal Prolongation: 2 Vowels'>ـٰ</tajweed>تِ قَ<tajweed class='qlq' data-type='qalaqah' data-description='Qalaqah' data-tajweed=':14612'>دْ</tajweed>حًا",
      "text_uthmani_transcribed": "Fal-mūriyāti qadḥā(n).",
      "translations": [
        {
          "id": 89934,
          "resource_id": 19,
          "text": "Striking sparks of fire"
        },
        {
          "id": 370435,
          "resource_id": 79,
          "text": "и выбивающими искры (когда камни под их копытами ударяются друг об друга),"
        },
        {
          "id": 257750,
          "resource_id": 45,
          "text": "Клянусь высекающими искры копытами!"
        },
        {
          "id": 139917,
          "resource_id": 27,
          "text": "den (mit ihren Hufen) Funken Schlagenden,"
        },
        {
          "id": 10025960,
          "resource_id": 445,
          "text": "Бьют искрами (из-под копыт)"
        }
      ],
