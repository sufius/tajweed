        {
          "id": 4528,
          "position": 1,
          "audio_url": "wbw/037_004_001.mp3",
          "char_type_name": "word",
          "code_v1": "ﭚ",
          "page_number": 446,
          "line_number": 3,
          "text": "ﭚ",
          "translation": {
            "text": "Indeed",
            "language_name": "english"
          },
          "transliteration": {
            "text": "inna",
            "language_name": "english"
          }
        },
        {
          "id": 4529,
          "position": 2,
          "audio_url": "wbw/037_004_002.mp3",
          "char_type_name": "word",
          "code_v1": "ﭛ",
          "page_number": 446,
          "line_number": 3,
          "text": "ﭛ",
          "translation": {
            "text": "your Lord",
            "language_name": "english"
          },
          "transliteration": {
            "text": "ilāhakum",
            "language_name": "english"
          }
        },
        {
          "id": 4530,
          "position": 3,
          "audio_url": "wbw/037_004_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﭜ",
          "page_number": 446,
          "line_number": 3,
          "text": "ﭜ",
