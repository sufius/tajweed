      "juz_number": 30,
      "words": [
        {
          "id": 6743,
          "position": 1,
          "audio_url": "wbw/087_006_001.mp3",
          "char_type_name": "word",
          "code_v1": "ﯕ",
          "page_number": 591,
          "line_number": 11,
          "text": "ﯕ",
          "translation": {
            "text": "We will make you recite",
            "language_name": "english"
          },
          "transliteration": {
            "text": "sanuq'ri-uka",
            "language_name": "english"
          }
        },
        {
          "id": 6744,
          "position": 2,
          "audio_url": "wbw/087_006_002.mp3",
          "char_type_name": "word",
          "code_v1": "ﯖ",
          "page_number": 591,
          "line_number": 12,
