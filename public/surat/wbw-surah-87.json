          "position": 3,
          "audio_url": "wbw/087_006_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﯗ",
          "page_number": 591,
          "line_number": 12,
          "text": "ﯗ",
          "translation": {
            "text": "you will forget",
            "language_name": "english"
          },
          "transliteration": {
            "text": "tansā",
            "language_name": "english"
          }
        },
        {
          "id": 6746,
          "position": 4,
          "audio_url": null,
          "char_type_name": "end",
          "code_v1": "ﯘ",
          "page_number": 591,
