        },
        {
          "id": 1761,
          "position": 2,
          "audio_url": "wbw/087_002_002.mp3",
          "char_type_name": "word",
          "code_v1": "ﮥ",
          "page_number": 591,
          "line_number": 10,
          "text": "ﮥ",
          "translation": {
            "text": "created",
            "language_name": "english"
          },
          "transliteration": {
            "text": "khalaqa",
            "language_name": "english"
