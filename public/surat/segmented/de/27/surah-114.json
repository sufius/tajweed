[
  {
    "index": 1,
    "verse_number": 1,
    "translation": "Sag: Ich nehme Zuflucht beim Herrn der Menschen,",
    "arabic": " قُلْ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
    "transcription": "Qul a'ūżu birabbin-nās(i)."
  },
  {
    "index": 2,
    "verse_number": 2,
    "translation": "dem König der Menschen,",
    "arabic": "مَلِكِ ٱلنَّاسِ",
    "transcription": "Malikin-nās(i)."
  },
  {
    "index": 3,
    "verse_number": 3,
    "translation": "dem Gott der Menschen,",
    "arabic": "إِلَـٰهِ ٱلنَّاسِ",
    "transcription": "Ilāhin-nās(i)."
  },
  {
    "index": 4,
    "verse_number": 4,
    "translation": "vor dem Übel des Einflüsterers, des Davonschleichers,",
    "arabic": "مِن شَرِّ ٱلْوَسْوَاسِ ٱلْخَنَّاسِ",
    "transcription": "Min syarril-waswāsil-khannās(i)."
  },
  {
    "index": 5,
    "verse_number": 5,
    "translation": "der in die Brüste der Menschen einflüstert,",
    "arabic": "ٱلَّذِى يُوَسْوِسُ فِى صُدُورِ ٱلنَّاسِ",
    "transcription": "Allażī yuwaswisu fī ṣudūrin-nās(i)."
  },
  {
    "index": 6,
    "verse_number": 6,
    "translation": "von den Ginn und den Menschen.",
    "arabic": "مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ",
    "transcription": "Minal jinnati wan-nās(i)."
  }
]