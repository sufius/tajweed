      "sajdah_number": null,
      "translations": [
        {
          "id": 89980,
          "resource_id": 19,
          "translation": "Rivalry in worldly increase distracteth you"
        },
        {
          "id": 370456,
          "resource_id": 79,
          "translation": "Увлекла [отвлекла от повиновения Аллаху] вас (о, люди) страсть к приумножению (благ этого мира),"
        },
        {
          "id": 257405,
          "resource_id": 45,
          "translation": "Страсть к приумножению (похвальба богатством и детьми) увлекает вас"
        },
        {
          "id": 140014,
          "resource_id": 27,
          "translation": "Die Vermehrung lenkt euch ab,"
        },
        {
          "id": 10216288,
          "resource_id": 445,
          "translation": "Страсть к накоплению добра Вас отвлекает от деяний должных"
        }
      ],
      "verse_key": "102:1",
      "verse_number": 1,
      "words": [
