          "page_number": 600,
          "position": 2,
          "text": "ﮌ",
          "translation": {
            "language_name": "english",
            "text": "the competition to increase"
          },
          "transliteration": {
            "language_name": "english",
            "text": "l-takāthuru"
          },
          "arabic": "ٱلتَّكَاثُرُ"
