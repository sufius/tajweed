      "sajdah_number": null,
      "translations": [
        {
          "id": 89936,
          "resource_id": 19,
          "translation": "Then, therewith, with their trail of dust,"
        },
        {
          "id": 370437,
          "resource_id": 79,
          "translation": "И они [кони] подняли этим [своим скачем] пыль,"
        },
        {
          "id": 254999,
          "resource_id": 45,
          "translation": "Тем самым они поднимают столб пыли"
        },
        {
          "id": 141751,
          "resource_id": 27,
          "translation": "die darin Staub aufwirbeln,"
        },
        {
          "id": 1004637,
          "resource_id": 445,
          "translation": "И пыль взметают в облака"
        }
      ],
      "verse_key": "100:4",
      "verse_number": 4,
      "words": [
