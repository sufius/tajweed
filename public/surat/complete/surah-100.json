          "line_number": 12,
          "page_number": 599,
          "position": 2,
          "text": "ﮙ",
          "translation": {
            "language_name": "english",
            "text": "striking"
          },
          "transliteration": {
            "language_name": "english",
            "text": "qadḥan"
          },
          "arabic": "قَدْحًۭا"
        },
        {
          "audio_url": null,
          "char_type_name": "end",
          "code_v1": "ﮚ",
          "id": 1943,
          "line_number": 12,
          "page_number": 599,
          "position": 3,
          "text": "ﮚ",
