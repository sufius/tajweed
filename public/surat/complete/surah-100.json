      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "sajdah_number": null,
      "translations": [
        {
          "id": 89935,
          "resource_id": 19,
          "translation": "And scouring to the raid at dawn,"
        },
        {
          "id": 370436,
          "resource_id": 79,
          "translation": "и нападающими на заре (на врагов)..."
        },
        {
          "id": 257603,
          "resource_id": 45,
          "translation": "Клянусь нападающими на заре!"
        },
        {
          "id": 141819,
          "resource_id": 27,
          "translation": "den am Morgen Angreifenden,"
        },
        {
          "id": 10037844,
          "resource_id": 445,
          "translation": "И атакуют на заре"
        }
      ],
      "verse_key": "100:3",
