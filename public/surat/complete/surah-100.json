      "manzil_number": 7,
      "page_number": 599,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "sajdah_number": null,
      "translations": [
        {
          "id": 89934,
          "resource_id": 19,
          "translation": "Striking sparks of fire"
        },
        {
          "id": 370435,
          "resource_id": 79,
          "translation": "и выбивающими искры (когда камни под их копытами ударяются друг об друга),"
        },
        {
          "id": 257750,
          "resource_id": 45,
          "translation": "Клянусь высекающими искры копытами!"
        },
        {
          "id": 139917,
          "resource_id": 27,
          "translation": "den (mit ihren Hufen) Funken Schlagenden,"
        },
        {
          "id": 10025960,
          "resource_id": 445,
          "translation": "Бьют искрами (из-под копыт)"
        }
      ],
      "verse_key": "100:2",
      "verse_number": 2,
      "words": [
