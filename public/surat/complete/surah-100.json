          "code_v1": "ﮖ",
          "id": 14,
          "line_number": 12,
          "page_number": 599,
          "position": 2,
          "text": "ﮖ",
          "translation": {
            "language_name": "english",
            "text": "panting"
          },
          "transliteration": {
            "language_name": "english",
            "text": "ḍabḥan"
          },
          "arabic": "ضَبْحًۭا"
        },
        {
          "audio_url": null,
          "char_type_name": "end",
          "code_v1": "ﮗ",
          "id": 15,
          "line_number": 12,
          "page_number": 599,
