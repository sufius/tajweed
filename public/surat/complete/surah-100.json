      "sajdah_number": null,
      "translations": [
        {
          "id": 89931,
          "resource_id": 19,
          "translation": "By the snorting courses,"
        },
        {
          "id": 370434,
          "resource_id": 79,
          "translation": "(Я, Аллах) клянусь (конями воинов) мчащимися, (и дышащими) запыхаясь,"
        },
        {
          "id": 252090,
          "resource_id": 45,
          "translation": "Клянусь скачущими, запыхаясь!"
        },
        {
          "id": 139907,
          "resource_id": 27,
          "translation": "Bei den schnaubend Rennenden,"
        },
        {
          "id": 10019622,
          "resource_id": 445,
          "translation": "В знак (Моего знаменья) - скакуны, Что мчатся, и, от бега задыхаясь"
        }
      ],
      "verse_key": "100:1",
      "verse_number": 1,
      "words": [
