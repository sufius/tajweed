      "sajdah_number": null,
      "translations": [
        {
          "id": 89938,
          "resource_id": 19,
          "translation": "Lo! man is an ingrate unto his Lord"
        },
        {
          "id": 370439,
          "resource_id": 79,
          "translation": "Поистине, человек по отношению к своему Господу конечно же неблагодарен,"
        },
        {
          "id": 255005,
          "resource_id": 45,
          "translation": "Воистину, человек неблагодарен своему Господу,"
        },
        {
          "id": 141509,
          "resource_id": 27,
          "translation": "Der Mensch ist seinem Herrn gegenüber wahrlich undankbar,"
        },
        {
          "id": 10064449,
          "resource_id": 445,
          "translation": "Поистине, и Богу своему неблагодарен человек"
        }
      ],
