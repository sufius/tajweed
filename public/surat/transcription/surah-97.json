[
  {
    "verse_number": 1,
    "transcription": "Innā anzalnāhu fī lailatil-qadr(i)."
  },
  {
    "verse_number": 2,
    "transcription": "Wa mā adrāka mā lailatul-qadr(i)."
  },
  {
    "verse_number": 3,
    "transcription": "Lailatul-qadri khairum min alfi syahr(in)."
  },
  {
    "verse_number": 4,
    "transcription": "Tanazzalul-malā'ikatu war rūḥu fīhā bi'iżni rabbihim min kulli amr(in)."
  },
  {
    "verse_number": 5,
    "transcription": "Salāmun hiya ḥattā maṭla'il-fajr(i)."
  }
]