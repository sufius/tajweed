[
  {
    "verse_number": 1,
    "transcription": "Iżas-samā'unsyaqqat."
  },
  {
    "verse_number": 2,
    "transcription": "Wa ażinat lirabbihā wa ḥuqqat."
  },
  {
    "verse_number": 3,
    "transcription": "Wa iẓal-arḍu muddat."
  },
  {
    "verse_number": 4,
    "transcription": "Wa alqat mā fīhā wa takhallat."
  },
  {
    "verse_number": 5,
    "transcription": "Wa ażinat lirabbihā wa ḥuqqat."
  },
  {
    "verse_number": 6,
    "transcription": "Yā ayyuhal-insānu innaka kādiḥun ilā rabbika kadḥan fa mulāqīh(i)."
  },
  {
    "verse_number": 7,
    "transcription": "Fa ammā man ūtiya kitābahū biyamīnih(ī)."
  },
  {
    "verse_number": 8,
    "transcription": "Fa saufa yuḥāsabu ḥisābay yasīrā(n)."
  },
  {
    "verse_number": 9,
    "transcription": "Wa yanqalibu ilā ahlihī masrūrā(n)."
  },
  {
    "verse_number": 10,
    "transcription": "Wa ammā man ūtiya kitābahū warā'a ẓahrih(ī)."
  },
  {
    "verse_number": 11,
    "transcription": "Fa saufa yad'ū ṡubūrā(n)."
  },
  {
    "verse_number": 12,
    "transcription": "Wa yaṣlā sa'īrā(n)."
  },
  {
    "verse_number": 13,
    "transcription": "Innahū kāna fī ahlihī masrūrā(n)."
  },
  {
    "verse_number": 14,
    "transcription": "Innahū ẓanna allay yaḥūr(a)."
  },
  {
    "verse_number": 15,
    "transcription": "Balā, inna rabbahū kāna bihī baṣīrā(n)."
  },
  {
    "verse_number": 16,
    "transcription": "Falā uqsimu bisy-syafaq(i)."
  },
  {
    "verse_number": 17,
    "transcription": "Wal-laili wa mā wasaq(a)."
  },
  {
    "verse_number": 18,
    "transcription": "Wal-qamari iżattasaq(a)."
  },
  {
    "verse_number": 19,
    "transcription": "Latarkabunna ṭabaqan 'an ṭabaq(in)."
  },
  {
    "verse_number": 20,
    "transcription": "Famā lahum lā yu'minūn(a)."
  },
  {
    "verse_number": 21,
    "transcription": "Wa iżā quri'a 'alaihimul-qur'ānu lā yasjudūn(a)."
  },
  {
    "verse_number": 22,
    "transcription": "Balil-lażīna kafarū yukażżibūn(a)."
  },
  {
    "verse_number": 23,
    "transcription": "Wallāhu a'lamu bimā yū'ūn(a)."
  },
  {
    "verse_number": 24,
    "transcription": "Fa basysyirhum bi'ażābin alīm(in)."
  },
  {
    "verse_number": 25,
    "transcription": "Illal-lażīna āmanū wa 'amiluṣ-ṣāliḥāti lahum ajrun gairu mamnūn(in)."
  }
]