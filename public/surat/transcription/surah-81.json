[
  {
    "verse_number": 1,
    "transcription": "Iżasy-syamsu kuwwirat."
  },
  {
    "verse_number": 2,
    "transcription": "Wa iżan-nujūmunkadarat."
  },
  {
    "verse_number": 3,
    "transcription": "Wa iżal-jibālu suyyirat."
  },
  {
    "verse_number": 4,
    "transcription": "Wa iżal-'isyāru 'uṭṭilat."
  },
  {
    "verse_number": 5,
    "transcription": "Wa iżal-wuḥūsy ḥusyirat."
  },
  {
    "verse_number": 6,
    "transcription": "Wa iżal-biḥāru sujjirat."
  },
  {
    "verse_number": 7,
    "transcription": "Wa iżan-nufūsu zuwwijat."
  },
  {
    "verse_number": 8,
    "transcription": "Wa iżal-mau'ūdatu su'ilat."
  },
  {
    "verse_number": 9,
    "transcription": "Bi'ayyi żambin qutilat."
  },
  {
    "verse_number": 10,
    "transcription": "Wa iżaṣ-ṣuḥufu nusyirat."
  },
  {
    "verse_number": 11,
    "transcription": "Wa iżas-samā'u kusyiṭat."
  },
  {
    "verse_number": 12,
    "transcription": "Wa iżal-jaḥīmu su''irat."
  },
  {
    "verse_number": 13,
    "transcription": "Wa iżal-jannatu uzlifat."
  },
  {
    "verse_number": 14,
    "transcription": "'Alimat nafsum mā aḥḍarat."
  },
  {
    "verse_number": 15,
    "transcription": "Falā uqsimu bil-khunnas(i)."
  },
  {
    "verse_number": 16,
    "transcription": "Al-jawāril-kunnas(i)."
  },
  {
    "verse_number": 17,
    "transcription": "Wal-laili iżā 'as'as(a)."
  },
  {
    "verse_number": 18,
    "transcription": "Waṣ-ṣubḥi iżā tanaffas(a)."
  },
  {
    "verse_number": 19,
    "transcription": "Innahū laqaulu rasūlin karīm(in)."
  },
  {
    "verse_number": 20,
    "transcription": "Żī quwwatin 'inda żil-'arsyi makīn(in)."
  },
  {
    "verse_number": 21,
    "transcription": "Muṭā'in ṡamma amīn(in)."
  },
  {
    "verse_number": 22,
    "transcription": "Wa mā ṣāḥibukum bimajnūn(in)."
  },
  {
    "verse_number": 23,
    "transcription": "Wa laqad ra'āhu bil-ufuqil-mubīn(i)."
  },
  {
    "verse_number": 24,
    "transcription": "Wa mā huwa 'alal-gaibi biḍanīn(in)."
  },
  {
    "verse_number": 25,
    "transcription": "Wa mā huwa biqauli syaiṭānir rajīm(in)."
  },
  {
    "verse_number": 26,
    "transcription": "Fa aina tażhabūn(a)."
  },
  {
    "verse_number": 27,
    "transcription": "In huwa illā żikrul lil-'ālamīn(a)."
  },
  {
    "verse_number": 28,
    "transcription": "Liman syā'a minkum ay yastaqīm(a)."
  },
  {
    "verse_number": 29,
    "transcription": "Wa mā tasyā'ūna illā ay yasyā'allāhu rabbul-'ālamīn(a)."
  }
]