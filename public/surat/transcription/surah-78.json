[
  {
    "verse_number": 1,
    "transcription": "'Amma yatasā'alūn(a)."
  },
  {
    "verse_number": 2,
    "transcription": "'Anin naba'il-'aẓīm(i)."
  },
  {
    "verse_number": 3,
    "transcription": "Allażī hum fīhi mukhtalifūn(a)."
  },
  {
    "verse_number": 4,
    "transcription": "Kallā saya'lamūn(a)."
  },
  {
    "verse_number": 5,
    "transcription": "Ṡumma kallā saya'lamūn(a)."
  },
  {
    "verse_number": 6,
    "transcription": "Alam naj'alil-arḍa mihādā(n)."
  },
  {
    "verse_number": 7,
    "transcription": "Wal-jibāla autādā(n)."
  },
  {
    "verse_number": 8,
    "transcription": "Wa khalaqnākum azwājā(n)."
  },
  {
    "verse_number": 9,
    "transcription": "Wa ja'alnā naumakum subātā(n)."
  },
  {
    "verse_number": 10,
    "transcription": "Wa ja'alnal-laila libāsā(n)."
  },
  {
    "verse_number": 11,
    "transcription": "Wa ja'alnan-nahāra ma'āsyā(n)."
  },
  {
    "verse_number": 12,
    "transcription": "Wa banainā fauqakum sab'an syidādā(n)."
  },
  {
    "verse_number": 13,
    "transcription": "Wa ja'alnā sirājaw wahhājā(n)."
  },
  {
    "verse_number": 14,
    "transcription": "Wa anzalnā minal-mu'ṣirāti mā'an ṡajjājā(n)."
  },
  {
    "verse_number": 15,
    "transcription": "Linukhrija bihī ḥabbaw wa nabātā(n)."
  },
  {
    "verse_number": 16,
    "transcription": "Wa jannātin alfāfā(n)."
  },
  {
    "verse_number": 17,
    "transcription": "Inna yaumal-faṣli kāna mīqātā(n)."
  },
  {
    "verse_number": 18,
    "transcription": "Yauma yunfakhu fiṣ-ṣūri fa ta'tūna afwājā(n)."
  },
  {
    "verse_number": 19,
    "transcription": "Wa futiḥatis-samā'u fa kānat abwābā(n)."
  },
  {
    "verse_number": 20,
    "transcription": "Wa suyyiratil-jibālu fa kānat sarābā(n)."
  },
  {
    "verse_number": 21,
    "transcription": "Inna jahannama kānat mirṣādā(n)."
  },
  {
    "verse_number": 22,
    "transcription": "Liṭ-ṭāgīna ma'ābā(n)."
  },
  {
    "verse_number": 23,
    "transcription": "Lābiṡīna fīhā aḥqābā(n)."
  },
  {
    "verse_number": 24,
    "transcription": "Lā yażūqūna fīhā bardaw wa lā syarābā(n)."
  },
  {
    "verse_number": 25,
    "transcription": "Illā ḥamīmaw wa gassāqā(n)."
  },
  {
    "verse_number": 26,
    "transcription": "Jazā'aw wifāqā(n)."
  },
  {
    "verse_number": 27,
    "transcription": "Innahum kānū lā yarjūna ḥisābā(n)."
  },
  {
    "verse_number": 28,
    "transcription": "Wa każżabū bi'āyātinā kiżżābā(n)."
  },
  {
    "verse_number": 29,
    "transcription": "Wa kulla syai'in aḥṣaināhu kitābā(n)."
  },
  {
    "verse_number": 30,
    "transcription": "Fa żūqū falan nazīdakum illā 'ażābā(n)"
  },
  {
    "verse_number": 31,
    "transcription": "Inna lil-muttaqīna mafāzā(n)."
  },
  {
    "verse_number": 32,
    "transcription": "Ḥadā'iqa wa a'nābā(n)."
  },
  {
    "verse_number": 33,
    "transcription": "Wa kawā'iba atrābā(n)."
  },
  {
    "verse_number": 34,
    "transcription": "Wa ka'san dihāqā(n)."
  },
  {
    "verse_number": 35,
    "transcription": "Lā yasma'ūna fīhā lagwaw wa lā kiżżābā(n)."
  },
  {
    "verse_number": 36,
    "transcription": "Jazā'am mir rabbika 'aṭā'an ḥisābā(n)."
  },
  {
    "verse_number": 37,
    "transcription": "Rabbis-samāwāti wal-arḍi wa mā bainahumar-raḥmāni lā yamlikūna minhu khiṭābā(n)."
  },
  {
    "verse_number": 38,
    "transcription": "Yauma yaqūmur-rūḥu wal-malā'ikatu ṣaffā(n), lā yatakallamūna illā man ażina lahur-raḥmānu wa qāla ṣawābā(n)."
  },
  {
    "verse_number": 39,
    "transcription": "Żālikal-yaumul-ḥaqq(u), faman syā'attakhaża ilā rabbihī ma'ābā(n)."
  },
  {
    "verse_number": 40,
    "transcription": "Innā anżarnākum 'ażāban qarībā(n), yauma yanẓurul-mar'u mā qaddamat yadāhu wa yaqūlul-kāfiru yā laitanī kuntu turābā(n)."
  }
]