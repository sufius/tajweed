[
  {
    "verse_number": 1,
    "transcription": "'Abasa wa tawallā."
  },
  {
    "verse_number": 2,
    "transcription": "An jā'ahul-a'mā."
  },
  {
    "verse_number": 3,
    "transcription": "Wa mā yudrīka la'allahū yazzakkā."
  },
  {
    "verse_number": 4,
    "transcription": "Au yażżakkaru fatanfa'ahuż-żikrā."
  },
  {
    "verse_number": 5,
    "transcription": "Ammā manistagnā."
  },
  {
    "verse_number": 6,
    "transcription": "Fa anta lahū taṣaddā."
  },
  {
    "verse_number": 7,
    "transcription": "Wa mā 'alaika allā yazzakkā."
  },
  {
    "verse_number": 8,
    "transcription": "Wa ammā man jā'aka yas'ā."
  },
  {
    "verse_number": 9,
    "transcription": "Wa huwa yakhsyā."
  },
  {
    "verse_number": 10,
    "transcription": "Fa anta 'anhu talahhā."
  },
  {
    "verse_number": 11,
    "transcription": "Kallā innahā tażkirah(tun)."
  },
  {
    "verse_number": 12,
    "transcription": "Faman syā'a żakarah(ū)."
  },
  {
    "verse_number": 13,
    "transcription": "Fī ṣuḥufim mukarrmah(tin)."
  },
  {
    "verse_number": 14,
    "transcription": "Marfū'atim muṭahharah(tin)."
  },
  {
    "verse_number": 15,
    "transcription": "Bi'aidī safarah(tin)."
  },
  {
    "verse_number": 16,
    "transcription": "Kirāmim bararah(tin)."
  },
  {
    "verse_number": 17,
    "transcription": "Qutilal-insānu mā akfarah(ū)."
  },
  {
    "verse_number": 18,
    "transcription": "Min ayyi syai'in khalaqah(ū)."
  },
  {
    "verse_number": 19,
    "transcription": "Min nuṭfah(tin), khalaqahū fa qaddarah(ū)."
  },
  {
    "verse_number": 20,
    "transcription": "Ṡummas-sabīla yassarah(ū)."
  },
  {
    "verse_number": 21,
    "transcription": "Ṡumma amātahū fa aqbarah(ū)."
  },
  {
    "verse_number": 22,
    "transcription": "Ṡumma iżā syā'a ansyarah(ū)."
  },
  {
    "verse_number": 23,
    "transcription": "Kallā lammā yaqḍi mā amarah(ū)."
  },
  {
    "verse_number": 24,
    "transcription": "Falyanẓuril-insānu ilā ṭa'āmih(ī)."
  },
  {
    "verse_number": 25,
    "transcription": "Annā ṣababnal-mā'a ṣabbā(n)."
  },
  {
    "verse_number": 26,
    "transcription": "Ṡumma syaqaqnal-arḍa syaqqā(n)."
  },
  {
    "verse_number": 27,
    "transcription": "Fa'ambatnā fīhā ḥabbā(n)."
  },
  {
    "verse_number": 28,
    "transcription": "Wa 'inabaw wa qaḍbā(n)."
  },
  {
    "verse_number": 29,
    "transcription": "Wa zaitūnaw wa nakhlā(n)."
  },
  {
    "verse_number": 30,
    "transcription": "Wa ḥadā'iqa gulbā(n)."
  },
  {
    "verse_number": 31,
    "transcription": "Wa fākihataw wa abbā(n)."
  },
  {
    "verse_number": 32,
    "transcription": "Matā'al lakum wa li'an'āmikum."
  },
  {
    "verse_number": 33,
    "transcription": "Fa iżā jā'atiṣ-ṣākhkhah(tu)."
  },
  {
    "verse_number": 34,
    "transcription": "Yauma yafirrul-mar'u min akhīh(i)."
  },
  {
    "verse_number": 35,
    "transcription": "Wa ummihī wa abīh(i)."
  },
  {
    "verse_number": 36,
    "transcription": "Wa ṣāḥibatihī wa banīh(i)."
  },
  {
    "verse_number": 37,
    "transcription": "Likullimri'im minhum yauma'iżin sya'nuy yugnīh(i)."
  },
  {
    "verse_number": 38,
    "transcription": "Wujūhuy yauma'iżim musfirah(tun)."
  },
  {
    "verse_number": 39,
    "transcription": "Ḍāḥikatum mustabsyirah(tun)."
  },
  {
    "verse_number": 40,
    "transcription": "Wa wujūhuy yauma'iżin 'alaihā gabarah(tun)."
  },
  {
    "verse_number": 41,
    "transcription": "Tarhaquhā qatarah(tun)."
  },
  {
    "verse_number": 42,
    "transcription": "Ulā'ika humul-kafaratul-fajarah(tu)."
  }
]