[
  {
    "verse_number": 1,
    "transcription": "Sa'ala sā'ilum bi'ażābiw wāqi'(in)."
  },
  {
    "verse_number": 2,
    "transcription": "Lil-kāfirīna laisa lahū dāfi'(un)."
  },
  {
    "verse_number": 3,
    "transcription": "Minallāhi żil-ma'ārij(i)."
  },
  {
    "verse_number": 4,
    "transcription": "Ta'rujul-malā'ikatu war-rūḥu ilaihi fī yaumin kāna miqdāruhū khamsīna alfa sanah(tin)."
  },
  {
    "verse_number": 5,
    "transcription": "Faṣbir ṣabran jamīlā(n)."
  },
  {
    "verse_number": 6,
    "transcription": "Innahum yaraunahū ba'īdā(n)."
  },
  {
    "verse_number": 7,
    "transcription": "Wa narāhu qarībā(n)."
  },
  {
    "verse_number": 8,
    "transcription": "Yauma takūnus-samā'u kal-muhl(i)."
  },
  {
    "verse_number": 9,
    "transcription": "Wa takūnul-jibālu kal-'ihn(i)."
  },
  {
    "verse_number": 10,
    "transcription": "Wa lā yas'alu ḥamīmun ḥamīmā(n)."
  },
  {
    "verse_number": 11,
    "transcription": "Yubaṣṣarūnahum, yawaddul-mujrimu lau yaftadī  min 'ażābi yaumi'iżim bibanīh(i)."
  },
  {
    "verse_number": 12,
    "transcription": "Wa ṣāḥibatihī wa akhīh(i)."
  },
  {
    "verse_number": 13,
    "transcription": "Wa faṣīlatihil-latī tu'wīh(i)."
  },
  {
    "verse_number": 14,
    "transcription": "Wa man fil-arḍi jamī'ā(n), ṡumma yunjīh(i)."
  },
  {
    "verse_number": 15,
    "transcription": "Kallā, innahā laẓā."
  },
  {
    "verse_number": 16,
    "transcription": "Nazzā'atal lisy-syawā."
  },
  {
    "verse_number": 17,
    "transcription": "Tad'ū man adbara wa tawallā."
  },
  {
    "verse_number": 18,
    "transcription": "Wa jama'a fa'au'ā."
  },
  {
    "verse_number": 19,
    "transcription": "Innal-insāna khuliqa halu'ā(n)."
  },
  {
    "verse_number": 20,
    "transcription": "Iżā massahusy-syarru jazū'ā(n)."
  },
  {
    "verse_number": 21,
    "transcription": "Wa iżā massahul-khairu manū'ā(n)."
  },
  {
    "verse_number": 22,
    "transcription": "Illal-muṣallīn(a)."
  },
  {
    "verse_number": 23,
    "transcription": "Allażīna hum 'alā ṣalātihim dā'imūn(a)."
  },
  {
    "verse_number": 24,
    "transcription": "Wal-lażīna fī amwālihim ḥaqqum ma'lūm(un)."
  },
  {
    "verse_number": 25,
    "transcription": "Lis-sā'ili wal-maḥrūm(i)."
  },
  {
    "verse_number": 26,
    "transcription": "Wal-lażīna yuṣaddiqūna biyaumid-dīn(i)."
  },
  {
    "verse_number": 27,
    "transcription": "Wal-lażīna hum min 'ażābi rabbihim musyfiqūn(a)."
  },
  {
    "verse_number": 28,
    "transcription": "Inna 'ażāba rabbihim gairu ma'mūn(in)."
  },
  {
    "verse_number": 29,
    "transcription": "Wal-lażīna hum lifurūjihim ḥāfiẓūn(a)."
  },
  {
    "verse_number": 30,
    "transcription": "Illā 'alā azwājihim au mā malakat aimānuhum fa innahum gairu malūmīn(a)."
  },
  {
    "verse_number": 31,
    "transcription": "Fa manibtagā warā'a żālika fa ulā'ika  humul-'ādūn(a)."
  },
  {
    "verse_number": 32,
    "transcription": "Wal-lażīna hum li'amānātihim wa 'ahdihim rā'ūn(a)."
  },
  {
    "verse_number": 33,
    "transcription": "Wal-lażīna hum bisyahādātihim qā'imūn(a)."
  },
  {
    "verse_number": 34,
    "transcription": "Wal-lażīna hum 'alā ṣalātihim yuḥāfiẓūn(a)."
  },
  {
    "verse_number": 35,
    "transcription": "Ulā'ika fī jannātim mukramūn(a)."
  },
  {
    "verse_number": 36,
    "transcription": "Famālil-lażīna kafarū qibalaka muhṭi'īn(a)."
  },
  {
    "verse_number": 37,
    "transcription": "'Anil-yamīni wa 'anisy-syimāli 'izīn(a)."
  },
  {
    "verse_number": 38,
    "transcription": "Ayaṭma'u kullumri'im minhum ay yudkhala jannata na'īm(in)."
  },
  {
    "verse_number": 39,
    "transcription": "Kallā, innā khalaqnāhum mimmā ya'malūn(a)."
  },
  {
    "verse_number": 40,
    "transcription": "Falā uqsimu birabbil-masyāriqi wal-magāribi  innā laqādirūn(a)."
  },
  {
    "verse_number": 41,
    "transcription": "'Alā an nubaddila khairam minhum, wa mā naḥnu bimasbūqīn(a)."
  },
  {
    "verse_number": 42,
    "transcription": "Fa żarhum yakhūḍū  wa yal'abū ḥattā yulāqū yaumahumul-lażī yū'adūn(a)"
  },
  {
    "verse_number": 43,
    "transcription": "Yauma yakhrujūna minal-ajdāṡi sirā'an ka'annahum ilā nuṣubiy yūfiḍūn(a)."
  },
  {
    "verse_number": 44,
    "transcription": "Khāsyi'atan abṣāruhum tarhaquhum żillah(tun), żālikal-yaumul-lażī kānū yū'adūn(a)."
  }
]