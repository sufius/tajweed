[
  {
    "verse_number": 1,
    "transcription": "Wat-tīni waz-zaitūn(i)."
  },
  {
    "verse_number": 2,
    "transcription": "Wa ṭūri sīnīn(a)."
  },
  {
    "verse_number": 3,
    "transcription": "Wa hāżal-baladil-amīn(i)."
  },
  {
    "verse_number": 4,
    "transcription": "Laqad khalaqnal-insāna fī aḥsani taqwīm(in)."
  },
  {
    "verse_number": 5,
    "transcription": "Ṡumma radadnāhu asfala sāfilīn(a)."
  },
  {
    "verse_number": 6,
    "transcription": "Illal-lażīna āmanū wa 'amiluṣ-ṣāliḥāti falahum ajrun gairu mamnūn(in)."
  },
  {
    "verse_number": 7,
    "transcription": "Famā yukażżibuka ba'du bid-dīn(i)."
  },
  {
    "verse_number": 8,
    "transcription": "Alaisallāhu bi'aḥkamil-ḥākimīn(a)."
  }
]