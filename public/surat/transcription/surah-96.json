[
  {
    "verse_number": 1,
    "transcription": "Iqra' bismi rabbikal-lażī khalaq(a)."
  },
  {
    "verse_number": 2,
    "transcription": "Khalaqal-insāna min 'alaq(in)."
  },
  {
    "verse_number": 3,
    "transcription": "Iqra' wa rabbukal-akram(u)."
  },
  {
    "verse_number": 4,
    "transcription": "Allażī 'allama bil-qalam(i)."
  },
  {
    "verse_number": 5,
    "transcription": "'Allamal-insāna mā lam ya'lam."
  },
  {
    "verse_number": 6,
    "transcription": "Kallā innal-insāna layaṭgā."
  },
  {
    "verse_number": 7,
    "transcription": "Ar ra'āhustagnā."
  },
  {
    "verse_number": 8,
    "transcription": "Inna ilā rabbikar-ruj'ā."
  },
  {
    "verse_number": 9,
    "transcription": "Ara'aital-lażī yanhā."
  },
  {
    "verse_number": 10,
    "transcription": "'Abdan iżā ṣallā."
  },
  {
    "verse_number": 11,
    "transcription": "Ara'aita in kāna 'alal-hudā."
  },
  {
    "verse_number": 12,
    "transcription": "Au amara bit-taqwā."
  },
  {
    "verse_number": 13,
    "transcription": "Ara'aita in każżaba wa tawallā."
  },
  {
    "verse_number": 14,
    "transcription": "Alam ya'lam bi'annallāha yarā."
  },
  {
    "verse_number": 15,
    "transcription": "Kallā la'il lam yantah(i), lanasfa'am bin-nāṣiyah(ti)."
  },
  {
    "verse_number": 16,
    "transcription": "Nāṣiyatin kāżibatin khāṭi'ah(tin)."
  },
  {
    "verse_number": 17,
    "transcription": "Falyad'u nādiyah(ū)."
  },
  {
    "verse_number": 18,
    "transcription": "Sanad'uz-zabāniyah(ta)."
  },
  {
    "verse_number": 19,
    "transcription": "Kallā, lā tuṭi'hu wasjud waqtarib."
  }
]