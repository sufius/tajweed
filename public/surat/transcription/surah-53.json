[
  {
    "verse_number": 1,
    "transcription": "Wan-najmi iżā hawā."
  },
  {
    "verse_number": 2,
    "transcription": "Mā ḍalla ṣāḥibukum wa mā gawā."
  },
  {
    "verse_number": 3,
    "transcription": "Wa mā yanṭiqu 'anil-hawā."
  },
  {
    "verse_number": 4,
    "transcription": "In huwa illā waḥyuy yūḥā."
  },
  {
    "verse_number": 5,
    "transcription": "'Allamahū syadīdul-quwā."
  },
  {
    "verse_number": 6,
    "transcription": "Żū mirrah(tin), fastawā."
  },
  {
    "verse_number": 7,
    "transcription": "Wa huwa bil-ufuqil-a'lā."
  },
  {
    "verse_number": 8,
    "transcription": "Ṡumma danā fa tadallā."
  },
  {
    "verse_number": 9,
    "transcription": "Fa kāna qāba qausaini au adnā."
  },
  {
    "verse_number": 10,
    "transcription": "Fa auḥā ilā 'abdihī mā auḥā."
  },
  {
    "verse_number": 11,
    "transcription": "Mā każabal-fu'ādu mā ra'ā."
  },
  {
    "verse_number": 12,
    "transcription": "Afa tumārūnahū 'alā mā yarā."
  },
  {
    "verse_number": 13,
    "transcription": "Wa laqad ra'āhu nazlatan ukhrā."
  },
  {
    "verse_number": 14,
    "transcription": "'Inda sidratil-muntahā."
  },
  {
    "verse_number": 15,
    "transcription": "'Indahā jannatul-ma'wā."
  },
  {
    "verse_number": 16,
    "transcription": "Iż yagsyas-sidrata mā yagsyā."
  },
  {
    "verse_number": 17,
    "transcription": "Mā zāgal-baṣaru wa mā ṭagā."
  },
  {
    "verse_number": 18,
    "transcription": "Laqad ra'ā min āyāti rabbihil-kubrā."
  },
  {
    "verse_number": 19,
    "transcription": "Afa ra'aitumul-lāta wal-'uzzā."
  },
  {
    "verse_number": 20,
    "transcription": "Wa manātaṡ-ṡāliṡatal-ukhrā."
  },
  {
    "verse_number": 21,
    "transcription": "Alakumuż-żakaru wa lahul-unṡā."
  },
  {
    "verse_number": 22,
    "transcription": "Tilka iżan qismatun ḍīzā."
  },
  {
    "verse_number": 23,
    "transcription": "In hiya  illā asmā'un sammaitumūhā antum wa ābā'ukum mā anzalallāhu bihā min sulṭān(in), iy yattabi'ūna illaẓ-ẓanna wa mā tahwal-anfus(u), wa laqad jā'ahum mir rabbihimul-hudā."
  },
  {
    "verse_number": 24,
    "transcription": "Am lil-insāni mā tamannā."
  },
  {
    "verse_number": 25,
    "transcription": "Fa lillāhil-ākhiratu wal-ūlā."
  },
  {
    "verse_number": 26,
    "transcription": "Wa kam mim malakin fis-samāwāti lā tugnī syafā'atuhum syai'an illā mim ba'di ay ya'żanallāhu limay yasyā'u wa yarḍā."
  },
  {
    "verse_number": 27,
    "transcription": "Innal-lażīna lā yu'minūna bil-ākhirati layusammūnal-malā'ikata tasmiyatal-unṡā."
  },
  {
    "verse_number": 28,
    "transcription": "Wa mā lahum bihī min 'ilm(in), iy yattabi'ūna illaẓ-ẓann(a), wa innaẓ-ẓanna lā yugnī minal-ḥaqqi syai'ā(n)."
  },
  {
    "verse_number": 29,
    "transcription": "Fa a'riḍ 'am man tawallā, 'an żikrinā wa lam yurid illal-ḥayātad-dun-yā."
  },
  {
    "verse_number": 30,
    "transcription": "Żālika mablaguhum minal-'ilm(i), inna rabbaka huwa a'lamu biman ḍalla 'an sabīlih(ī), wa huwa a'lamu bimanihtadā."
  },
  {
    "verse_number": 31,
    "transcription": "Wa lillāhi mā fis-samāwāti wa mā fil-arḍ(i), liyajziyal-lażīna asā'ū bimā 'amilū wa yajziyal-lażīna aḥsanū bil-ḥusnā."
  },
  {
    "verse_number": 32,
    "transcription": "Allażīna yajtanibūna kabā'iral-iṡmi wal-fawāḥisya illal-lamam(a), inna rabbaka wāsi'ul-magfirah(ti), huwa a'lamu bikum iż ansya'akum minal-arḍi wa iż antum ajinnatun fī buṭūni ummahātikum, falā tuzakkū anfusakum, huwa a'lamu bimanittaqā."
  },
  {
    "verse_number": 33,
    "transcription": "Afa ra'aital-lażī tawallā."
  },
  {
    "verse_number": 34,
    "transcription": "Wa a'ṭā qalīlaw wa akdā."
  },
  {
    "verse_number": 35,
    "transcription": "A'indahū 'ilmul-gaibi fahuwa yarā."
  },
  {
    "verse_number": 36,
    "transcription": "Am lam yunabba' bimā fī ṣuḥufi mūsā."
  },
  {
    "verse_number": 37,
    "transcription": "Wa ibrāhīmal-lażī waffā."
  },
  {
    "verse_number": 38,
    "transcription": "Allā taziru wāziratuw wizra ukhrā."
  },
  {
    "verse_number": 39,
    "transcription": "Wa al laisa lil-insāni illā mā sa'ā."
  },
  {
    "verse_number": 40,
    "transcription": "Wa anna sa'yahū saufa yurā."
  },
  {
    "verse_number": 41,
    "transcription": "Ṡumma yujzāhul-jazā'al-aufā."
  },
  {
    "verse_number": 42,
    "transcription": "Wa anna ilā rabbikal-muntahā."
  },
  {
    "verse_number": 43,
    "transcription": "Wa annahū huwa aḍḥaka wa abkā."
  },
  {
    "verse_number": 44,
    "transcription": "Wa annahū huwa amāta wa aḥyā."
  },
  {
    "verse_number": 45,
    "transcription": "Wa annahū khalaqaz-zaujainiż-żakara wal-unṡā."
  },
  {
    "verse_number": 46,
    "transcription": "Min nuṭfatin iżā tumnā."
  },
  {
    "verse_number": 47,
    "transcription": "Wa anna 'alaihin nasy'atal-ukhrā."
  },
  {
    "verse_number": 48,
    "transcription": "Wa annahū huwa agnā wa aqnā."
  },
  {
    "verse_number": 49,
    "transcription": "Wa annahū huwa rabbusy-syi'rā."
  },
  {
    "verse_number": 50,
    "transcription": "Wa annahū ahlaka 'ādanil-ūlā."
  },
  {
    "verse_number": 51,
    "transcription": "Wa ṡamūda famā abqā."
  },
  {
    "verse_number": 52,
    "transcription": "Wa qauma nūḥim min qabl(u), innahum kānū hum aẓlama wa aṭgā."
  },
  {
    "verse_number": 53,
    "transcription": "Wal-mu'tafikata ahwā."
  },
  {
    "verse_number": 54,
    "transcription": "Fa gasysyāhā mā gasysyā."
  },
  {
    "verse_number": 55,
    "transcription": "Fa bi'ayyi ālā'i rabbika tatamārā."
  },
  {
    "verse_number": 56,
    "transcription": "Hāżā nażīrum minan-nużuril-ūlā."
  },
  {
    "verse_number": 57,
    "transcription": "Azifatil-āzifah(tu)."
  },
  {
    "verse_number": 58,
    "transcription": "Laisa lahā min dūnillāhi kāsyifah(tun)."
  },
  {
    "verse_number": 59,
    "transcription": "Afamin hāżal-ḥadīṡi ta'jabūn(a)."
  },
  {
    "verse_number": 60,
    "transcription": "Wa taḍḥakūna wa lā tabkūn(a)."
  },
  {
    "verse_number": 61,
    "transcription": "Wa antum sāmidūn(a)."
  },
  {
    "verse_number": 62,
    "transcription": "Fasjudū lillāhi wa'budū."
  }
]