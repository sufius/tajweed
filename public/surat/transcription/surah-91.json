[
  {
    "verse_number": 1,
    "transcription": "Wasy-syamsi wa ḍuḥāhā."
  },
  {
    "verse_number": 2,
    "transcription": "Wal-qamari iżā talāhā."
  },
  {
    "verse_number": 3,
    "transcription": "Wan-nahāri iżā jallāhā."
  },
  {
    "verse_number": 4,
    "transcription": "Wal-laili iżā yagsyāhā."
  },
  {
    "verse_number": 5,
    "transcription": "Was-samā'i wa mā banāhā."
  },
  {
    "verse_number": 6,
    "transcription": "Wal-arḍi wa mā ṭaḥāhā."
  },
  {
    "verse_number": 7,
    "transcription": "Wa nafsiw wa mā sawwāhā."
  },
  {
    "verse_number": 8,
    "transcription": "Fa alhamahā fujūrahā wa taqwāhā."
  },
  {
    "verse_number": 9,
    "transcription": "Qad aflaḥa man zakkāhā."
  },
  {
    "verse_number": 10,
    "transcription": "Wa qad khāba man dassāhā."
  },
  {
    "verse_number": 11,
    "transcription": "Każżabat ṡamūdu biṭagwāhā."
  },
  {
    "verse_number": 12,
    "transcription": "Iżimba'aṡa asyqāhā."
  },
  {
    "verse_number": 13,
    "transcription": "Fa qāla lahum rasūlullāhi nāqatallāhi wa suqyāhā."
  },
  {
    "verse_number": 14,
    "transcription": "Fa każżabūhu fa 'aqarūhā fa damdama 'alaihim rabbuhum biżambihim fa sawwāhā."
  },
  {
    "verse_number": 15,
    "transcription": "Wa lā yakhāfu 'uqbāhā."
  }
]