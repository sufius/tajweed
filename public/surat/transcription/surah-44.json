[
  {
    "verse_number": 1,
    "transcription": "Ḥā mīm."
  },
  {
    "verse_number": 2,
    "transcription": "Wal-kitābil-mubīn(i)."
  },
  {
    "verse_number": 3,
    "transcription": "Innā anzalnāhu fī lailatim mubārakatin innā kunnā munżirīn(a)."
  },
  {
    "verse_number": 4,
    "transcription": "Fīhā yufraqu kullu amrin ḥakīm(in)."
  },
  {
    "verse_number": 5,
    "transcription": "Amram min 'indinā, innā kunnā mursilīn(a)."
  },
  {
    "verse_number": 6,
    "transcription": "Raḥmatam mir rabbik(a), innahū huwas-samī'ul-'alīm(u)."
  },
  {
    "verse_number": 7,
    "transcription": "Rabbis-samāwāti wal-arḍi wa mā bainahumā, in kuntum mūqinīn(a)."
  },
  {
    "verse_number": 8,
    "transcription": "Lā ilāha illā huwa yuḥyī wa yumīt(u), rabbukum wa rabbu ābā'ikumul-awwalīn(a)."
  },
  {
    "verse_number": 9,
    "transcription": "Bal hum fī syakkiy yal'abūn(a)."
  },
  {
    "verse_number": 10,
    "transcription": "Fartaqib yauma ta'tis-samā'u bidukhānim mubīn(in)."
  },
  {
    "verse_number": 11,
    "transcription": "Yagsyan-nās(a), hāżā 'ażābun alīm(un)."
  },
  {
    "verse_number": 12,
    "transcription": "Rabbanaksyif 'annal-'ażāba innā mu'minūn(a)."
  },
  {
    "verse_number": 13,
    "transcription": "Annā lahumuż-żikrā wa qad jā'ahum rasūlum mubīn(un)."
  },
  {
    "verse_number": 14,
    "transcription": "Ṡumma tawallau 'anhu wa qālū mu'allamum majnūn(un)."
  },
  {
    "verse_number": 15,
    "transcription": "Innā kāsyiful-'ażābi qalīlan innakum 'ā'idūn(a)."
  },
  {
    "verse_number": 16,
    "transcription": "Yauma nabṭisyul baṭsyatal kubrā, innā muntaqimūn(a)."
  },
  {
    "verse_number": 17,
    "transcription": "Wa laqad fatannā qablahum qauma fir'auna wa jā'ahum rasūlun karīm(un)."
  },
  {
    "verse_number": 18,
    "transcription": "An addū ilayya 'ibādallāh(i), innī lakum rasūlun amīn(un)."
  },
  {
    "verse_number": 19,
    "transcription": "Wa al lā ta'lū 'alallāh(i), innī ātīkum bisulṭānim mubīn(in)."
  },
  {
    "verse_number": 20,
    "transcription": "Wa innī 'użtu birabbī wa rabbikum an tarjumūn(i)."
  },
  {
    "verse_number": 21,
    "transcription": "Wa il lam tu'minū lī fa'tazilūn(i)."
  },
  {
    "verse_number": 22,
    "transcription": "Fa da'ā rabbahū anna hā'ulā'i qaumum mujrimūn(a)."
  },
  {
    "verse_number": 23,
    "transcription": "Fa asri bi'ibādī lailan innakum muttaba'ūn(a)."
  },
  {
    "verse_number": 24,
    "transcription": "Watrukil-baḥra rahwā(n), innahum jundum mugraqūn(a)."
  },
  {
    "verse_number": 25,
    "transcription": "Kam tarakū min jannātiw wa 'uyūn(in)."
  },
  {
    "verse_number": 26,
    "transcription": "Wa zurū'iw wa maqāmin karīm(in)."
  },
  {
    "verse_number": 27,
    "transcription": "Wa na'matin kānū fīhā fākihīn(a)."
  },
  {
    "verse_number": 28,
    "transcription": "Każālik(a), wa auraṡnāhā qauman ākharīn(a)."
  },
  {
    "verse_number": 29,
    "transcription": "Famā bakat 'alaihimus-samā'u wal-arḍ(u), wa mā kānū munẓarīn(a)."
  },
  {
    "verse_number": 30,
    "transcription": "Wa laqad najjainā banī isrā'īla minal-'ażābil-muhīn(i)."
  },
  {
    "verse_number": 31,
    "transcription": "Min fir'aun(a), innahū kāna 'āliyam minal-musrifīn(a)."
  },
  {
    "verse_number": 32,
    "transcription": "Wa laqadikhtarnāhum 'alā 'ilmin 'alal-'ālamīn(a)."
  },
  {
    "verse_number": 33,
    "transcription": "Wa ātaināhum minal-āyāti mā fīhi balā'um mubīn(un)."
  },
  {
    "verse_number": 34,
    "transcription": "Inna hā'ulā'i layaqūlūn(a)."
  },
  {
    "verse_number": 35,
    "transcription": "In hiya illā mautatunal-ūlā wa mā naḥnu bimunsyarīn(a)."
  },
  {
    "verse_number": 36,
    "transcription": "Fa'tū bi'ābā'inā in kuntum ṣādiqīn(a)."
  },
  {
    "verse_number": 37,
    "transcription": "Ahum khairun am qaumu tubba'(in), wal-lażīna min qablihim, ahlaknāhum innahum kānū mujrimīn(a)."
  },
  {
    "verse_number": 38,
    "transcription": "Wa mā khalaqnas-samāwāti wal-arḍa wa mā bainahumā lā'ibīn(a)."
  },
  {
    "verse_number": 39,
    "transcription": "Mā khalaqnāhumā illā bil-ḥaqqi wa lākinna akṡarahum lā ya'lamūn(a)."
  },
  {
    "verse_number": 40,
    "transcription": "Inna yaumal-faṣli mīqātuhum ajma'īn(a)."
  },
  {
    "verse_number": 41,
    "transcription": "Yauma lā yugnī maulan 'am maulan syai'aw wa lā hum yunṣarūn(a)."
  },
  {
    "verse_number": 42,
    "transcription": "Illā mar raḥimallāh(u), innahū huwal-'azīzur-raḥīm(u)."
  },
  {
    "verse_number": 43,
    "transcription": "Inna syajarataz-zaqqūm(i)."
  },
  {
    "verse_number": 44,
    "transcription": "Ṭa'āmul-aṡīm(i)."
  },
  {
    "verse_number": 45,
    "transcription": "Kal-muhl(i), yaglī fil-buṭūn(i)."
  },
  {
    "verse_number": 46,
    "transcription": "Kagalyil-ḥamīm(i)."
  },
  {
    "verse_number": 47,
    "transcription": "Khużūhu fa'tilūhu ilā sawā'il-jaḥīm(i)."
  },
  {
    "verse_number": 48,
    "transcription": "Ṡumma ṣubbū fauqa ra'sihī min 'azābil-ḥamīm(i)."
  },
  {
    "verse_number": 49,
    "transcription": "Żuq, innaka antal-'azīzul-karīm(u)."
  },
  {
    "verse_number": 50,
    "transcription": "Inna hāżā mā kuntum bihī tamtarūn(a)."
  },
  {
    "verse_number": 51,
    "transcription": "Innal-muttaqīna fī maqāmin amīn(in)."
  },
  {
    "verse_number": 52,
    "transcription": "Fī jannātiw wa 'uyūn(in)."
  },
  {
    "verse_number": 53,
    "transcription": "Yalbasūna min sundusiw wa istabraqim mutaqābilīn(a)."
  },
  {
    "verse_number": 54,
    "transcription": "Każālik(a), wa zawwajnāhum biḥūrin 'īn(in)."
  },
  {
    "verse_number": 55,
    "transcription": "Yad'ūna fīhā bikulli fākihatin āminīn(a)."
  },
  {
    "verse_number": 56,
    "transcription": "Lā yażūqūna fīhal-mauta illal-mautatal-ūlā, wa waqāhum 'ażābal-jaḥīm(i)."
  },
  {
    "verse_number": 57,
    "transcription": "Faḍlam mir rabbika żālika huwal-fauzul-'aẓīm(u)."
  },
  {
    "verse_number": 58,
    "transcription": "Fa innamā yassarnāhu bilisānika la'allahum yatażakkarūn(a)."
  },
  {
    "verse_number": 59,
    "transcription": "Fartaqib innahum murtaqibūn(a)."
  }
]