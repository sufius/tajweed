[
  {
    "verse_number": 1,
    "transcription": "Wailul lil-muṭaffifīn(a)."
  },
  {
    "verse_number": 2,
    "transcription": "Allażīna iżaktālū 'alan-nāsi yastaufūn(a)."
  },
  {
    "verse_number": 3,
    "transcription": "Wa iżā kālūhum au wazanūhum yukhsirūn(a)."
  },
  {
    "verse_number": 4,
    "transcription": "Alā yaẓunnu ulā'ika annahum mab'ūṡūn(a)."
  },
  {
    "verse_number": 5,
    "transcription": "Liyaumin 'aẓīm(in)."
  },
  {
    "verse_number": 6,
    "transcription": "Yauma yaqūmun-nāsu lirabbil-'ālamīn(a)."
  },
  {
    "verse_number": 7,
    "transcription": "Kallā inna kitābal-fujjāri lafī sijjīn(in)."
  },
  {
    "verse_number": 8,
    "transcription": "Wa mā adrāka mā sijjīn(un)."
  },
  {
    "verse_number": 9,
    "transcription": "Kitābum marqūm(un)."
  },
  {
    "verse_number": 10,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 11,
    "transcription": "Allażīna yukażżibūna biyaumid-dīn(i)."
  },
  {
    "verse_number": 12,
    "transcription": "Wa mā yukażżibu bihī illā kullu mu'tadin aṡīm(in)."
  },
  {
    "verse_number": 13,
    "transcription": "Iżā tutlā 'alaihi āyātunā qāla asāṭīrul-awwalīn(a)."
  },
  {
    "verse_number": 14,
    "transcription": "Kallā bal…rāna 'alā qulūbihim mā kānū yaksibūn(a)."
  },
  {
    "verse_number": 15,
    "transcription": "Kallā innahum 'ar rabbihim yauma'iżil lamaḥjūbūn(a)."
  },
  {
    "verse_number": 16,
    "transcription": "Ṡumma innahum laṣālul-jaḥīm(i)."
  },
  {
    "verse_number": 17,
    "transcription": "Ṡumma yuqālu hāżal-lażī kuntum bihī tukażżibūn(a)."
  },
  {
    "verse_number": 18,
    "transcription": "Kallā inna kitābal-abrāri lafī 'illiyyīn(a)."
  },
  {
    "verse_number": 19,
    "transcription": "Wa mā adrāka mā 'illiyyūn(a)."
  },
  {
    "verse_number": 20,
    "transcription": "Kitābum marqūm(un)."
  },
  {
    "verse_number": 21,
    "transcription": "Yasyhaduhul-muqarrabūn(a)."
  },
  {
    "verse_number": 22,
    "transcription": "Innal-abrāra lafī na'īm(in)."
  },
  {
    "verse_number": 23,
    "transcription": "'Alal-arā'iki yanẓurūn(a)."
  },
  {
    "verse_number": 24,
    "transcription": "Ta'rifu fī wujūhihim naḍratan na'īm(i)."
  },
  {
    "verse_number": 25,
    "transcription": "Yusqauna mir raḥīqim makhtūm(in)."
  },
  {
    "verse_number": 26,
    "transcription": "Khitāmuhū misk(un), wa fī żālika falyatanāfasil-mutanāfisūn(a)."
  },
  {
    "verse_number": 27,
    "transcription": "Wa mizājuhū min tasnīm(in)."
  },
  {
    "verse_number": 28,
    "transcription": "'Ainay yasyrabu bihal-muqarrabūn(a)."
  },
  {
    "verse_number": 29,
    "transcription": "Innal-lażīna ajramū kānū minal-lażīna āmanū yaḍḥakūn(a)."
  },
  {
    "verse_number": 30,
    "transcription": "Wa iżā marrū bihim yatagāmazūn(a)."
  },
  {
    "verse_number": 31,
    "transcription": "Wa iżanqalabū ilā ahlihimunqalabū fakihīn(a)."
  },
  {
    "verse_number": 32,
    "transcription": "Wa iżā ra'auhum qālū inna hā'ulā'i laḍāllūn(a)."
  },
  {
    "verse_number": 33,
    "transcription": "Wa mā ursilū 'alaihim ḥāfiẓīn(a)."
  },
  {
    "verse_number": 34,
    "transcription": "Fal-yaumal-lażīna āmanū minal kuffāri yaḍḥakūn(a)."
  },
  {
    "verse_number": 35,
    "transcription": "'Alal-arā'iki yanẓurūn(a)."
  },
  {
    "verse_number": 36,
    "transcription": "Hal ṡuwwibal-kuffāru mā kānū yaf'alūn(a)."
  }
]