[
  {
    "verse_number": 1,
    "transcription": "Ṭā Sīm Mīm."
  },
  {
    "verse_number": 2,
    "transcription": "Tilka āyātul-kitābil-mubīn(i)."
  },
  {
    "verse_number": 3,
    "transcription": "La'allaka bākhi'un nafsaka allā yakūnū mu'minīn(a)."
  },
  {
    "verse_number": 4,
    "transcription": "In nasya' nunazzil 'alaihim minas-samā'i āyatan fa ẓallat a'nāquhum lahā khāḍi'īn(a)."
  },
  {
    "verse_number": 5,
    "transcription": "Wa mā ya'tīhim min żikrim minar-raḥmāni muḥdaṡin illā kānū 'anhu mu'riḍīn(a)."
  },
  {
    "verse_number": 6,
    "transcription": "Faqad każżabū fa saya'tīhim ambā'u mā kānū bihī yastahzi'ūn(a)."
  },
  {
    "verse_number": 7,
    "transcription": "Awalam yarau ilal-arḍi kam ambatnā fīhā min kulli zaujin karīm(in)."
  },
  {
    "verse_number": 8,
    "transcription": "Inna fī żālika la'āyah(tan), wa mā kāna akṡaruhum mu'minīn(a)."
  },
  {
    "verse_number": 9,
    "transcription": "Inna rabbaka lahuwal-'azīzur-raḥīm(u)."
  },
  {
    "verse_number": 10,
    "transcription": "Wa iż nādā rabbuka mūsā ani'til-qaumaẓ-ẓālimīn(a)."
  },
  {
    "verse_number": 11,
    "transcription": "Qauma fir'aun(a), alā yattaqūn(a)."
  },
  {
    "verse_number": 12,
    "transcription": "Qāla rabbi innī akhāfu ay yukażżibūn(i)."
  },
  {
    "verse_number": 13,
    "transcription": "Wa yaḍīqu ṣadrī wa lā yanṭaliqu lisānī fa arsil ilā hārūn(a)."
  },
  {
    "verse_number": 14,
    "transcription": "Wa lahum 'alayya żambun fa akhāfu ay yaqtulūn(i)."
  },
  {
    "verse_number": 15,
    "transcription": "Qāla kallā, fażhabā bi'āyātinā innā ma'akum mustami'ūn(a)."
  },
  {
    "verse_number": 16,
    "transcription": "Fa'tiyā fir'auna faqūlā innā rasūlu rabbil-'ālamīn(a)."
  },
  {
    "verse_number": 17,
    "transcription": "An arsil ma'anā banī isrā'īl(a)."
  },
  {
    "verse_number": 18,
    "transcription": "Qāla alam nurabbika fīnā walīdaw wa labiṡta fīnā min 'umurika sinīn(a)."
  },
  {
    "verse_number": 19,
    "transcription": "Wa fa'alta fa'latakal-latī fa'alta wa anta minal-kāfirīn(a)."
  },
  {
    "verse_number": 20,
    "transcription": "Qāla fa'altuhā iżaw wa ana minaḍ-ḍāllīn(a)."
  },
  {
    "verse_number": 21,
    "transcription": "Fa farartu minkum lammā khiftukum fa wahaba lī rabbī ḥukmaw wa ja'alanī minal-mursalīn(a)."
  },
  {
    "verse_number": 22,
    "transcription": "Wa tilka ni'matun tamunnuhā 'alayya an 'abbatta banī isrā'īl(a)."
  },
  {
    "verse_number": 23,
    "transcription": "Qāla fir'aunu wa mā rabbul-'ālamīn(a)."
  },
  {
    "verse_number": 24,
    "transcription": "Qāla rabbus-samāwāti wal-arḍi wa mā bainahumā, in kuntum mūqinīn(a)."
  },
  {
    "verse_number": 25,
    "transcription": "Qāla liman ḥaulahū alā tastami'ūn(a)."
  },
  {
    "verse_number": 26,
    "transcription": "Qāla rabbukum wa rabbu ābā'ikumul-awwalīn(a)."
  },
  {
    "verse_number": 27,
    "transcription": "Qāla inna rasūlakumul-lażī ursila ilaikum lamajnūn(un)."
  },
  {
    "verse_number": 28,
    "transcription": "Qāla rabbul-masyriqi wal-magribi wa mā bainahumā, in kuntum ta'qilūn(a)."
  },
  {
    "verse_number": 29,
    "transcription": "Qāla la'inittakhażta ilāhan gairī la'aj'alannaka minal-masjūnīn(a)."
  },
  {
    "verse_number": 30,
    "transcription": "Qāla awalau ji'tuka bisyai'im mubīn(in)."
  },
  {
    "verse_number": 31,
    "transcription": "Qāla fa'ti bihī in kunta minaṣ-ṣādiqīn(a)."
  },
  {
    "verse_number": 32,
    "transcription": "Fa alqā 'aṣāhu fa iżā hiya ṡu'bānum mubīn(un)."
  },
  {
    "verse_number": 33,
    "transcription": "Wa naza'a yadahū fa iżā hiya baiḍā'u lin-nāẓirīn(a)."
  },
  {
    "verse_number": 34,
    "transcription": "Qāla lil-mala'i ḥaulahū inna hāżā lasāḥirun 'alīm(un)."
  },
  {
    "verse_number": 35,
    "transcription": "Yurīdu ay yukhrijakum min arḍikum bisiḥrih(ī), fa māżā ta'murūn(a)."
  },
  {
    "verse_number": 36,
    "transcription": "Qālū arjih wa akhāhu wab'aṡ fil-madā'ini ḥāsyirīn(a)."
  },
  {
    "verse_number": 37,
    "transcription": "Ya'tūka bikulli saḥḥārin 'alīm(in)."
  },
  {
    "verse_number": 38,
    "transcription": "Fa jumi'as-saḥaratu limīqāti yaumim ma'lūm(in)."
  },
  {
    "verse_number": 39,
    "transcription": "Wa qīla lin-nāsi hal antum mujtami'ūn(a)."
  },
  {
    "verse_number": 40,
    "transcription": "La'allanā nattabi'us-saḥarata in kānū humul-gālibīn(a)."
  },
  {
    "verse_number": 41,
    "transcription": "Falammā jā'as-saḥaratu qālū lifir'auna a'inna lanā la'ajran in kunnā naḥnul-gālibīn(a)."
  },
  {
    "verse_number": 42,
    "transcription": "Qāla na'am wa innakum iżal laminal-muqarrabīn(a)."
  },
  {
    "verse_number": 43,
    "transcription": "Qāla lahum mūsā alqū mā antum mulqūn(a)."
  },
  {
    "verse_number": 44,
    "transcription": "Fa alqau ḥibālahum wa 'iṣiyyahum wa qālū bi'izzati fir'auna innā lanaḥnul-gālibūn(a)."
  },
  {
    "verse_number": 45,
    "transcription": "Fa alqā mūsā 'aṣāhu fa iżā hiya talqafu mā ya'fikūn(a)."
  },
  {
    "verse_number": 46,
    "transcription": "Fa ulqiyas-saḥaratu sājidīn(a)."
  },
  {
    "verse_number": 47,
    "transcription": "Qālū āmannā birabbil-'ālamīn(a)."
  },
  {
    "verse_number": 48,
    "transcription": "Rabbi mūsā wa hārūn(a)."
  },
  {
    "verse_number": 49,
    "transcription": "Qāla āmantum lahū qabla an āżana lakum, innahū lakabīrukumul-lażī 'allamakumus-siḥr(a), fa lasaufa ta'lamūn(a), la'uqaṭṭi'anna aidiyakum wa arjulakum min khilāfiw wa la'uṣallibannakum  ajma'īn(a)."
  },
  {
    "verse_number": 50,
    "transcription": "Qālū lā ḍair(a), innā ilā rabbinā munqalibūn(a)."
  },
  {
    "verse_number": 51,
    "transcription": "Innā naṭma'u ay yagfira lanā rabbunā khaṭāyānā an kunnā awwalal-mu'minīn(a)."
  },
  {
    "verse_number": 52,
    "transcription": "Wa auḥainā ilā mūsā an asri bi'ibādī innakum muttaba'ūn(a)."
  },
  {
    "verse_number": 53,
    "transcription": "Fa arsala fir'aunu fil-madā'ini ḥāsyirīn(a)."
  },
  {
    "verse_number": 54,
    "transcription": "Inna hā'ulā'i lasyirzimatun qalīlūn(a)."
  },
  {
    "verse_number": 55,
    "transcription": "Wa innahum lanā lagā'iẓūn(a)."
  },
  {
    "verse_number": 56,
    "transcription": "Wa innā lajamī'un ḥāżirūn(a)."
  },
  {
    "verse_number": 57,
    "transcription": "Fa akhrajnāhum min jannātiw wa 'uyūn(in)."
  },
  {
    "verse_number": 58,
    "transcription": "Wa kunūziw wa maqāmin karīm(in)."
  },
  {
    "verse_number": 59,
    "transcription": "Każālik(a), wa auraṡnāhā banī isrā'īl(a)."
  },
  {
    "verse_number": 60,
    "transcription": "Fa atba'ūhum musyriqīn(a)."
  },
  {
    "verse_number": 61,
    "transcription": "Falammā tarā'al-jam'āni qāla aṣḥābu mūsā innā lamudrakūn(a)."
  },
  {
    "verse_number": 62,
    "transcription": "Qāla kallā, inna ma'iya rabbī sayahdīn(i)."
  },
  {
    "verse_number": 63,
    "transcription": "Fa auḥainā ilā mūsā aniḍrib bi'aṣākal-baḥr(a), fanfalaqa fa kāna kullu firqin kaṭ-ṭaudil 'aẓīm(i)."
  },
  {
    "verse_number": 64,
    "transcription": "Wa azlafnā ṡammal-ākharīn(a)."
  },
  {
    "verse_number": 65,
    "transcription": "Wa anjainā mūsā wa mam ma'ahū ajma'īn(a)."
  },
  {
    "verse_number": 66,
    "transcription": "Ṡumma agraqnal-ākharīn(a)."
  },
  {
    "verse_number": 67,
    "transcription": "Inna fī żālika la'āyah(tan), wa mā kāna akṡaruhum mu'minīn(a)."
  },
  {
    "verse_number": 68,
    "transcription": "Wa inna rabbaka lahuwal-'azīzur-raḥīm(u)."
  },
  {
    "verse_number": 69,
    "transcription": "Watlu 'alaihim naba'a ibrāhīm(a)."
  },
  {
    "verse_number": 70,
    "transcription": "Iż qāla li'abīhi wa qaumihī mā ta'budūn(a)."
  },
  {
    "verse_number": 71,
    "transcription": "Qālū na'budu aṣnāman fa naẓallu lahā 'ākifīn(a)."
  },
  {
    "verse_number": 72,
    "transcription": "Qāla hal yasma'ūnakum iż tad'ūn(a)."
  },
  {
    "verse_number": 73,
    "transcription": "Au yanfa'ūnakum au yaḍurrūn(a)."
  },
  {
    "verse_number": 74,
    "transcription": "Qālū bal wajadnā ābā'anā każālika yaf'alūn(a)."
  },
  {
    "verse_number": 75,
    "transcription": "Qāla afa ra'aitum mā kuntum ta'budūn(a)."
  },
  {
    "verse_number": 76,
    "transcription": "Antum wa ābā'ukumul-aqdamūn(a)."
  },
  {
    "verse_number": 77,
    "transcription": "Fa innahum 'aduwwul lī illā rabbal-'ālamīn(a)."
  },
  {
    "verse_number": 78,
    "transcription": "Allażī khalaqanī fa huwa yahdīn(i)."
  },
  {
    "verse_number": 79,
    "transcription": "Wal-lażī huwa yuṭ'imunī wa yasqīn(i)."
  },
  {
    "verse_number": 80,
    "transcription": "Wa iżā mariḍtu fa huwa yasyfīn(i)."
  },
  {
    "verse_number": 81,
    "transcription": "Wal-lażī yumītunī ṡumma yuḥyīn(i)."
  },
  {
    "verse_number": 82,
    "transcription": "Wal-lażī aṭma'u ay yagfira lī khaṭī'atī yaumad-dīn(i)."
  },
  {
    "verse_number": 83,
    "transcription": "Rabbi hab lī ḥukmaw wa alḥiqnī biṣ-ṣāliḥīn(a)."
  },
  {
    "verse_number": 84,
    "transcription": "Waj'al lī lisāna ṣidqin fil-ākhirīn(a)."
  },
  {
    "verse_number": 85,
    "transcription": "Waj'alnī miw waraṡati janatin na'īm(i)."
  },
  {
    "verse_number": 86,
    "transcription": "Wagfir li'abī  innahū kāna minaḍ-ḍāllīn(a)."
  },
  {
    "verse_number": 87,
    "transcription": "Wa lā tukhzinī yauma yub'aṡūn(a)."
  },
  {
    "verse_number": 88,
    "transcription": "Yauma lā yanfa'u māluw wa lā banūn(a)."
  },
  {
    "verse_number": 89,
    "transcription": "Illā man atallāha biqalbin salīm(in)."
  },
  {
    "verse_number": 90,
    "transcription": "Wa uzlifatil-jannatu lil-muttaqīn(a)."
  },
  {
    "verse_number": 91,
    "transcription": "Wa burrizatil-jaḥīmu lil-gāwīn(a)."
  },
  {
    "verse_number": 92,
    "transcription": "Wa qīla lahum ainamā kuntum ta'budūn(a)."
  },
  {
    "verse_number": 93,
    "transcription": "Min dūnillāh(i), hal  yanṣurūnakum au yantaṣirūn(a)."
  },
  {
    "verse_number": 94,
    "transcription": "Fa kubkibū fīhā hum wal-gāwūn(a)."
  },
  {
    "verse_number": 95,
    "transcription": "Wa junūdu iblīsa ajma'ūn(a)."
  },
  {
    "verse_number": 96,
    "transcription": "Qālū wa hum fīhā yakhtaṣimūn(a)."
  },
  {
    "verse_number": 97,
    "transcription": "Tallāhi in kunnā lafī ḍalālim mubīn(in)."
  },
  {
    "verse_number": 98,
    "transcription": "Iż nusawwīkum birabbil-'ālamīn(a)."
  },
  {
    "verse_number": 99,
    "transcription": "Wa mā aḍallanā illal-mujrimūn(a)."
  },
  {
    "verse_number": 100,
    "transcription": "Famā lanā min syāfi'īn(a)."
  },
  {
    "verse_number": 101,
    "transcription": "Wa lā ṣadīqin ḥamīm(in)."
  },
  {
    "verse_number": 102,
    "transcription": "Falau anna lanā karratan fa nakūna minal-mu'minīn(a)."
  },
  {
    "verse_number": 103,
    "transcription": "Inna fī żālika la'āyah(tan), wa mā kāna akṡaruhum mu'minīn(a)."
  },
  {
    "verse_number": 104,
    "transcription": "Wa inna rabbaka lahuwal-'azīzur-raḥīm(u)."
  },
  {
    "verse_number": 105,
    "transcription": "Każżabat  qaumu nūḥinil-mursalīn(a)."
  },
  {
    "verse_number": 106,
    "transcription": "Iż qāla lahum akhūhum nūḥun alā tattaqūn(a)."
  },
  {
    "verse_number": 107,
    "transcription": "Innī lakum rasūlun amīn(un)."
  },
  {
    "verse_number": 108,
    "transcription": "Fattaqullāha wa aṭī'ūn(i)."
  },
  {
    "verse_number": 109,
    "transcription": "Wa mā as'alukum 'alaihi min ajr(in), in ajriya illā 'alā rabbil-'ālamīn(a)."
  },
  {
    "verse_number": 110,
    "transcription": "Fattaqullāha wa aṭī'ūn(i)."
  },
  {
    "verse_number": 111,
    "transcription": "Qālū anu'minu laka wattaba'akal-arżalūn(a)."
  },
  {
    "verse_number": 112,
    "transcription": "Qāla wa mā 'ilmī bimā kānū ya'malūn(a)."
  },
  {
    "verse_number": 113,
    "transcription": "In ḥisābuhum illā 'alā rabbī lau tasy'urūn(a)."
  },
  {
    "verse_number": 114,
    "transcription": "Wa mā ana biṭāridil-mu'minīn(a)."
  },
  {
    "verse_number": 115,
    "transcription": "In ana illā nażīrum mubīn(un)."
  },
  {
    "verse_number": 116,
    "transcription": "Qālū la'illam tantahi yā nūḥu latakūnanna minal-marjūmīn(a)."
  },
  {
    "verse_number": 117,
    "transcription": "Qāla rabbi inna qaumī każżabūn(i)."
  },
  {
    "verse_number": 118,
    "transcription": "Faftaḥ bainī wa bainahum fatḥaw wa najjinī wa mam ma'iya minal-mu'minīn(a)."
  },
  {
    "verse_number": 119,
    "transcription": "Fa anjaināhu wa mam ma'ahū fil-fulkil-masyḥūn(i)."
  },
  {
    "verse_number": 120,
    "transcription": "Ṡumma agraqnā ba'dul-bāqīn(a)."
  },
  {
    "verse_number": 121,
    "transcription": "Inna fī żālika la'āyah(tan), wa mā kāna akṡaruhum mu'minīn(a)."
  },
  {
    "verse_number": 122,
    "transcription": "Wa inna rabbaka lahuwal-'azīzur-raḥīm(u)."
  },
  {
    "verse_number": 123,
    "transcription": "Każżabat 'ādunil-mursalīn(a)."
  },
  {
    "verse_number": 124,
    "transcription": "Iż qāla lahum akhūhum hūdun alā tattaqūn(a)."
  },
  {
    "verse_number": 125,
    "transcription": "Innī lakum rasūlun amīn(un)."
  },
  {
    "verse_number": 126,
    "transcription": "Fattaqullāha wa aṭī'ūn(i)."
  },
  {
    "verse_number": 127,
    "transcription": "Wa mā as'alukum 'alaihi min ajr(in), in ajriya illā 'alā rabbil-'ālamīn(a)."
  },
  {
    "verse_number": 128,
    "transcription": "Atabnūna bikulli rī'in āyatan ta'baṡūn(a)."
  },
  {
    "verse_number": 129,
    "transcription": "Wa tattakhiżūna maṣāni'a la'allakum takhludūn(a)."
  },
  {
    "verse_number": 130,
    "transcription": "Wa iżā baṭasytum baṭasytum jabbārīn(a)."
  },
  {
    "verse_number": 131,
    "transcription": "Fattaqullāha wa aṭī'ūn(i)."
  },
  {
    "verse_number": 132,
    "transcription": "Wattaqul-lażī amaddakum bimā ta'lamūn(a)."
  },
  {
    "verse_number": 133,
    "transcription": "Amaddakum bi'an'āmiw wa banīn(a)."
  },
  {
    "verse_number": 134,
    "transcription": "Wa jannātiw wa 'uyūn(in)."
  },
  {
    "verse_number": 135,
    "transcription": "Innī akhāfu 'alaikum 'ażāba yaumin 'aẓīm(in)."
  },
  {
    "verse_number": 136,
    "transcription": "Qālū sawā'un 'alainā awa'aẓta am lam takum minal-wā'iẓīn(a)."
  },
  {
    "verse_number": 137,
    "transcription": "In hāżā illā khuluqul-awwalīn(a)."
  },
  {
    "verse_number": 138,
    "transcription": "Wa mā naḥnu bimu'ażżabīn(a)."
  },
  {
    "verse_number": 139,
    "transcription": "Fa każżabūhu fa ahlaknāhum, inna fī żālika la'āyah(tan),  wa mā kāna akṡaruhum mu'minīn(a)."
  },
  {
    "verse_number": 140,
    "transcription": "Wa inna rabbaka lahuwal-'azīzur-raḥīm(u)."
  },
  {
    "verse_number": 141,
    "transcription": "Każżabat ṡamūdul-mursalīn(a)."
  },
  {
    "verse_number": 142,
    "transcription": "Iż qāla lahum akhūhum ṣāliḥun alā tattaqūn(a)."
  },
  {
    "verse_number": 143,
    "transcription": "Innī lakum rasūlun amīn(un)."
  },
  {
    "verse_number": 144,
    "transcription": "Fattaqullāha wa aṭī'ūn(i)."
  },
  {
    "verse_number": 145,
    "transcription": "Wa mā as'alukum 'alaihi min ajr(in), in ajriya illā 'alā rabbil-'ālamīn(a)."
  },
  {
    "verse_number": 146,
    "transcription": "Atutrakūna fīmā hāhunā āminīn(a)."
  },
  {
    "verse_number": 147,
    "transcription": "Fī jannātiw wa 'uyūn(in)."
  },
  {
    "verse_number": 148,
    "transcription": "Wa zurū'iw wa nakhlin ṭal'uhā haḍīm(un)."
  },
  {
    "verse_number": 149,
    "transcription": "Wa tanḥitūna minal-jibāli buyūtan fārihīn(a)."
  },
  {
    "verse_number": 150,
    "transcription": "Fattaqullāha wa aṭī'ūn(i)."
  },
  {
    "verse_number": 151,
    "transcription": "Wa lā tuṭī'ū amral-musrifīn(a)."
  },
  {
    "verse_number": 152,
    "transcription": "Allażīna yufsidūna fil-arḍi wa lā yaṣliḥūn(a)."
  },
  {
    "verse_number": 153,
    "transcription": "Qālū innamā anta minal-musaḥḥarīn(a)."
  },
  {
    "verse_number": 154,
    "transcription": "Mā anta illā basyarum miṡlunā, fa'ti bi'āyatin in kunta minaṣ-ṣādiqīn(a)."
  },
  {
    "verse_number": 155,
    "transcription": "Qāla hāżihī nāqatul lahā syirbuw wa lakum syirbu yaumim ma'lūm(in)."
  },
  {
    "verse_number": 156,
    "transcription": "Wa lā tamassūhā bisū'in fa ya'khużakum 'ażābu yaumin 'aẓīm(in)."
  },
  {
    "verse_number": 157,
    "transcription": "Fa 'aqarūhā fa aṣbaḥū nādimīn(a)."
  },
  {
    "verse_number": 158,
    "transcription": "Fa akhażahumul-'ażāb(u), inna fī żālika la'āyah(tan), wa mā kāna akṡaruhum mu'minīn(a)."
  },
  {
    "verse_number": 159,
    "transcription": "Wa inna rabbaka lahuwal-'azīzur-raḥīm(u)."
  },
  {
    "verse_number": 160,
    "transcription": "Każżabat qaumu lūṭinil-mursalīn(a)."
  },
  {
    "verse_number": 161,
    "transcription": "Iż qāla lahum akhūhum lūṭun alā tattaqūn(a)."
  },
  {
    "verse_number": 162,
    "transcription": "Innī lakum rasūlun amīn(un)."
  },
  {
    "verse_number": 163,
    "transcription": "Fattaqullāha wa aṭī'ūn(i)."
  },
  {
    "verse_number": 164,
    "transcription": "Wa mā as'alukum 'alaihi min ajrin in ajriya illā 'alā rabbil-'ālamīn(a)."
  },
  {
    "verse_number": 165,
    "transcription": "Ata'tūnaż-żukrāna minal-'ālamīn(a)."
  },
  {
    "verse_number": 166,
    "transcription": "Wa tażarūna mā khalaqa lakum rabbukum min azwājikum, bal antum qamun 'ādūn(a)."
  },
  {
    "verse_number": 167,
    "transcription": "Qālū la'illam tantahi yā lūṭu latakūnanna minal-mukhrajīn(a)."
  },
  {
    "verse_number": 168,
    "transcription": "Qāla innī li'amalikum minal-qālīn(a)."
  },
  {
    "verse_number": 169,
    "transcription": "Rabbi najjinī wa ahlī mimmā ya'malūn(a)."
  },
  {
    "verse_number": 170,
    "transcription": "Fa najjaināhu wa ahlahū ajma'īn(a)."
  },
  {
    "verse_number": 171,
    "transcription": "Illā 'ajūzan fil-gābirīn(a)."
  },
  {
    "verse_number": 172,
    "transcription": "Ṡumma dammarnal-ākharīn(a)."
  },
  {
    "verse_number": 173,
    "transcription": "Wa amṭarnā 'alaihim maṭarā(n), fa sā'a maṭarul-munżarīn(a)."
  },
  {
    "verse_number": 174,
    "transcription": "Inna fī żālika la'āyah(tan),  wa mā kāna akṡaruhum mu'minīn(a)."
  },
  {
    "verse_number": 175,
    "transcription": "Wa inna rabbaka lahuwal-'azīzur-raḥīm(u)."
  },
  {
    "verse_number": 176,
    "transcription": "Każżaba aṣḥābul-aikatil-mursalīn(a)."
  },
  {
    "verse_number": 177,
    "transcription": "Iż qāla lahum syu'aibun alā tattaqūn(a)."
  },
  {
    "verse_number": 178,
    "transcription": "Innī lakum rasūlun amīn(un)."
  },
  {
    "verse_number": 179,
    "transcription": "Fattaqullāha wa aṭī'ūn(i)."
  },
  {
    "verse_number": 180,
    "transcription": "Wa mā as'alukum 'alaihi min ajrin in ajriya illā 'alā rabbil-'ālamīn(a)."
  },
  {
    "verse_number": 181,
    "transcription": "Auful-kaila wa lā takūnū minal-mukhsirīn(a)."
  },
  {
    "verse_number": 182,
    "transcription": "Wazinū bil-qisṭāsil-mustaqīm(i)."
  },
  {
    "verse_number": 183,
    "transcription": "Wa lā tabkhasun-nāsa asy-yā'ahum wa lā ta'ṡau fil-arḍi mufsidīn(a)."
  },
  {
    "verse_number": 184,
    "transcription": "Wattaqul-lażī khalaqakum wal-jibillatal-awwalīn(a)."
  },
  {
    "verse_number": 185,
    "transcription": "Qālū innamā anta minal-musaḥḥarīn(a)."
  },
  {
    "verse_number": 186,
    "transcription": "Wa mā anta illā basyarum miṡlunā wa in naẓunnuka laminal-kāżibīn(a)."
  },
  {
    "verse_number": 187,
    "transcription": "Fa asqiṭ 'alainā kisafam minas-samā'i in kunta minaṣ-ṣādiqīn(a)."
  },
  {
    "verse_number": 188,
    "transcription": "Qāla rabbī a'lamu bimā ta'malūn(a)."
  },
  {
    "verse_number": 189,
    "transcription": "Fa każżabūhu fa akhażahum 'ażābu yaumiẓ-ẓullah(ti), innahū kāna 'ażāba yaumin 'aẓīm(in)."
  },
  {
    "verse_number": 190,
    "transcription": "Inna fī żālika la'āyah(tan),  wa mā kāna akṡaruhum mu'minīn(a)."
  },
  {
    "verse_number": 191,
    "transcription": "Wa inna rabbaka lahuwal-'azīzur-raḥīm(u)."
  },
  {
    "verse_number": 192,
    "transcription": "Wa innahū latanzīlu rabbil-'ālamīn(a)."
  },
  {
    "verse_number": 193,
    "transcription": "Nazala bihir rūḥul-amīn(u)."
  },
  {
    "verse_number": 194,
    "transcription": "'Alā qalbika litakūna minal-munżirīn(a)."
  },
  {
    "verse_number": 195,
    "transcription": "Bilisānin 'arabiyyim mubīn(in)."
  },
  {
    "verse_number": 196,
    "transcription": "Wa innahū lafī zuburil-awwalīn(a)."
  },
  {
    "verse_number": 197,
    "transcription": "Awalam yakul lahum āyatan ay ya'lamahū 'ulamā'u banī isrā'īl(a)."
  },
  {
    "verse_number": 198,
    "transcription": "Wa lau nazzalnāhū 'alā ba'ḍil-a'jamīn(a)."
  },
  {
    "verse_number": 199,
    "transcription": "Fa qara'ahū 'alaihim mā kānū bihī mu'minīn(a)."
  },
  {
    "verse_number": 200,
    "transcription": "Każālika salaknāhu fī qulūbil-mujrimīn(a)."
  },
  {
    "verse_number": 201,
    "transcription": "Lā yu'minūna bihī ḥattā yarawul-'ażābal-alīm(a)."
  },
  {
    "verse_number": 202,
    "transcription": "Fa ya'tiyahum bagtataw wa hum lā yasy'urūn(a)."
  },
  {
    "verse_number": 203,
    "transcription": "Fa yaqūlū hal naḥnu munẓarūn(a)."
  },
  {
    "verse_number": 204,
    "transcription": "Afa bi'ażābinā yasta'jilūn(a)."
  },
  {
    "verse_number": 205,
    "transcription": "Afa ra'aita im matta'nāhum sinīn(a)."
  },
  {
    "verse_number": 206,
    "transcription": "Ṡumma jā'ahum mā kānū yū'adūn(a)."
  },
  {
    "verse_number": 207,
    "transcription": "Mā agnā 'anhum mā kānū yumatta'ūn(a)."
  },
  {
    "verse_number": 208,
    "transcription": "Wa mā ahlaknā min qaryatin illā lahā munżirūn(a)."
  },
  {
    "verse_number": 209,
    "transcription": "Żikrā, wa mā kunnā ẓālimīn(a)."
  },
  {
    "verse_number": 210,
    "transcription": "Wa mā tanazzalat bihisy-syayāṭīn(u)."
  },
  {
    "verse_number": 211,
    "transcription": "Wa mā yambagī lahum wa mā yastaṭī'ūn(a)."
  },
  {
    "verse_number": 212,
    "transcription": "Innahum 'anis-sam'i lama'zūlūn(a)."
  },
  {
    "verse_number": 213,
    "transcription": "Falā tad'u ma'allāhi ilāhan ākhara fa takūna minal-mu'ażżabīn(a)."
  },
  {
    "verse_number": 214,
    "transcription": "Wa anżir 'asyīratakal-aqrabīn(a)."
  },
  {
    "verse_number": 215,
    "transcription": "Wakhfiḍ janāḥaka limanittaba'aka minal-mu'minīn(a)."
  },
  {
    "verse_number": 216,
    "transcription": "Fa in 'aṣauka faqul innī barī'um mimmā ta'malūn(a)."
  },
  {
    "verse_number": 217,
    "transcription": "Wa tawakkal 'alal-'azīzir-raḥīm(i)."
  },
  {
    "verse_number": 218,
    "transcription": "Allażī yarāka ḥīna taqūm(u)."
  },
  {
    "verse_number": 219,
    "transcription": "Wa taqallubaka fis-sājidīn(a)."
  },
  {
    "verse_number": 220,
    "transcription": "Innahū huwas-samī'ul-'alīm(u)."
  },
  {
    "verse_number": 221,
    "transcription": "Hal unabbi'ukum 'alā man tanazzalusy-syayāṭīn(u)."
  },
  {
    "verse_number": 222,
    "transcription": "Tanazzalu 'alā kulli affākin aṡīm(in)."
  },
  {
    "verse_number": 223,
    "transcription": "Yulqūnas-sam'a wa akṡaruhum kāżibūn(a)."
  },
  {
    "verse_number": 224,
    "transcription": "Wasy-syu'arā'u yattabi'uhumul-gāwūn(a)."
  },
  {
    "verse_number": 225,
    "transcription": "Alam tara annahum fī kulli wādiy yahīmūn(a)."
  },
  {
    "verse_number": 226,
    "transcription": "Wa annahum yaqūlūna mā lā yaf'alūn(a)."
  },
  {
    "verse_number": 227,
    "transcription": "Illal-lażīna āmanū wa 'amiluṣ-ṣāliḥāti wa żakarullāha kaṡīraw wantaṣarū mim ba'di mā ẓulimū, wa saya'lamul-lażīna ẓalamū ayya munqalabiy yanqalibūn(a)."
  }
]