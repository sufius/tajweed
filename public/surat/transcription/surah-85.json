[
  {
    "verse_number": 1,
    "transcription": "Was-samā'i żātil-burūj(i)."
  },
  {
    "verse_number": 2,
    "transcription": "Wal-yaumil-mau'ūd(i)."
  },
  {
    "verse_number": 3,
    "transcription": "Wa syāhidiw wa masyhūd(in)."
  },
  {
    "verse_number": 4,
    "transcription": "Qutila aṣḥābul-ukhdūd(i)."
  },
  {
    "verse_number": 5,
    "transcription": "An-nāri żātil-waqūd(i)."
  },
  {
    "verse_number": 6,
    "transcription": "Iż hum 'alaihā qu'ūd(un)."
  },
  {
    "verse_number": 7,
    "transcription": "Wa hum 'alā mā yaf'alūna bil-mu'minīna syuhūd(un)."
  },
  {
    "verse_number": 8,
    "transcription": "Wa mā naqamū minhum illā ay yu'minū billāhil-'azīzil-ḥamīd(i)."
  },
  {
    "verse_number": 9,
    "transcription": "Allażī lahū mulkus-samāwāti wal-arḍ(i), wallāhu 'alā kulli syai'in syahīd(un)."
  },
  {
    "verse_number": 10,
    "transcription": "Innal-lażīna fatanul-mu'minīna wal-mu'mināti ṡumma lam yatūbū fa lahum 'ażābu jahannama wa lahum 'ażābul-ḥarīq(i)."
  },
  {
    "verse_number": 11,
    "transcription": "Innal-lażīna āmanū wa 'amiluṣ-ṣāliḥāti lahum jannātun tajrī min taḥtihal-anhār(u), żālikal-fauzul-kabīr(u)."
  },
  {
    "verse_number": 12,
    "transcription": "Inna baṭsya rabbika lasyadīd(un)."
  },
  {
    "verse_number": 13,
    "transcription": "Innahū huwa yubdi'u wa yu'īd(u)."
  },
  {
    "verse_number": 14,
    "transcription": "Wa huwal-gafūrul-wadūd(u)."
  },
  {
    "verse_number": 15,
    "transcription": "Żul-'arsyil-majīd(i)."
  },
  {
    "verse_number": 16,
    "transcription": "Fa''ālul limā yurīd(u)."
  },
  {
    "verse_number": 17,
    "transcription": "Hal atāka ḥadīṡul-junūd(i)."
  },
  {
    "verse_number": 18,
    "transcription": "Fir'auna wa ṡamūd(a)."
  },
  {
    "verse_number": 19,
    "transcription": "Balil-lażīna kafarū fī takżīb(in)."
  },
  {
    "verse_number": 20,
    "transcription": "Wallāhu miw warā'ihim muḥīṭ(un)."
  },
  {
    "verse_number": 21,
    "transcription": "Bal huwa qur'ānum majīd(un)."
  },
  {
    "verse_number": 22,
    "transcription": "Fī lauḥim maḥfūẓ(in)."
  }
]