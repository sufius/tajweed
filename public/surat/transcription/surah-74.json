[
  {
    "verse_number": 1,
    "transcription": "Yā ayyuhal-muddaṡṡir(u)."
  },
  {
    "verse_number": 2,
    "transcription": "Qum fa'anżir."
  },
  {
    "verse_number": 3,
    "transcription": "Wa rabbaka fakabbir."
  },
  {
    "verse_number": 4,
    "transcription": "Wa ṡiyābaka faṭahhir."
  },
  {
    "verse_number": 5,
    "transcription": "War-rujza fahjur."
  },
  {
    "verse_number": 6,
    "transcription": "Wa lā tamnun tastakṡir(u)."
  },
  {
    "verse_number": 7,
    "transcription": "Wa lirabbika faṣbir."
  },
  {
    "verse_number": 8,
    "transcription": "Fa iżā nuqira fin-nāqūr(i)."
  },
  {
    "verse_number": 9,
    "transcription": "Fa żālika yauma'iżiy yaumun 'asīr(un)."
  },
  {
    "verse_number": 10,
    "transcription": "'Alal-kāfirīna gairu yasīr(in)."
  },
  {
    "verse_number": 11,
    "transcription": "Żarnī wa man khalaqtu waḥīdā(n)."
  },
  {
    "verse_number": 12,
    "transcription": "Wa ja'altu lahū mālam mamdūdā(n)."
  },
  {
    "verse_number": 13,
    "transcription": "Wa banīna syuhūdā(n)."
  },
  {
    "verse_number": 14,
    "transcription": "Wa mahhattu lahū tamhīdā(n)."
  },
  {
    "verse_number": 15,
    "transcription": "Ṡumma yaṭma'u an azīd(a)."
  },
  {
    "verse_number": 16,
    "transcription": "Kallā, innahū kāna li'āyātinā 'anīdā(n)."
  },
  {
    "verse_number": 17,
    "transcription": "Sa'urhiquhū ṣa'ūdā(n)."
  },
  {
    "verse_number": 18,
    "transcription": "Innahū fakkara wa qaddar(a)."
  },
  {
    "verse_number": 19,
    "transcription": "Faqutila kaifa qaddar(a)."
  },
  {
    "verse_number": 20,
    "transcription": "Ṡumma qutila kaifa qaddar(a)."
  },
  {
    "verse_number": 21,
    "transcription": "Ṡumma naẓar(a)."
  },
  {
    "verse_number": 22,
    "transcription": "Ṡumma 'abasa wa basar(a)."
  },
  {
    "verse_number": 23,
    "transcription": "Ṡumma adbara wastakbar(a)."
  },
  {
    "verse_number": 24,
    "transcription": "Faqāla in hāżā illā siḥruy yu'ṡar(u)."
  },
  {
    "verse_number": 25,
    "transcription": "In hāżā illā qaulul-basyar(i)."
  },
  {
    "verse_number": 26,
    "transcription": "Sa'uṣlīhi saqar(a)."
  },
  {
    "verse_number": 27,
    "transcription": "Wa mā adrāka mā saqar(a)."
  },
  {
    "verse_number": 28,
    "transcription": "Lā tubqī wa lā tażar(u)."
  },
  {
    "verse_number": 29,
    "transcription": "Lawwāḥatul lil-basyar(i)."
  },
  {
    "verse_number": 30,
    "transcription": "'Alaihā tis'ata 'asyar(a)."
  },
  {
    "verse_number": 31,
    "transcription": "Wa mā ja'alnā aṣḥāban-nāri illā malā'ikah(tan), wa mā ja'alnā 'iddatahum illā fitnatal lil-lażīna kafarū, liyastaiqinal-lażīna ūtul-kitāba wa yazdādal-lażīna āmanū īmānaw wa lā yartābal-lażīna ūtul-kitāba wal-mu'minūn(a), wa liyaqūlal-lażīna fī qulūbihim maraḍuw wal-kāfirūna māżā arādallāhu bihāżā maṡalā(n), każālika yuḍillullāhu may yasyā'u wa yahdī may yasyā'(u), wa mā ya'lamu junūda rabbika illā huw(a), wa mā hiya illā żikrā lil-basyar(i)."
  },
  {
    "verse_number": 32,
    "transcription": "Kallā wal-qamar(i)."
  },
  {
    "verse_number": 33,
    "transcription": "Wal-laili iż adbar(a)."
  },
  {
    "verse_number": 34,
    "transcription": "Waṣ-ṣubḥi iżā asfar(a)."
  },
  {
    "verse_number": 35,
    "transcription": "Innahā la'iḥdal-kubar(i)."
  },
  {
    "verse_number": 36,
    "transcription": "Nażīral lil-basyar(i)."
  },
  {
    "verse_number": 37,
    "transcription": "Liman syā'a minkum ay yataqaddama au yata'akhkhar(a)."
  },
  {
    "verse_number": 38,
    "transcription": "Kullu nafsim bimā kasabat rahīnah(tun)."
  },
  {
    "verse_number": 39,
    "transcription": "Illā aṣḥābal-yamīn(i)."
  },
  {
    "verse_number": 40,
    "transcription": "Fī jannātin - yatasā'alūn(a)."
  },
  {
    "verse_number": 41,
    "transcription": "'Anil-mujrimīn(a)."
  },
  {
    "verse_number": 42,
    "transcription": "Mā salakakum fī saqar(a)."
  },
  {
    "verse_number": 43,
    "transcription": "Qālū lam naku minal-muṣallīn(a)."
  },
  {
    "verse_number": 44,
    "transcription": "Wa lam naku nuṭ'imul-miskīn(a)."
  },
  {
    "verse_number": 45,
    "transcription": "Wa kunnā nakhūḍu ma'al-khā'iḍīn(a)."
  },
  {
    "verse_number": 46,
    "transcription": "Wa kunnā nukażżibu biyaumid-dīn(i)."
  },
  {
    "verse_number": 47,
    "transcription": "Ḥattā atānal-yaqīn(u)."
  },
  {
    "verse_number": 48,
    "transcription": "Famā tanfa'uhum syafā'atusy-syāfi'īn(a)."
  },
  {
    "verse_number": 49,
    "transcription": "Famā lahum 'anit-tażkirati mu'riḍīn(a)."
  },
  {
    "verse_number": 50,
    "transcription": "Ka'annahum ḥumurum mustanfirah(tun)."
  },
  {
    "verse_number": 51,
    "transcription": "Farrat min qaswarah(tin)."
  },
  {
    "verse_number": 52,
    "transcription": "Bal yurīdu kullumri'im minhum ay yu'tā ṣuḥufam munasysyarah(tan)."
  },
  {
    "verse_number": 53,
    "transcription": "Kallā, bal lā yakhāfūnal-ākhirah(ta)."
  },
  {
    "verse_number": 54,
    "transcription": "Kallā innahū tażkirah(tun)."
  },
  {
    "verse_number": 55,
    "transcription": "Faman syā'a żakarah(ū)."
  },
  {
    "verse_number": 56,
    "transcription": "Wa mā yażkurūna illā ay yasyā'allāh(u), huwa ahlut-taqwā wa ahlul-magfirah(ti)."
  }
]