[
  {
    "verse_number": 1,
    "transcription": "Qul a'ūżu birabbil-falaq(i)."
  },
  {
    "verse_number": 2,
    "transcription": "Min syarri mā khalaq(a)."
  },
  {
    "verse_number": 3,
    "transcription": "Wa min syarri gāsiqin iżā waqab(a)."
  },
  {
    "verse_number": 4,
    "transcription": "Wa min syarrin-naffāṡāti fil-'uqad(i)."
  },
  {
    "verse_number": 5,
    "transcription": "Wa min syarri ḥāsidin iżā ḥasad(a)."
  }
]