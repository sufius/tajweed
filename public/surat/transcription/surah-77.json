[
  {
    "verse_number": 1,
    "transcription": "Wal-mursalāti 'urfā(n)."
  },
  {
    "verse_number": 2,
    "transcription": "Fal-'āṣifāti 'aṣfā(n)."
  },
  {
    "verse_number": 3,
    "transcription": "Wan-nāsyirāti nasyrā(n)."
  },
  {
    "verse_number": 4,
    "transcription": "Fal-fāriqāti farqā(n)."
  },
  {
    "verse_number": 5,
    "transcription": "Fal-mulqiyāti żikrā(n)."
  },
  {
    "verse_number": 6,
    "transcription": "'Użran au nużrā(n)."
  },
  {
    "verse_number": 7,
    "transcription": "Innamā tū'adūna lawāqi'(un)."
  },
  {
    "verse_number": 8,
    "transcription": "Fa iżan-nujūmu ṭumisat."
  },
  {
    "verse_number": 9,
    "transcription": "Wa iżas-samā'u furijat."
  },
  {
    "verse_number": 10,
    "transcription": "Wa iżal-jibālu nusifat."
  },
  {
    "verse_number": 11,
    "transcription": "Wa iżar-rusulu uqqitat."
  },
  {
    "verse_number": 12,
    "transcription": "Li'ayyi yaumin ujjilat."
  },
  {
    "verse_number": 13,
    "transcription": "Liyaumil-faṣl(i)."
  },
  {
    "verse_number": 14,
    "transcription": "Wa mā adrāka mā yaumul-faṣl(i)."
  },
  {
    "verse_number": 15,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 16,
    "transcription": "Alam nuhlikil-awwalīn(a)."
  },
  {
    "verse_number": 17,
    "transcription": "Ṡumma nutbi'uhumul-ākhirīn(a)."
  },
  {
    "verse_number": 18,
    "transcription": "Każālika naf'alu bil-mujrimīn(a)."
  },
  {
    "verse_number": 19,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 20,
    "transcription": "Alam nakhluqkum mim mā'im mahīn(in)."
  },
  {
    "verse_number": 21,
    "transcription": "Fa ja'alnāhu fī qarārim makīn(in)."
  },
  {
    "verse_number": 22,
    "transcription": "Ilā qadarim ma'lūm(in)."
  },
  {
    "verse_number": 23,
    "transcription": "Fa qadarnā, fani'mal-qādirūn(a)."
  },
  {
    "verse_number": 24,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 25,
    "transcription": "Alam naj'alil-arḍa kifātā(n)."
  },
  {
    "verse_number": 26,
    "transcription": "Aḥyā'aw wa amwātā(n)."
  },
  {
    "verse_number": 27,
    "transcription": "Wa ja'alnā fīhā rawāsiya syāmikhātiw wa asqainākum mā'an furātā(n)."
  },
  {
    "verse_number": 28,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 29,
    "transcription": "Inṭaliqū ilā mā kuntum bihī tukażżibūn(a)."
  },
  {
    "verse_number": 30,
    "transcription": "Inṭaliqū ilā ẓillin żī ṡalāṡi syu'ab(in)."
  },
  {
    "verse_number": 31,
    "transcription": "Lā ẓalīliw wa lā yugnī minal-lahab(i)."
  },
  {
    "verse_number": 32,
    "transcription": "Innahā tarmī bisyararin kal-qaṣr(i)."
  },
  {
    "verse_number": 33,
    "transcription": "Ka'annahū jimālatun ṣufr(un)."
  },
  {
    "verse_number": 34,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 35,
    "transcription": "Hāżā yaumu lā yanṭiqūn(a)."
  },
  {
    "verse_number": 36,
    "transcription": "Wa lā yu'żanu lahum fa ya'tażirūn(a)."
  },
  {
    "verse_number": 37,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 38,
    "transcription": "Hāżā yaumul-faṣli jama'nākum wal-awwalīn(a)."
  },
  {
    "verse_number": 39,
    "transcription": "Fa'in kāna lakum kaidun fakīdūn(i)."
  },
  {
    "verse_number": 40,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 41,
    "transcription": "Innal-muttaqīna fī ẓilāliw wa 'uyūn(in)."
  },
  {
    "verse_number": 42,
    "transcription": "Wa fawākiha mimmā yasytahūn(a)."
  },
  {
    "verse_number": 43,
    "transcription": "Kulū wasyrabū hanī'am bimā kuntum ta'malūn(a)."
  },
  {
    "verse_number": 44,
    "transcription": "Innā każālika najzil-muḥsinīn(a)."
  },
  {
    "verse_number": 45,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 46,
    "transcription": "Kulū wa tamatta'ū qalīlan innakum mujrimūn(a)."
  },
  {
    "verse_number": 47,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 48,
    "transcription": "Wa iżā qīla lahumurka'ū lā yarka'ūn(a)."
  },
  {
    "verse_number": 49,
    "transcription": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 50,
    "transcription": "Fa bi'ayyi ḥadīṡim ba'dahū yu'minūn(a)."
  }
]