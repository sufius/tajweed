[
  {
    "verse_number": 1,
    "transcription": "Alam tara kaifa fa'ala rabbuka bi'aṣḥābil-fīl(i)."
  },
  {
    "verse_number": 2,
    "transcription": "Alam yaj'al kaidahum fī taḍlīl(in)."
  },
  {
    "verse_number": 3,
    "transcription": "Wa arsala 'alaihim ṭairan abābīl(a)."
  },
  {
    "verse_number": 4,
    "transcription": "Tarmīhim biḥijāratim min sijjīl(in)."
  },
  {
    "verse_number": 5,
    "transcription": "Fa ja'alahum ka'aṣfim ma'kūl(in)."
  }
]