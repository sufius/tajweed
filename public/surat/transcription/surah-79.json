[
  {
    "verse_number": 1,
    "transcription": "Wan-nāzi'āti garqā(n)."
  },
  {
    "verse_number": 2,
    "transcription": "Wan-nāsyiṭāti nasyṭā(n)."
  },
  {
    "verse_number": 3,
    "transcription": "Was-sābiḥāti sabḥā(n)."
  },
  {
    "verse_number": 4,
    "transcription": "Fas-sābiqāti sabqā(n)."
  },
  {
    "verse_number": 5,
    "transcription": "Fal-mudabbirāti amrā(n)."
  },
  {
    "verse_number": 6,
    "transcription": "Yauma tarjufur-rājifah(tu)."
  },
  {
    "verse_number": 7,
    "transcription": "Tatba'uhar-rādifah(tu)."
  },
  {
    "verse_number": 8,
    "transcription": "Qulūbuy yauma'iżiw wājifah(tun)."
  },
  {
    "verse_number": 9,
    "transcription": "Abṣāruhā khāsyi'ah(tun)."
  },
  {
    "verse_number": 10,
    "transcription": "Yaqūlūna a'innā lamardūdūna fil-ḥāfirah(ti)."
  },
  {
    "verse_number": 11,
    "transcription": "A'iżā kunnā 'iẓāman nakhirah(tan)."
  },
  {
    "verse_number": 12,
    "transcription": "Qālū tilka iżan karratun khāsirah(tun)."
  },
  {
    "verse_number": 13,
    "transcription": "Fa innamā hiya zajratuw wāḥidah(tun)."
  },
  {
    "verse_number": 14,
    "transcription": "Fa iżā hum bis-sāhirah(ti)."
  },
  {
    "verse_number": 15,
    "transcription": "Hal atāka ḥadīṡu mūsā."
  },
  {
    "verse_number": 16,
    "transcription": "Iż nādāhu rabbuhū bil-wādil-muqaddasi ṭuwā(n)."
  },
  {
    "verse_number": 17,
    "transcription": "Iżhab ilā fir'auna innahū ṭagā."
  },
  {
    "verse_number": 18,
    "transcription": "Fa qul hal laka ilā an tazakkā."
  },
  {
    "verse_number": 19,
    "transcription": "Wa ahdiyaka ilā rabbika fa takhsyā."
  },
  {
    "verse_number": 20,
    "transcription": "Fa arāhul-āyatal-kubrā."
  },
  {
    "verse_number": 21,
    "transcription": "Fa każżaba wa 'aṣā."
  },
  {
    "verse_number": 22,
    "transcription": "Ṡumma adbara yas'ā."
  },
  {
    "verse_number": 23,
    "transcription": "Fa ḥasyara fanādā."
  },
  {
    "verse_number": 24,
    "transcription": "Fa qāla ana rabbukumul-a'lā."
  },
  {
    "verse_number": 25,
    "transcription": "Fa akhażahullāhu nakālal-ākhirati wal-ūlā."
  },
  {
    "verse_number": 26,
    "transcription": "Inna fī żālika la'ibratal limay yakhsyā."
  },
  {
    "verse_number": 27,
    "transcription": "A'antum asyaddu khalqan amis-samā'u banāhā."
  },
  {
    "verse_number": 28,
    "transcription": "Rafa'a samkahā fa sawwāhā."
  },
  {
    "verse_number": 29,
    "transcription": "Wa agṭasya lailahā wa akhraja ḍuḥāhā."
  },
  {
    "verse_number": 30,
    "transcription": "Wal-arḍa ba'da żālika daḥāhā."
  },
  {
    "verse_number": 31,
    "transcription": "Akhraja minhā mā'ahā wa mar'āhā."
  },
  {
    "verse_number": 32,
    "transcription": "Wal-jibāla arsāhā."
  },
  {
    "verse_number": 33,
    "transcription": "Matā'al lakum wa li'an'āmikum."
  },
  {
    "verse_number": 34,
    "transcription": "Fa iżā jā'atiṭ-ṭāmmatul-kubrā."
  },
  {
    "verse_number": 35,
    "transcription": "Yauma yatażakkarul-insānu mā sa'ā."
  },
  {
    "verse_number": 36,
    "transcription": "Wa burrizatil-jaḥīmu limay yarā."
  },
  {
    "verse_number": 37,
    "transcription": "Fa ammā man ṭagā."
  },
  {
    "verse_number": 38,
    "transcription": "Wa āṡaral-ḥayātad-dun-yā."
  },
  {
    "verse_number": 39,
    "transcription": "Fa innal-jaḥīma hiyal-ma'wā."
  },
  {
    "verse_number": 40,
    "transcription": "Wa ammā man khāfa maqāma rabbihī wa nahan-nafsa 'anil-hawā."
  },
  {
    "verse_number": 41,
    "transcription": "Fa innal-jannata hiyal-ma'wā."
  },
  {
    "verse_number": 42,
    "transcription": "Yas'alūnaka 'anis-sā'ati ayyāna mursāhā."
  },
  {
    "verse_number": 43,
    "transcription": "Fīma anta min żikrāhā."
  },
  {
    "verse_number": 44,
    "transcription": "Ilā rabbika muntahāhā."
  },
  {
    "verse_number": 45,
    "transcription": "Innamā anta munżiru may yakhsyāhā."
  },
  {
    "verse_number": 46,
    "transcription": "Ka'annahum yauma yaraunahā lam yalbaṡū illā 'asyiyyatan au ḍuḥāhā."
  }
]