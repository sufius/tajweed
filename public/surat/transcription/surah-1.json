[
  {
    "verse_number": 1,
    "transcription": "Bismillāhir-raḥmānir-raḥīm(i)."
  },
  {
    "verse_number": 2,
    "transcription": "Al-ḥamdu lillāhi rabbil-'ālamīn(a)."
  },
  {
    "verse_number": 3,
    "transcription": "Ar-raḥmānir-raḥīm(i)."
  },
  {
    "verse_number": 4,
    "transcription": "Māliki yaumid-dīn(i)."
  },
  {
    "verse_number": 5,
    "transcription": "Iyyāka na'budu wa iyyāka nasta'īn(u),"
  },
  {
    "verse_number": 6,
    "transcription": "Ihdinaṣ-ṣirāṭal-mustaqīm(a)."
  },
  {
    "verse_number": 7,
    "transcription": "Ṣirāṭal-lażīna an'amta 'alaihim, gairil-magḍūbi 'alaihim wa laḍ-ḍāllīn(a)."
  }
]