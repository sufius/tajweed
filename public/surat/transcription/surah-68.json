[
  {
    "verse_number": 1,
    "transcription": "Nūn, wal-qalami wa mā yasṭurūn(a)."
  },
  {
    "verse_number": 2,
    "transcription": "Mā anta bini'mati rabbika bimajnūn(in)."
  },
  {
    "verse_number": 3,
    "transcription": "Wa inna laka la'ajran gaira mamnūn(in)."
  },
  {
    "verse_number": 4,
    "transcription": "Wa innaka la'alā khuluqin 'aẓīm(in)."
  },
  {
    "verse_number": 5,
    "transcription": "Fasatubṣiru wa yubṣirūn(a)."
  },
  {
    "verse_number": 6,
    "transcription": "Bi'ayyikumul-maftūn(u)."
  },
  {
    "verse_number": 7,
    "transcription": "Inna rabbaka huwa a'lamu biman ḍalla 'an sabīlih(ī), wa huwa a'lamu bil-muhtadīn(a)."
  },
  {
    "verse_number": 8,
    "transcription": "Falā tuṭi'il-mukażżibīn(a)."
  },
  {
    "verse_number": 9,
    "transcription": "Waddū lau tudhinu fayudhinūn(a)."
  },
  {
    "verse_number": 10,
    "transcription": "Wa lā tuṭi' kulla ḥallāfim mahīn(in)."
  },
  {
    "verse_number": 11,
    "transcription": "Hammāzim masysyā'im binamīm(in)."
  },
  {
    "verse_number": 12,
    "transcription": "Mannā'il lil-khairi mu'tadin aṡīm(in)."
  },
  {
    "verse_number": 13,
    "transcription": "'Utullim ba'da żālika zanīm(in)."
  },
  {
    "verse_number": 14,
    "transcription": "An kāna żā māliw wa banīn(a)."
  },
  {
    "verse_number": 15,
    "transcription": "Iżā tutlā 'alaihi āyātunā qāla asāṭīrul-awwalīn(a)."
  },
  {
    "verse_number": 16,
    "transcription": "Sanasimuhū 'alal-khurṭūm(i)."
  },
  {
    "verse_number": 17,
    "transcription": "Innā balaunāhum kamā balaunā aṣḥābal-jannah(ti), iż aqsamū layaṣrimunnahā muṣbiḥīn(a)."
  },
  {
    "verse_number": 18,
    "transcription": "Wa lā yastaṡnūn(a)."
  },
  {
    "verse_number": 19,
    "transcription": "Fa ṭāfa 'alaihā ṭā'ifum mir rabbika wa hum nā'imūn(a)."
  },
  {
    "verse_number": 20,
    "transcription": "Fa aṣbaḥat kaṣ-ṣarīm(i)."
  },
  {
    "verse_number": 21,
    "transcription": "Fa tanādau muṣbiḥīn(a)."
  },
  {
    "verse_number": 22,
    "transcription": "Anigdū 'alā ḥarṡikum in kuntum ṣārimīn(a)."
  },
  {
    "verse_number": 23,
    "transcription": "Fanṭalaqū wa hum yatakhāfatūn(a)."
  },
  {
    "verse_number": 24,
    "transcription": "Allā yadkhulannahal-yauma 'alaikum miskīn(un)."
  },
  {
    "verse_number": 25,
    "transcription": "Wa gadau 'alā ḥardin qādirīn(a)."
  },
  {
    "verse_number": 26,
    "transcription": "Falammā ra'auhā qālū innā laḍāllūn(a)."
  },
  {
    "verse_number": 27,
    "transcription": "Bal naḥnu maḥrūmūn(a)."
  },
  {
    "verse_number": 28,
    "transcription": "Qāla ausaṭuhum alam aqul lakum lau lā tusabbiḥūn(a)."
  },
  {
    "verse_number": 29,
    "transcription": "Qālū subḥāna rabbinā innā kunnā ẓālimīn(a)."
  },
  {
    "verse_number": 30,
    "transcription": "Fa'aqbala ba'ḍuhum 'alā ba'ḍiy yatalāwamūn(a)."
  },
  {
    "verse_number": 31,
    "transcription": "Qālū yā wailanā innā kunnā ṭāgīn(a)."
  },
  {
    "verse_number": 32,
    "transcription": "'Asā rabbunā ay yubdilanā khairam minhā innā ilā rabbinā rāgibūn(a)."
  },
  {
    "verse_number": 33,
    "transcription": "Każālikal-'ażāb(u), wa la'ażābul-ākhirati akbar(u), lau kānū ya'lamūn(a)."
  },
  {
    "verse_number": 34,
    "transcription": "Inna lil-muttaqīna 'inda rabbihim jannātin-na'īm(i)."
  },
  {
    "verse_number": 35,
    "transcription": "Afanaj'alul-muslimīna kal-mujrimīn(a)."
  },
  {
    "verse_number": 36,
    "transcription": "Mā lakum, kaifa taḥkumūn(a)."
  },
  {
    "verse_number": 37,
    "transcription": "Am lakum kitābun fīhi tadrusūn(a)."
  },
  {
    "verse_number": 38,
    "transcription": "Inna lakum fīhi lamā takhayyarūn(a)."
  },
  {
    "verse_number": 39,
    "transcription": "Am lakum aimānun 'alainā bāligatun ilā yaumil-qiyāmah(ti), inna lakum lamā taḥkumūn(a)."
  },
  {
    "verse_number": 40,
    "transcription": "Salhum ayyuhum biżālika za'īm(un)."
  },
  {
    "verse_number": 41,
    "transcription": "Am lahum syurakā'(u), falya'tū bisyurakā'ihim in kānū ṣādiqīn(a)."
  },
  {
    "verse_number": 42,
    "transcription": "Yauma yuksyafu 'an sāqiw wa yud'auna ilas-sujūdi falā yastaṭī'ūn(a)."
  },
  {
    "verse_number": 43,
    "transcription": "Khāsyi'atan abṣāruhum tarhaquhum żillah(tun), wa qad kānū yud'auna ilas-sujūdi wa hum sālimun(a)."
  },
  {
    "verse_number": 44,
    "transcription": "Fażarnī wa may yukażżibu bihāżal-ḥadīṡi sanastadrijuhum min ḥaiṡu lā ya'lamūn(a)."
  },
  {
    "verse_number": 45,
    "transcription": "Wa umlī lahum, inna kaidī matīn(un)."
  },
  {
    "verse_number": 46,
    "transcription": "Am tas'aluhum ajran fahum mim magramim muṡqalūn(a)."
  },
  {
    "verse_number": 47,
    "transcription": "Am 'indahumul-gaibu fahum yaktubūn(a)."
  },
  {
    "verse_number": 48,
    "transcription": "Faṣbir liḥukmi rabbika wa lā takun kaṣāḥibil-ḥūt(i), iż nādā wa huwa makẓūm(un)."
  },
  {
    "verse_number": 49,
    "transcription": "Lau lā an tadārakahū ni'matum mir rabbihī lanubiża bil-'arā'i wa huwa mażmūm(un)."
  },
  {
    "verse_number": 50,
    "transcription": "Fajtabāhu rabbuhū faja'alahū minaṣ-ṣāliḥīn(a)."
  },
  {
    "verse_number": 51,
    "transcription": "Wa iy yakādul-lażīna kafarū layuzliqūnaka bi'abṣārihim lammā sami'uż żikra wa yaqūlūna innahū lamajnūn(un)."
  },
  {
    "verse_number": 52,
    "transcription": "Wa mā huwa illā żikrul lil-'ālamīn(a)."
  }
]