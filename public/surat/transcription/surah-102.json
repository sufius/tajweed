[
  {
    "verse_number": 1,
    "transcription": "Alhākumut-takāṡur(u)."
  },
  {
    "verse_number": 2,
    "transcription": "Ḥattā zurtumul-maqābir(a)."
  },
  {
    "verse_number": 3,
    "transcription": "Kallā saufa ta'lamūn(a)."
  },
  {
    "verse_number": 4,
    "transcription": "Ṡumma kallā saufa ta'lamūn(a)."
  },
  {
    "verse_number": 5,
    "transcription": "Kallā lau ta'lamūna 'ilmal-yaqīn(i)."
  },
  {
    "verse_number": 6,
    "transcription": "Latarawunnal-jaḥīm(a)."
  },
  {
    "verse_number": 7,
    "transcription": "Ṡumma latarawunnahā 'ainal-yaqīn(i)."
  },
  {
    "verse_number": 8,
    "transcription": "Ṡumma latus'alunna yauma'iżin 'anin-na'īm(i)."
  }
]