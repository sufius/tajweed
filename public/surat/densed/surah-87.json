[
  {
    "verse_number": 1,
    "text_uthmani": " سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
    "text_uthmani_transcribed": "Sabbiḥisma rabbikal-a'lā."
  },
  {
    "verse_number": 2,
    "text_uthmani": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
    "text_uthmani_transcribed": "Allażī khalaqa fasawwā."
  },
  {
    "verse_number": 3,
    "text_uthmani": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
    "text_uthmani_transcribed": "Wal-lażī qaddara fahadā."
  },
  {
    "verse_number": 4,
    "text_uthmani": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
    "text_uthmani_transcribed": "Wal-lażī akhrajal-mar'ā."
  },
  {
    "verse_number": 5,
    "text_uthmani": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
    "text_uthmani_transcribed": "Fa ja'alahū guṡā'an aḥwā."
  },
  {
    "verse_number": 6,
    "text_uthmani": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
    "text_uthmani_transcribed": "Sanuqri'uka falā tansā."
  },
  {
    "verse_number": 7,
    "text_uthmani": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
    "text_uthmani_transcribed": "Illā mā syā'allāh(u), innahū ya'lamul-jahra wa mā yakhfā."
  },
  {
    "verse_number": 8,
    "text_uthmani": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
    "text_uthmani_transcribed": "Wa nuyassiruka lil-yusrā."
  },
  {
    "verse_number": 9,
    "text_uthmani": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
    "text_uthmani_transcribed": "Fa żakkir in nafa'atiż-żikrā."
  },
  {
    "verse_number": 10,
    "text_uthmani": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
    "text_uthmani_transcribed": "Sayażżakkaru may yakhsyā."
  },
  {
    "verse_number": 11,
    "text_uthmani": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
    "text_uthmani_transcribed": "Wa yatajannabuhal-asyqā."
  },
  {
    "verse_number": 12,
    "text_uthmani": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
    "text_uthmani_transcribed": "Allażī yaṣlan-nāral-kubrā."
  },
  {
    "verse_number": 13,
    "text_uthmani": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
    "text_uthmani_transcribed": "Ṡumma lā yamūtu fīhā wa lā yaḥyā."
  },
  {
    "verse_number": 14,
    "text_uthmani": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
    "text_uthmani_transcribed": "Qad aflaḥa man tazakkā."
  },
  {
    "verse_number": 15,
    "text_uthmani": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
    "text_uthmani_transcribed": "Wa żakarasma rabbihī fa ṣallā."
  },
  {
    "verse_number": 16,
    "text_uthmani": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
    "text_uthmani_transcribed": "Bal tu'ṡirūnal-ḥayātad-dun-yā."
  },
  {
    "verse_number": 17,
    "text_uthmani": "وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ",
    "text_uthmani_transcribed": "Wal-ākhiratu khairuw wa abqā."
  },
  {
    "verse_number": 18,
    "text_uthmani": "إِنَّ هَـٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
    "text_uthmani_transcribed": "Inna hāżā lafiṣ-ṣuḥufil-ūlā."
  },
  {
    "verse_number": 19,
    "text_uthmani": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ",
    "text_uthmani_transcribed": "Ṣuḥufi ibrāhīma wa mūsā."
  }
]