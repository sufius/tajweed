[
  {
    "verse_number": 1,
    "arabic": " سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
    "transcription": "Sabbiḥisma rabbikal-a'lā."
  },
  {
    "verse_number": 2,
    "arabic": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
    "transcription": "Allażī khalaqa fasawwā."
  },
  {
    "verse_number": 3,
    "arabic": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
    "transcription": "Wal-lażī qaddara fahadā."
  },
  {
    "verse_number": 4,
    "arabic": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
    "transcription": "Wal-lażī akhrajal-mar'ā."
  },
  {
    "verse_number": 5,
    "arabic": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
    "transcription": "Fa ja'alahū guṡā'an aḥwā."
  },
  {
    "verse_number": 6,
    "arabic": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
    "transcription": "Sanuqri'uka falā tansā."
  },
  {
    "verse_number": 7,
    "arabic": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
    "transcription": "Illā mā syā'allāh(u), innahū ya'lamul-jahra wa mā yakhfā."
  },
  {
    "verse_number": 8,
    "arabic": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
    "transcription": "Wa nuyassiruka lil-yusrā."
  },
  {
    "verse_number": 9,
    "arabic": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
    "transcription": "Fa żakkir in nafa'atiż-żikrā."
  },
  {
    "verse_number": 10,
    "arabic": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
    "transcription": "Sayażżakkaru may yakhsyā."
  },
  {
    "verse_number": 11,
    "arabic": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
    "transcription": "Wa yatajannabuhal-asyqā."
  },
  {
    "verse_number": 12,
    "arabic": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
    "transcription": "Allażī yaṣlan-nāral-kubrā."
  },
  {
    "verse_number": 13,
    "arabic": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
    "transcription": "Ṡumma lā yamūtu fīhā wa lā yaḥyā."
  },
  {
    "verse_number": 14,
    "arabic": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
    "transcription": "Qad aflaḥa man tazakkā."
  },
  {
    "verse_number": 15,
    "arabic": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
    "transcription": "Wa żakarasma rabbihī fa ṣallā."
  },
  {
    "verse_number": 16,
    "arabic": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
    "transcription": "Bal tu'ṡirūnal-ḥayātad-dun-yā."
  },
  {
    "verse_number": 17,
    "arabic": "وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ",
    "transcription": "Wal-ākhiratu khairuw wa abqā."
  },
  {
    "verse_number": 18,
    "arabic": "إِنَّ هَـٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
    "transcription": "Inna hāżā lafiṣ-ṣuḥufil-ūlā."
  },
  {
    "verse_number": 19,
    "arabic": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ",
    "transcription": "Ṣuḥufi ibrāhīma wa mūsā."
  }
]