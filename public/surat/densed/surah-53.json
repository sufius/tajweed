[
  {
    "verse_number": 1,
    "text_uthmani": " وَٱلنَّجْمِ إِذَا هَوَىٰ",
    "text_uthmani_transcribed": "Wan-najmi iżā hawā."
  },
  {
    "verse_number": 2,
    "text_uthmani": "مَا ضَلَّ صَاحِبُكُمْ وَمَا غَوَىٰ",
    "text_uthmani_transcribed": "Mā ḍalla ṣāḥibukum wa mā gawā."
  },
  {
    "verse_number": 3,
    "text_uthmani": "وَمَا يَنطِقُ عَنِ ٱلْهَوَىٰٓ",
    "text_uthmani_transcribed": "Wa mā yanṭiqu 'anil-hawā."
  },
  {
    "verse_number": 4,
    "text_uthmani": "إِنْ هُوَ إِلَّا وَحْىٌ يُوحَىٰ",
    "text_uthmani_transcribed": "In huwa illā waḥyuy yūḥā."
  },
  {
    "verse_number": 5,
    "text_uthmani": "عَلَّمَهُۥ شَدِيدُ ٱلْقُوَىٰ",
    "text_uthmani_transcribed": "'Allamahū syadīdul-quwā."
  },
  {
    "verse_number": 6,
    "text_uthmani": "ذُو مِرَّةٍ فَٱسْتَوَىٰ",
    "text_uthmani_transcribed": "Żū mirrah(tin), fastawā."
  },
  {
    "verse_number": 7,
    "text_uthmani": "وَهُوَ بِٱلْأُفُقِ ٱلْأَعْلَىٰ",
    "text_uthmani_transcribed": "Wa huwa bil-ufuqil-a'lā."
  },
  {
    "verse_number": 8,
    "text_uthmani": "ثُمَّ دَنَا فَتَدَلَّىٰ",
    "text_uthmani_transcribed": "Ṡumma danā fa tadallā."
  },
  {
    "verse_number": 9,
    "text_uthmani": "فَكَانَ قَابَ قَوْسَيْنِ أَوْ أَدْنَىٰ",
    "text_uthmani_transcribed": "Fa kāna qāba qausaini au adnā."
  },
  {
    "verse_number": 10,
    "text_uthmani": "فَأَوْحَىٰٓ إِلَىٰ عَبْدِهِۦ مَآ أَوْحَىٰ",
    "text_uthmani_transcribed": "Fa auḥā ilā 'abdihī mā auḥā."
  },
  {
    "verse_number": 11,
    "text_uthmani": "مَا كَذَبَ ٱلْفُؤَادُ مَا رَأَىٰٓ",
    "text_uthmani_transcribed": "Mā każabal-fu'ādu mā ra'ā."
  },
  {
    "verse_number": 12,
    "text_uthmani": "أَفَتُمَـٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
    "text_uthmani_transcribed": "Afa tumārūnahū 'alā mā yarā."
  },
  {
    "verse_number": 13,
    "text_uthmani": "وَلَقَدْ رَءَاهُ نَزْلَةً أُخْرَىٰ",
    "text_uthmani_transcribed": "Wa laqad ra'āhu nazlatan ukhrā."
  },
  {
    "verse_number": 14,
    "text_uthmani": "عِندَ سِدْرَةِ ٱلْمُنتَهَىٰ",
    "text_uthmani_transcribed": "'Inda sidratil-muntahā."
  },
  {
    "verse_number": 15,
    "text_uthmani": "عِندَهَا جَنَّةُ ٱلْمَأْوَىٰٓ",
    "text_uthmani_transcribed": "'Indahā jannatul-ma'wā."
  },
  {
    "verse_number": 16,
    "text_uthmani": "إِذْ يَغْشَى ٱلسِّدْرَةَ مَا يَغْشَىٰ",
    "text_uthmani_transcribed": "Iż yagsyas-sidrata mā yagsyā."
  },
  {
    "verse_number": 17,
    "text_uthmani": "مَا زَاغَ ٱلْبَصَرُ وَمَا طَغَىٰ",
    "text_uthmani_transcribed": "Mā zāgal-baṣaru wa mā ṭagā."
  },
  {
    "verse_number": 18,
    "text_uthmani": "لَقَدْ رَأَىٰ مِنْ ءَايَـٰتِ رَبِّهِ ٱلْكُبْرَىٰٓ",
    "text_uthmani_transcribed": "Laqad ra'ā min āyāti rabbihil-kubrā."
  },
  {
    "verse_number": 19,
    "text_uthmani": "أَفَرَءَيْتُمُ ٱللَّـٰتَ وَٱلْعُزَّىٰ",
    "text_uthmani_transcribed": "Afa ra'aitumul-lāta wal-'uzzā."
  },
  {
    "verse_number": 20,
    "text_uthmani": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلْأُخْرَىٰٓ",
    "text_uthmani_transcribed": "Wa manātaṡ-ṡāliṡatal-ukhrā."
  },
  {
    "verse_number": 21,
    "text_uthmani": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلْأُنثَىٰ",
    "text_uthmani_transcribed": "Alakumuż-żakaru wa lahul-unṡā."
  },
  {
    "verse_number": 22,
    "text_uthmani": "تِلْكَ إِذًا قِسْمَةٌ ضِيزَىٰٓ",
    "text_uthmani_transcribed": "Tilka iżan qismatun ḍīzā."
  },
  {
    "verse_number": 23,
    "text_uthmani": "إِنْ هِىَ إِلَّآ أَسْمَآءٌ سَمَّيْتُمُوهَآ أَنتُمْ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلْطَـٰنٍ ۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهْوَى ٱلْأَنفُسُ ۖ وَلَقَدْ جَآءَهُم مِّن رَّبِّهِمُ ٱلْهُدَىٰٓ",
    "text_uthmani_transcribed": "In hiya  illā asmā'un sammaitumūhā antum wa ābā'ukum mā anzalallāhu bihā min sulṭān(in), iy yattabi'ūna illaẓ-ẓanna wa mā tahwal-anfus(u), wa laqad jā'ahum mir rabbihimul-hudā."
  },
  {
    "verse_number": 24,
    "text_uthmani": "أَمْ لِلْإِنسَـٰنِ مَا تَمَنَّىٰ",
    "text_uthmani_transcribed": "Am lil-insāni mā tamannā."
  },
  {
    "verse_number": 25,
    "text_uthmani": "فَلِلَّهِ ٱلْـَٔاخِرَةُ وَٱلْأُولَىٰ",
    "text_uthmani_transcribed": "Fa lillāhil-ākhiratu wal-ūlā."
  },
  {
    "verse_number": 26,
    "text_uthmani": "۞ وَكَم مِّن مَّلَكٍ فِى ٱلسَّمَـٰوَٰتِ لَا تُغْنِى شَفَـٰعَتُهُمْ شَيْـًٔا إِلَّا مِنۢ بَعْدِ أَن يَأْذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرْضَىٰٓ",
    "text_uthmani_transcribed": "Wa kam mim malakin fis-samāwāti lā tugnī syafā'atuhum syai'an illā mim ba'di ay ya'żanallāhu limay yasyā'u wa yarḍā."
  },
  {
    "verse_number": 27,
    "text_uthmani": "إِنَّ ٱلَّذِينَ لَا يُؤْمِنُونَ بِٱلْـَٔاخِرَةِ لَيُسَمُّونَ ٱلْمَلَـٰٓئِكَةَ تَسْمِيَةَ ٱلْأُنثَىٰ",
    "text_uthmani_transcribed": "Innal-lażīna lā yu'minūna bil-ākhirati layusammūnal-malā'ikata tasmiyatal-unṡā."
  },
  {
    "verse_number": 28,
    "text_uthmani": "وَمَا لَهُم بِهِۦ مِنْ عِلْمٍ ۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ ۖ وَإِنَّ ٱلظَّنَّ لَا يُغْنِى مِنَ ٱلْحَقِّ شَيْـًٔا",
    "text_uthmani_transcribed": "Wa mā lahum bihī min 'ilm(in), iy yattabi'ūna illaẓ-ẓann(a), wa innaẓ-ẓanna lā yugnī minal-ḥaqqi syai'ā(n)."
  },
  {
    "verse_number": 29,
    "text_uthmani": "فَأَعْرِضْ عَن مَّن تَوَلَّىٰ عَن ذِكْرِنَا وَلَمْ يُرِدْ إِلَّا ٱلْحَيَوٰةَ ٱلدُّنْيَا",
    "text_uthmani_transcribed": "Fa a'riḍ 'am man tawallā, 'an żikrinā wa lam yurid illal-ḥayātad-dun-yā."
  },
  {
    "verse_number": 30,
    "text_uthmani": "ذَٰلِكَ مَبْلَغُهُم مِّنَ ٱلْعِلْمِ ۚ إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعْلَمُ بِمَنِ ٱهْتَدَىٰ",
    "text_uthmani_transcribed": "Żālika mablaguhum minal-'ilm(i), inna rabbaka huwa a'lamu biman ḍalla 'an sabīlih(ī), wa huwa a'lamu bimanihtadā."
  },
  {
    "verse_number": 31,
    "text_uthmani": "وَلِلَّهِ مَا فِى ٱلسَّمَـٰوَٰتِ وَمَا فِى ٱلْأَرْضِ لِيَجْزِىَ ٱلَّذِينَ أَسَـٰٓـُٔوا۟ بِمَا عَمِلُوا۟ وَيَجْزِىَ ٱلَّذِينَ أَحْسَنُوا۟ بِٱلْحُسْنَى",
    "text_uthmani_transcribed": "Wa lillāhi mā fis-samāwāti wa mā fil-arḍ(i), liyajziyal-lażīna asā'ū bimā 'amilū wa yajziyal-lażīna aḥsanū bil-ḥusnā."
  },
  {
    "verse_number": 32,
    "text_uthmani": "ٱلَّذِينَ يَجْتَنِبُونَ كَبَـٰٓئِرَ ٱلْإِثْمِ وَٱلْفَوَٰحِشَ إِلَّا ٱللَّمَمَ ۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلْمَغْفِرَةِ ۚ هُوَ أَعْلَمُ بِكُمْ إِذْ أَنشَأَكُم مِّنَ ٱلْأَرْضِ وَإِذْ أَنتُمْ أَجِنَّةٌ فِى بُطُونِ أُمَّهَـٰتِكُمْ ۖ فَلَا تُزَكُّوٓا۟ أَنفُسَكُمْ ۖ هُوَ أَعْلَمُ بِمَنِ ٱتَّقَىٰٓ",
    "text_uthmani_transcribed": "Allażīna yajtanibūna kabā'iral-iṡmi wal-fawāḥisya illal-lamam(a), inna rabbaka wāsi'ul-magfirah(ti), huwa a'lamu bikum iż ansya'akum minal-arḍi wa iż antum ajinnatun fī buṭūni ummahātikum, falā tuzakkū anfusakum, huwa a'lamu bimanittaqā."
  },
  {
    "verse_number": 33,
    "text_uthmani": "أَفَرَءَيْتَ ٱلَّذِى تَوَلَّىٰ",
    "text_uthmani_transcribed": "Afa ra'aital-lażī tawallā."
  },
  {
    "verse_number": 34,
    "text_uthmani": "وَأَعْطَىٰ قَلِيلًا وَأَكْدَىٰٓ",
    "text_uthmani_transcribed": "Wa a'ṭā qalīlaw wa akdā."
  },
  {
    "verse_number": 35,
    "text_uthmani": "أَعِندَهُۥ عِلْمُ ٱلْغَيْبِ فَهُوَ يَرَىٰٓ",
    "text_uthmani_transcribed": "A'indahū 'ilmul-gaibi fahuwa yarā."
  },
  {
    "verse_number": 36,
    "text_uthmani": "أَمْ لَمْ يُنَبَّأْ بِمَا فِى صُحُفِ مُوسَىٰ",
    "text_uthmani_transcribed": "Am lam yunabba' bimā fī ṣuḥufi mūsā."
  },
  {
    "verse_number": 37,
    "text_uthmani": "وَإِبْرَٰهِيمَ ٱلَّذِى وَفَّىٰٓ",
    "text_uthmani_transcribed": "Wa ibrāhīmal-lażī waffā."
  },
  {
    "verse_number": 38,
    "text_uthmani": "أَلَّا تَزِرُ وَازِرَةٌ وِزْرَ أُخْرَىٰ",
    "text_uthmani_transcribed": "Allā taziru wāziratuw wizra ukhrā."
  },
  {
    "verse_number": 39,
    "text_uthmani": "وَأَن لَّيْسَ لِلْإِنسَـٰنِ إِلَّا مَا سَعَىٰ",
    "text_uthmani_transcribed": "Wa al laisa lil-insāni illā mā sa'ā."
  },
  {
    "verse_number": 40,
    "text_uthmani": "وَأَنَّ سَعْيَهُۥ سَوْفَ يُرَىٰ",
    "text_uthmani_transcribed": "Wa anna sa'yahū saufa yurā."
  },
  {
    "verse_number": 41,
    "text_uthmani": "ثُمَّ يُجْزَىٰهُ ٱلْجَزَآءَ ٱلْأَوْفَىٰ",
    "text_uthmani_transcribed": "Ṡumma yujzāhul-jazā'al-aufā."
  },
  {
    "verse_number": 42,
    "text_uthmani": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلْمُنتَهَىٰ",
    "text_uthmani_transcribed": "Wa anna ilā rabbikal-muntahā."
  },
  {
    "verse_number": 43,
    "text_uthmani": "وَأَنَّهُۥ هُوَ أَضْحَكَ وَأَبْكَىٰ",
    "text_uthmani_transcribed": "Wa annahū huwa aḍḥaka wa abkā."
  },
  {
    "verse_number": 44,
    "text_uthmani": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحْيَا",
    "text_uthmani_transcribed": "Wa annahū huwa amāta wa aḥyā."
  },
  {
    "verse_number": 45,
    "text_uthmani": "وَأَنَّهُۥ خَلَقَ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰ",
    "text_uthmani_transcribed": "Wa annahū khalaqaz-zaujainiż-żakara wal-unṡā."
  },
  {
    "verse_number": 46,
    "text_uthmani": "مِن نُّطْفَةٍ إِذَا تُمْنَىٰ",
    "text_uthmani_transcribed": "Min nuṭfatin iżā tumnā."
  },
  {
    "verse_number": 47,
    "text_uthmani": "وَأَنَّ عَلَيْهِ ٱلنَّشْأَةَ ٱلْأُخْرَىٰ",
    "text_uthmani_transcribed": "Wa anna 'alaihin nasy'atal-ukhrā."
  },
  {
    "verse_number": 48,
    "text_uthmani": "وَأَنَّهُۥ هُوَ أَغْنَىٰ وَأَقْنَىٰ",
    "text_uthmani_transcribed": "Wa annahū huwa agnā wa aqnā."
  },
  {
    "verse_number": 49,
    "text_uthmani": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعْرَىٰ",
    "text_uthmani_transcribed": "Wa annahū huwa rabbusy-syi'rā."
  },
  {
    "verse_number": 50,
    "text_uthmani": "وَأَنَّهُۥٓ أَهْلَكَ عَادًا ٱلْأُولَىٰ",
    "text_uthmani_transcribed": "Wa annahū ahlaka 'ādanil-ūlā."
  },
  {
    "verse_number": 51,
    "text_uthmani": "وَثَمُودَا۟ فَمَآ أَبْقَىٰ",
    "text_uthmani_transcribed": "Wa ṡamūda famā abqā."
  },
  {
    "verse_number": 52,
    "text_uthmani": "وَقَوْمَ نُوحٍ مِّن قَبْلُ ۖ إِنَّهُمْ كَانُوا۟ هُمْ أَظْلَمَ وَأَطْغَىٰ",
    "text_uthmani_transcribed": "Wa qauma nūḥim min qabl(u), innahum kānū hum aẓlama wa aṭgā."
  },
  {
    "verse_number": 53,
    "text_uthmani": "وَٱلْمُؤْتَفِكَةَ أَهْوَىٰ",
    "text_uthmani_transcribed": "Wal-mu'tafikata ahwā."
  },
  {
    "verse_number": 54,
    "text_uthmani": "فَغَشَّىٰهَا مَا غَشَّىٰ",
    "text_uthmani_transcribed": "Fa gasysyāhā mā gasysyā."
  },
  {
    "verse_number": 55,
    "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكَ تَتَمَارَىٰ",
    "text_uthmani_transcribed": "Fa bi'ayyi ālā'i rabbika tatamārā."
  },
  {
    "verse_number": 56,
    "text_uthmani": "هَـٰذَا نَذِيرٌ مِّنَ ٱلنُّذُرِ ٱلْأُولَىٰٓ",
    "text_uthmani_transcribed": "Hāżā nażīrum minan-nużuril-ūlā."
  },
  {
    "verse_number": 57,
    "text_uthmani": "أَزِفَتِ ٱلْـَٔازِفَةُ",
    "text_uthmani_transcribed": "Azifatil-āzifah(tu)."
  },
  {
    "verse_number": 58,
    "text_uthmani": "لَيْسَ لَهَا مِن دُونِ ٱللَّهِ كَاشِفَةٌ",
    "text_uthmani_transcribed": "Laisa lahā min dūnillāhi kāsyifah(tun)."
  },
  {
    "verse_number": 59,
    "text_uthmani": "أَفَمِنْ هَـٰذَا ٱلْحَدِيثِ تَعْجَبُونَ",
    "text_uthmani_transcribed": "Afamin hāżal-ḥadīṡi ta'jabūn(a)."
  },
  {
    "verse_number": 60,
    "text_uthmani": "وَتَضْحَكُونَ وَلَا تَبْكُونَ",
    "text_uthmani_transcribed": "Wa taḍḥakūna wa lā tabkūn(a)."
  },
  {
    "verse_number": 61,
    "text_uthmani": "وَأَنتُمْ سَـٰمِدُونَ",
    "text_uthmani_transcribed": "Wa antum sāmidūn(a)."
  },
  {
    "verse_number": 62,
    "text_uthmani": "فَٱسْجُدُوا۟ لِلَّهِ وَٱعْبُدُوا۟ ۩",
    "text_uthmani_transcribed": "Fasjudū lillāhi wa'budū."
  }
]