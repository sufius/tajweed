[
  {
    "verse_number": 1,
    "arabic": " لَآ أُقْسِمُ بِهَـٰذَا ٱلْبَلَدِ",
    "transcription": "Lā uqsimu bihāżal-balad(i)."
  },
  {
    "verse_number": 2,
    "arabic": "وَأَنتَ حِلٌّۢ بِهَـٰذَا ٱلْبَلَدِ",
    "transcription": "Wa anta ḥillum bihāżal-balad(i)."
  },
  {
    "verse_number": 3,
    "arabic": "وَوَالِدٍ وَمَا وَلَدَ",
    "transcription": "Wa wālidiw wa mā walad(a)."
  },
  {
    "verse_number": 4,
    "arabic": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِى كَبَدٍ",
    "transcription": "Laqad khalaqnal-insāna fī kabad(in)."
  },
  {
    "verse_number": 5,
    "arabic": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ",
    "transcription": "Ayaḥsabu allay yaqdira 'alaihi  aḥad(un)."
  },
  {
    "verse_number": 6,
    "arabic": "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا",
    "transcription": "Yaqūlu ahlaktu mālal lubadā(n)."
  },
  {
    "verse_number": 7,
    "arabic": "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ",
    "transcription": "Ayaḥsabu allam yarahū aḥad(un)."
  },
  {
    "verse_number": 8,
    "arabic": "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ",
    "transcription": "Alam naj'al lahū 'ainain(i)."
  },
  {
    "verse_number": 9,
    "arabic": "وَلِسَانًا وَشَفَتَيْنِ",
    "transcription": "Wa lisānaw wa syafatain(i)."
  },
  {
    "verse_number": 10,
    "arabic": "وَهَدَيْنَـٰهُ ٱلنَّجْدَيْنِ",
    "transcription": "Wa hadaināhun-najdain(i)."
  },
  {
    "verse_number": 11,
    "arabic": "فَلَا ٱقْتَحَمَ ٱلْعَقَبَةَ",
    "transcription": "Falaqtaḥamal-'aqabah(ta)."
  },
  {
    "verse_number": 12,
    "arabic": "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ",
    "transcription": "Wa mā adrāka mal-'aqabah(tu)."
  },
  {
    "verse_number": 13,
    "arabic": "فَكُّ رَقَبَةٍ",
    "transcription": "Fakku raqabah(tin)."
  },
  {
    "verse_number": 14,
    "arabic": "أَوْ إِطْعَـٰمٌ فِى يَوْمٍ ذِى مَسْغَبَةٍ",
    "transcription": "Au iṭ'āmun fī yaumin żī masgabah(tin)."
  },
  {
    "verse_number": 15,
    "arabic": "يَتِيمًا ذَا مَقْرَبَةٍ",
    "transcription": "Yatīman żā maqrabah(tin)."
  },
  {
    "verse_number": 16,
    "arabic": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ",
    "transcription": "Au miskīnan żā matrabah(tin)."
  },
  {
    "verse_number": 17,
    "arabic": "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُوا۟ وَتَوَاصَوْا۟ بِٱلصَّبْرِ وَتَوَاصَوْا۟ بِٱلْمَرْحَمَةِ",
    "transcription": "Ṡumma kāna minal-lażīna āmanū wa tawāṣau biṣ-ṣabri wa tawāṣau bil-marḥamah(ti)."
  },
  {
    "verse_number": 18,
    "arabic": "أُو۟لَـٰٓئِكَ أَصْحَـٰبُ ٱلْمَيْمَنَةِ",
    "transcription": "Ulā'ika aṣḥābul-maimanah(ti)."
  },
  {
    "verse_number": 19,
    "arabic": "وَٱلَّذِينَ كَفَرُوا۟ بِـَٔايَـٰتِنَا هُمْ أَصْحَـٰبُ ٱلْمَشْـَٔمَةِ",
    "transcription": "Wal-lażīna kafarū bi'āyātinā hum aṣḥābul-masy'amah(ti)."
  },
  {
    "verse_number": 20,
    "arabic": "عَلَيْهِمْ نَارٌ مُّؤْصَدَةٌۢ",
    "transcription": "'Alaihim nārum mu'ṣadah(tun)."
  }
]