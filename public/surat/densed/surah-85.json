[
  {
    "verse_number": 1,
    "text_uthmani": " وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ",
    "text_uthmani_transcribed": "Was-samā'i żātil-burūj(i)."
  },
  {
    "verse_number": 2,
    "text_uthmani": "وَٱلْيَوْمِ ٱلْمَوْعُودِ",
    "text_uthmani_transcribed": "Wal-yaumil-mau'ūd(i)."
  },
  {
    "verse_number": 3,
    "text_uthmani": "وَشَاهِدٍ وَمَشْهُودٍ",
    "text_uthmani_transcribed": "Wa syāhidiw wa masyhūd(in)."
  },
  {
    "verse_number": 4,
    "text_uthmani": "قُتِلَ أَصْحَـٰبُ ٱلْأُخْدُودِ",
    "text_uthmani_transcribed": "Qutila aṣḥābul-ukhdūd(i)."
  },
  {
    "verse_number": 5,
    "text_uthmani": "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ",
    "text_uthmani_transcribed": "An-nāri żātil-waqūd(i)."
  },
  {
    "verse_number": 6,
    "text_uthmani": "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
    "text_uthmani_transcribed": "Iż hum 'alaihā qu'ūd(un)."
  },
  {
    "verse_number": 7,
    "text_uthmani": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌ",
    "text_uthmani_transcribed": "Wa hum 'alā mā yaf'alūna bil-mu'minīna syuhūd(un)."
  },
  {
    "verse_number": 8,
    "text_uthmani": "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ",
    "text_uthmani_transcribed": "Wa mā naqamū minhum illā ay yu'minū billāhil-'azīzil-ḥamīd(i)."
  },
  {
    "verse_number": 9,
    "text_uthmani": "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ شَهِيدٌ",
    "text_uthmani_transcribed": "Allażī lahū mulkus-samāwāti wal-arḍ(i), wallāhu 'alā kulli syai'in syahīd(un)."
  },
  {
    "verse_number": 10,
    "text_uthmani": "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَـٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ",
    "text_uthmani_transcribed": "Innal-lażīna fatanul-mu'minīna wal-mu'mināti ṡumma lam yatūbū fa lahum 'ażābu jahannama wa lahum 'ażābul-ḥarīq(i)."
  },
  {
    "verse_number": 11,
    "text_uthmani": "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ لَهُمْ جَنَّـٰتٌ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَـٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ",
    "text_uthmani_transcribed": "Innal-lażīna āmanū wa 'amiluṣ-ṣāliḥāti lahum jannātun tajrī min taḥtihal-anhār(u), żālikal-fauzul-kabīr(u)."
  },
  {
    "verse_number": 12,
    "text_uthmani": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
    "text_uthmani_transcribed": "Inna baṭsya rabbika lasyadīd(un)."
  },
  {
    "verse_number": 13,
    "text_uthmani": "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ",
    "text_uthmani_transcribed": "Innahū huwa yubdi'u wa yu'īd(u)."
  },
  {
    "verse_number": 14,
    "text_uthmani": "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ",
    "text_uthmani_transcribed": "Wa huwal-gafūrul-wadūd(u)."
  },
  {
    "verse_number": 15,
    "text_uthmani": "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ",
    "text_uthmani_transcribed": "Żul-'arsyil-majīd(i)."
  },
  {
    "verse_number": 16,
    "text_uthmani": "فَعَّالٌ لِّمَا يُرِيدُ",
    "text_uthmani_transcribed": "Fa''ālul limā yurīd(u)."
  },
  {
    "verse_number": 17,
    "text_uthmani": "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ",
    "text_uthmani_transcribed": "Hal atāka ḥadīṡul-junūd(i)."
  },
  {
    "verse_number": 18,
    "text_uthmani": "فِرْعَوْنَ وَثَمُودَ",
    "text_uthmani_transcribed": "Fir'auna wa ṡamūd(a)."
  },
  {
    "verse_number": 19,
    "text_uthmani": "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى تَكْذِيبٍ",
    "text_uthmani_transcribed": "Balil-lażīna kafarū fī takżīb(in)."
  },
  {
    "verse_number": 20,
    "text_uthmani": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطٌۢ",
    "text_uthmani_transcribed": "Wallāhu miw warā'ihim muḥīṭ(un)."
  },
  {
    "verse_number": 21,
    "text_uthmani": "بَلْ هُوَ قُرْءَانٌ مَّجِيدٌ",
    "text_uthmani_transcribed": "Bal huwa qur'ānum majīd(un)."
  },
  {
    "verse_number": 22,
    "text_uthmani": "فِى لَوْحٍ مَّحْفُوظٍۭ",
    "text_uthmani_transcribed": "Fī lauḥim maḥfūẓ(in)."
  }
]