[
  {
    "verse_number": 1,
    "arabic": "وَٱلتِّينِ وَٱلزَّيْتُونِ",
    "transcription": "Wat-tīni waz-zaitūn(i)."
  },
  {
    "verse_number": 2,
    "arabic": "وَطُورِ سِينِينَ",
    "transcription": "Wa ṭūri sīnīn(a)."
  },
  {
    "verse_number": 3,
    "arabic": "وَهَـٰذَا ٱلْبَلَدِ ٱلْأَمِينِ",
    "transcription": "Wa hāżal-baladil-amīn(i)."
  },
  {
    "verse_number": 4,
    "arabic": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِىٓ أَحْسَنِ تَقْوِيمٍ",
    "transcription": "Laqad khalaqnal-insāna fī aḥsani taqwīm(in)."
  },
  {
    "verse_number": 5,
    "arabic": "ثُمَّ رَدَدْنَـٰهُ أَسْفَلَ سَـٰفِلِينَ",
    "transcription": "Ṡumma radadnāhu asfala sāfilīn(a)."
  },
  {
    "verse_number": 6,
    "arabic": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ",
    "transcription": "Illal-lażīna āmanū wa 'amiluṣ-ṣāliḥāti falahum ajrun gairu mamnūn(in)."
  },
  {
    "verse_number": 7,
    "arabic": "فَمَا يُكَذِّبُكَ بَعْدُ بِٱلدِّينِ",
    "transcription": "Famā yukażżibuka ba'du bid-dīn(i)."
  },
  {
    "verse_number": 8,
    "arabic": "أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَـٰكِمِينَ",
    "transcription": "Alaisallāhu bi'aḥkamil-ḥākimīn(a)."
  }
]