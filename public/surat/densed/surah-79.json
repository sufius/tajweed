[
  {
    "verse_number": 1,
    "arabic": " وَٱلنَّـٰزِعَـٰتِ غَرْقًا",
    "transcription": "Wan-nāzi'āti garqā(n)."
  },
  {
    "verse_number": 2,
    "arabic": "وَٱلنَّـٰشِطَـٰتِ نَشْطًا",
    "transcription": "Wan-nāsyiṭāti nasyṭā(n)."
  },
  {
    "verse_number": 3,
    "arabic": "وَٱلسَّـٰبِحَـٰتِ سَبْحًا",
    "transcription": "Was-sābiḥāti sabḥā(n)."
  },
  {
    "verse_number": 4,
    "arabic": "فَٱلسَّـٰبِقَـٰتِ سَبْقًا",
    "transcription": "Fas-sābiqāti sabqā(n)."
  },
  {
    "verse_number": 5,
    "arabic": "فَٱلْمُدَبِّرَٰتِ أَمْرًا",
    "transcription": "Fal-mudabbirāti amrā(n)."
  },
  {
    "verse_number": 6,
    "arabic": "يَوْمَ تَرْجُفُ ٱلرَّاجِفَةُ",
    "transcription": "Yauma tarjufur-rājifah(tu)."
  },
  {
    "verse_number": 7,
    "arabic": "تَتْبَعُهَا ٱلرَّادِفَةُ",
    "transcription": "Tatba'uhar-rādifah(tu)."
  },
  {
    "verse_number": 8,
    "arabic": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
    "transcription": "Qulūbuy yauma'iżiw wājifah(tun)."
  },
  {
    "verse_number": 9,
    "arabic": "أَبْصَـٰرُهَا خَـٰشِعَةٌ",
    "transcription": "Abṣāruhā khāsyi'ah(tun)."
  },
  {
    "verse_number": 10,
    "arabic": "يَقُولُونَ أَءِنَّا لَمَرْدُودُونَ فِى ٱلْحَافِرَةِ",
    "transcription": "Yaqūlūna a'innā lamardūdūna fil-ḥāfirah(ti)."
  },
  {
    "verse_number": 11,
    "arabic": "أَءِذَا كُنَّا عِظَـٰمًا نَّخِرَةً",
    "transcription": "A'iżā kunnā 'iẓāman nakhirah(tan)."
  },
  {
    "verse_number": 12,
    "arabic": "قَالُوا۟ تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
    "transcription": "Qālū tilka iżan karratun khāsirah(tun)."
  },
  {
    "verse_number": 13,
    "arabic": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ",
    "transcription": "Fa innamā hiya zajratuw wāḥidah(tun)."
  },
  {
    "verse_number": 14,
    "arabic": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
    "transcription": "Fa iżā hum bis-sāhirah(ti)."
  },
  {
    "verse_number": 15,
    "arabic": "هَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
    "transcription": "Hal atāka ḥadīṡu mūsā."
  },
  {
    "verse_number": 16,
    "arabic": "إِذْ نَادَىٰهُ رَبُّهُۥ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى",
    "transcription": "Iż nādāhu rabbuhū bil-wādil-muqaddasi ṭuwā(n)."
  },
  {
    "verse_number": 17,
    "arabic": "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ",
    "transcription": "Iżhab ilā fir'auna innahū ṭagā."
  },
  {
    "verse_number": 18,
    "arabic": "فَقُلْ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
    "transcription": "Fa qul hal laka ilā an tazakkā."
  },
  {
    "verse_number": 19,
    "arabic": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
    "transcription": "Wa ahdiyaka ilā rabbika fa takhsyā."
  },
  {
    "verse_number": 20,
    "arabic": "فَأَرَىٰهُ ٱلْـَٔايَةَ ٱلْكُبْرَىٰ",
    "transcription": "Fa arāhul-āyatal-kubrā."
  },
  {
    "verse_number": 21,
    "arabic": "فَكَذَّبَ وَعَصَىٰ",
    "transcription": "Fa każżaba wa 'aṣā."
  },
  {
    "verse_number": 22,
    "arabic": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
    "transcription": "Ṡumma adbara yas'ā."
  },
  {
    "verse_number": 23,
    "arabic": "فَحَشَرَ فَنَادَىٰ",
    "transcription": "Fa ḥasyara fanādā."
  },
  {
    "verse_number": 24,
    "arabic": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلْأَعْلَىٰ",
    "transcription": "Fa qāla ana rabbukumul-a'lā."
  },
  {
    "verse_number": 25,
    "arabic": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلْـَٔاخِرَةِ وَٱلْأُولَىٰٓ",
    "transcription": "Fa akhażahullāhu nakālal-ākhirati wal-ūlā."
  },
  {
    "verse_number": 26,
    "arabic": "إِنَّ فِى ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰٓ",
    "transcription": "Inna fī żālika la'ibratal limay yakhsyā."
  },
  {
    "verse_number": 27,
    "arabic": "ءَأَنتُمْ أَشَدُّ خَلْقًا أَمِ ٱلسَّمَآءُ ۚ بَنَىٰهَا",
    "transcription": "A'antum asyaddu khalqan amis-samā'u banāhā."
  },
  {
    "verse_number": 28,
    "arabic": "رَفَعَ سَمْكَهَا فَسَوَّىٰهَا",
    "transcription": "Rafa'a samkahā fa sawwāhā."
  },
  {
    "verse_number": 29,
    "arabic": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَىٰهَا",
    "transcription": "Wa agṭasya lailahā wa akhraja ḍuḥāhā."
  },
  {
    "verse_number": 30,
    "arabic": "وَٱلْأَرْضَ بَعْدَ ذَٰلِكَ دَحَىٰهَآ",
    "transcription": "Wal-arḍa ba'da żālika daḥāhā."
  },
  {
    "verse_number": 31,
    "arabic": "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَىٰهَا",
    "transcription": "Akhraja minhā mā'ahā wa mar'āhā."
  },
  {
    "verse_number": 32,
    "arabic": "وَٱلْجِبَالَ أَرْسَىٰهَا",
    "transcription": "Wal-jibāla arsāhā."
  },
  {
    "verse_number": 33,
    "arabic": "مَتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
    "transcription": "Matā'al lakum wa li'an'āmikum."
  },
  {
    "verse_number": 34,
    "arabic": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلْكُبْرَىٰ",
    "transcription": "Fa iżā jā'atiṭ-ṭāmmatul-kubrā."
  },
  {
    "verse_number": 35,
    "arabic": "يَوْمَ يَتَذَكَّرُ ٱلْإِنسَـٰنُ مَا سَعَىٰ",
    "transcription": "Yauma yatażakkarul-insānu mā sa'ā."
  },
  {
    "verse_number": 36,
    "arabic": "وَبُرِّزَتِ ٱلْجَحِيمُ لِمَن يَرَىٰ",
    "transcription": "Wa burrizatil-jaḥīmu limay yarā."
  },
  {
    "verse_number": 37,
    "arabic": "فَأَمَّا مَن طَغَىٰ",
    "transcription": "Fa ammā man ṭagā."
  },
  {
    "verse_number": 38,
    "arabic": "وَءَاثَرَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
    "transcription": "Wa āṡaral-ḥayātad-dun-yā."
  },
  {
    "verse_number": 39,
    "arabic": "فَإِنَّ ٱلْجَحِيمَ هِىَ ٱلْمَأْوَىٰ",
    "transcription": "Fa innal-jaḥīma hiyal-ma'wā."
  },
  {
    "verse_number": 40,
    "arabic": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفْسَ عَنِ ٱلْهَوَىٰ",
    "transcription": "Wa ammā man khāfa maqāma rabbihī wa nahan-nafsa 'anil-hawā."
  },
  {
    "verse_number": 41,
    "arabic": "فَإِنَّ ٱلْجَنَّةَ هِىَ ٱلْمَأْوَىٰ",
    "transcription": "Fa innal-jannata hiyal-ma'wā."
  },
  {
    "verse_number": 42,
    "arabic": "يَسْـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرْسَىٰهَا",
    "transcription": "Yas'alūnaka 'anis-sā'ati ayyāna mursāhā."
  },
  {
    "verse_number": 43,
    "arabic": "فِيمَ أَنتَ مِن ذِكْرَىٰهَآ",
    "transcription": "Fīma anta min żikrāhā."
  },
  {
    "verse_number": 44,
    "arabic": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
    "transcription": "Ilā rabbika muntahāhā."
  },
  {
    "verse_number": 45,
    "arabic": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخْشَىٰهَا",
    "transcription": "Innamā anta munżiru may yakhsyāhā."
  },
  {
    "verse_number": 46,
    "arabic": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوٓا۟ إِلَّا عَشِيَّةً أَوْ ضُحَىٰهَا",
    "transcription": "Ka'annahum yauma yaraunahā lam yalbaṡū illā 'asyiyyatan au ḍuḥāhā."
  }
]