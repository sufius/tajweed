[
  {
    "verse_number": 1,
    "arabic": " وَٱلصَّـٰٓفَّـٰتِ صَفًّا",
    "transcription": "Waṣ-ṣāffāti ṣaffā(n)."
  },
  {
    "verse_number": 2,
    "arabic": "فَٱلزَّٰجِرَٰتِ زَجْرًا",
    "transcription": "Faz-zājirāti zajrā(n)."
  },
  {
    "verse_number": 3,
    "arabic": "فَٱلتَّـٰلِيَـٰتِ ذِكْرًا",
    "transcription": "Fat-tāliyāti żikrā(n)."
  },
  {
    "verse_number": 4,
    "arabic": "إِنَّ إِلَـٰهَكُمْ لَوَٰحِدٌ",
    "transcription": "Inna ilāhakum lawāḥid(un)."
  },
  {
    "verse_number": 5,
    "arabic": "رَّبُّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ ٱلْمَشَـٰرِقِ",
    "transcription": "Rabbus-samāwāti wal-arḍi wa mā bainahumā wa rabbul-masyāriq(i)."
  },
  {
    "verse_number": 6,
    "arabic": "إِنَّا زَيَّنَّا ٱلسَّمَآءَ ٱلدُّنْيَا بِزِينَةٍ ٱلْكَوَاكِبِ",
    "transcription": "Innā zayyannas-samā'ad-dun-yā bizīnatinil-kawākib(i)."
  },
  {
    "verse_number": 7,
    "arabic": "وَحِفْظًا مِّن كُلِّ شَيْطَـٰنٍ مَّارِدٍ",
    "transcription": "Wa ḥifẓam min kulli syaiṭānim mārid(in)."
  },
  {
    "verse_number": 8,
    "arabic": "لَّا يَسَّمَّعُونَ إِلَى ٱلْمَلَإِ ٱلْأَعْلَىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٍ",
    "transcription": "Lā yassammā'ūna ilal-mala'il-a'lā wa yuqżafūna min kulli jānib(in)."
  },
  {
    "verse_number": 9,
    "arabic": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ",
    "transcription": "Duḥūraw wa lahum 'ażābuw wāṣib(un)."
  },
  {
    "verse_number": 10,
    "arabic": "إِلَّا مَنْ خَطِفَ ٱلْخَطْفَةَ فَأَتْبَعَهُۥ شِهَابٌ ثَاقِبٌ",
    "transcription": "Illā man khaṭifal-khaṭfata fa'atba'ahū syihābun ṡāqib(un)."
  },
  {
    "verse_number": 11,
    "arabic": "فَٱسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَم مَّنْ خَلَقْنَآ ۚ إِنَّا خَلَقْنَـٰهُم مِّن طِينٍ لَّازِبٍۭ",
    "transcription": "Fastaftihim ahum asyaddu khalqan am man khalaqnā, innā khalaqnāhum min ṭīnil lāzib(in)."
  },
  {
    "verse_number": 12,
    "arabic": "بَلْ عَجِبْتَ وَيَسْخَرُونَ",
    "transcription": "Bal 'ajibta wa yaskharūn(a)."
  },
  {
    "verse_number": 13,
    "arabic": "وَإِذَا ذُكِّرُوا۟ لَا يَذْكُرُونَ",
    "transcription": "Wa iżā żukkirū lā yażkurūn(a)."
  },
  {
    "verse_number": 14,
    "arabic": "وَإِذَا رَأَوْا۟ ءَايَةً يَسْتَسْخِرُونَ",
    "transcription": "Wa iżā ra'au āyatay yastaskhirūn(a)."
  },
  {
    "verse_number": 15,
    "arabic": "وَقَالُوٓا۟ إِنْ هَـٰذَآ إِلَّا سِحْرٌ مُّبِينٌ",
    "transcription": "Wa qālū in hāżā illā siḥrum mubīn(un)."
  },
  {
    "verse_number": 16,
    "arabic": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَبْعُوثُونَ",
    "transcription": "A'iżā mitnā wa kunnā turābaw wa 'iẓāman a'innā lamab'ūṡūn(a)."
  },
  {
    "verse_number": 17,
    "arabic": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ",
    "transcription": "Awa ābā'unal-awwalūn(a)."
  },
  {
    "verse_number": 18,
    "arabic": "قُلْ نَعَمْ وَأَنتُمْ دَٰخِرُونَ",
    "transcription": "Qul na'am wa antum dākhirūn(a)."
  },
  {
    "verse_number": 19,
    "arabic": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ فَإِذَا هُمْ يَنظُرُونَ",
    "transcription": "Fa'innamā hiya zajratuw wāḥidatun fa'iżā hum yanẓurūn(a)."
  },
  {
    "verse_number": 20,
    "arabic": "وَقَالُوا۟ يَـٰوَيْلَنَا هَـٰذَا يَوْمُ ٱلدِّينِ",
    "transcription": "Wa qālū yā wailanā hāżā yaumud-dīn(i)."
  },
  {
    "verse_number": 21,
    "arabic": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ",
    "transcription": "Hāżā yaumul-faṣlil-lażī kuntum bihī tukażżibūn(a)."
  },
  {
    "verse_number": 22,
    "arabic": "۞ ٱحْشُرُوا۟ ٱلَّذِينَ ظَلَمُوا۟ وَأَزْوَٰجَهُمْ وَمَا كَانُوا۟ يَعْبُدُونَ",
    "transcription": "Uḥsyurul-lażīna ẓalamū wa azwājahum wa mā kānū ya'budūn(a)."
  },
  {
    "verse_number": 23,
    "arabic": "مِن دُونِ ٱللَّهِ فَٱهْدُوهُمْ إِلَىٰ صِرَٰطِ ٱلْجَحِيمِ",
    "transcription": "Min dūnillāhi fahdūhum ilā ṣirāṭil-jaḥīm(i)."
  },
  {
    "verse_number": 24,
    "arabic": "وَقِفُوهُمْ ۖ إِنَّهُم مَّسْـُٔولُونَ",
    "transcription": "Waqifūhum innahum mas'ūlūn(a)."
  },
  {
    "verse_number": 25,
    "arabic": "مَا لَكُمْ لَا تَنَاصَرُونَ",
    "transcription": "Mā lakum lā tanāṣarūn(a)."
  },
  {
    "verse_number": 26,
    "arabic": "بَلْ هُمُ ٱلْيَوْمَ مُسْتَسْلِمُونَ",
    "transcription": "Bal humul-yauma mustaslimūn(a)."
  },
  {
    "verse_number": 27,
    "arabic": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ",
    "transcription": "Wa aqbala ba'ḍuhum 'alā ba'ḍiy yatasā'alūn(a)."
  },
  {
    "verse_number": 28,
    "arabic": "قَالُوٓا۟ إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ ٱلْيَمِينِ",
    "transcription": "Qālū innakum kuntum ta'tūnanā 'anil-yamīn(i)."
  },
  {
    "verse_number": 29,
    "arabic": "قَالُوا۟ بَل لَّمْ تَكُونُوا۟ مُؤْمِنِينَ",
    "transcription": "Qālū bal lam takūnū mu'minīn(a)."
  },
  {
    "verse_number": 30,
    "arabic": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَـٰنٍۭ ۖ بَلْ كُنتُمْ قَوْمًا طَـٰغِينَ",
    "transcription": "Wa mā kāna lanā 'alaikum min sulṭān(in), bal kuntum qauman ṭāgīn(a)."
  },
  {
    "verse_number": 31,
    "arabic": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَآ ۖ إِنَّا لَذَآئِقُونَ",
    "transcription": "Fa ḥaqqa 'alainā qaulu rabbinā, innā lażā'iqūn(a)."
  },
  {
    "verse_number": 32,
    "arabic": "فَأَغْوَيْنَـٰكُمْ إِنَّا كُنَّا غَـٰوِينَ",
    "transcription": "Fa agwainākum innā kunnā gāwīn(a)."
  },
  {
    "verse_number": 33,
    "arabic": "فَإِنَّهُمْ يَوْمَئِذٍ فِى ٱلْعَذَابِ مُشْتَرِكُونَ",
    "transcription": "Fa innahum yauma'iżin fil-'ażābi musytarikūn(a)."
  },
  {
    "verse_number": 34,
    "arabic": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
    "transcription": "Innā każālika naf'alu bil-mujrimīn(a)."
  },
  {
    "verse_number": 35,
    "arabic": "إِنَّهُمْ كَانُوٓا۟ إِذَا قِيلَ لَهُمْ لَآ إِلَـٰهَ إِلَّا ٱللَّهُ يَسْتَكْبِرُونَ",
    "transcription": "Innahum kānū iżā qīla lahum lā ilāha illallāhu yastakbirūn(a)."
  },
  {
    "verse_number": 36,
    "arabic": "وَيَقُولُونَ أَئِنَّا لَتَارِكُوٓا۟ ءَالِهَتِنَا لِشَاعِرٍ مَّجْنُونٍۭ",
    "transcription": "Wa yaqūlūna a'innā latārikū ālihatinā lisyā'irim majnūn(in)."
  },
  {
    "verse_number": 37,
    "arabic": "بَلْ جَآءَ بِٱلْحَقِّ وَصَدَّقَ ٱلْمُرْسَلِينَ",
    "transcription": "Bal jā'a bil-ḥaqqi wa ṣaddaqal-mursalīn(a)."
  },
  {
    "verse_number": 38,
    "arabic": "إِنَّكُمْ لَذَآئِقُوا۟ ٱلْعَذَابِ ٱلْأَلِيمِ",
    "transcription": "Innakum lażā'iqul-'ażābil-alīm(i)."
  },
  {
    "verse_number": 39,
    "arabic": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ",
    "transcription": "Wa mā tujzauna illā mā kuntum ta'malūn(a)."
  },
  {
    "verse_number": 40,
    "arabic": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "transcription": "Illā 'ibādallāhil-mukhlaṣīn(a)."
  },
  {
    "verse_number": 41,
    "arabic": "أُو۟لَـٰٓئِكَ لَهُمْ رِزْقٌ مَّعْلُومٌ",
    "transcription": "Ulā'ika lahum rizqum ma'lūm(un)."
  },
  {
    "verse_number": 42,
    "arabic": "فَوَٰكِهُ ۖ وَهُم مُّكْرَمُونَ",
    "transcription": "Fawākihu wa hum mukramūn(a)."
  },
  {
    "verse_number": 43,
    "arabic": "فِى جَنَّـٰتِ ٱلنَّعِيمِ",
    "transcription": "Fī jannātin na'īm(i)."
  },
  {
    "verse_number": 44,
    "arabic": "عَلَىٰ سُرُرٍ مُّتَقَـٰبِلِينَ",
    "transcription": "'Alā sururim mutaqābilīn(a)."
  },
  {
    "verse_number": 45,
    "arabic": "يُطَافُ عَلَيْهِم بِكَأْسٍ مِّن مَّعِينٍۭ",
    "transcription": "Yuṭāfu 'alaihim bika'sim mim ma'īn(in)."
  },
  {
    "verse_number": 46,
    "arabic": "بَيْضَآءَ لَذَّةٍ لِّلشَّـٰرِبِينَ",
    "transcription": "Baiḍā'a lażżatil lisy-syāribīn(a)."
  },
  {
    "verse_number": 47,
    "arabic": "لَا فِيهَا غَوْلٌ وَلَا هُمْ عَنْهَا يُنزَفُونَ",
    "transcription": "Lā fīhā gauluw wa lā hum 'anhā yunzafūn(a)."
  },
  {
    "verse_number": 48,
    "arabic": "وَعِندَهُمْ قَـٰصِرَٰتُ ٱلطَّرْفِ عِينٌ",
    "transcription": "Wa 'indahum qāṣirātuṭ-ṭarfi 'īn(un)."
  },
  {
    "verse_number": 49,
    "arabic": "كَأَنَّهُنَّ بَيْضٌ مَّكْنُونٌ",
    "transcription": "Ka'annahum baiḍum maknūn(un)."
  },
  {
    "verse_number": 50,
    "arabic": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَآءَلُونَ",
    "transcription": "Fa'aqbala ba'ḍuhum 'alā ba'ḍiy yatasā'alūn(a)."
  },
  {
    "verse_number": 51,
    "arabic": "قَالَ قَآئِلٌ مِّنْهُمْ إِنِّى كَانَ لِى قَرِينٌ",
    "transcription": "Qāla qā'ilum minhum innī kāna lī qarīn(un)."
  },
  {
    "verse_number": 52,
    "arabic": "يَقُولُ أَءِنَّكَ لَمِنَ ٱلْمُصَدِّقِينَ",
    "transcription": "Yaqūlu a'innaka laminal-muṣaddiqīn(a)."
  },
  {
    "verse_number": 53,
    "arabic": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَدِينُونَ",
    "transcription": "A'iżā mitnā wa kunnā turāban a'innā lamadīnūn(a)."
  },
  {
    "verse_number": 54,
    "arabic": "قَالَ هَلْ أَنتُم مُّطَّلِعُونَ",
    "transcription": "Qāla hal antum muṭṭali'ūn(a)."
  },
  {
    "verse_number": 55,
    "arabic": "فَٱطَّلَعَ فَرَءَاهُ فِى سَوَآءِ ٱلْجَحِيمِ",
    "transcription": "Faṭṭala'a fa ra'āhu fī sawā'il-jaḥīm(i)."
  },
  {
    "verse_number": 56,
    "arabic": "قَالَ تَٱللَّهِ إِن كِدتَّ لَتُرْدِينِ",
    "transcription": "Qāla tallāhi in kitta laturdīn(i)."
  },
  {
    "verse_number": 57,
    "arabic": "وَلَوْلَا نِعْمَةُ رَبِّى لَكُنتُ مِنَ ٱلْمُحْضَرِينَ",
    "transcription": "Wa lau lā ni'matu rabbī lakuntu minal-muḥḍarīn(a)."
  },
  {
    "verse_number": 58,
    "arabic": "أَفَمَا نَحْنُ بِمَيِّتِينَ",
    "transcription": "Afamā naḥnu bimayyitīn(a)."
  },
  {
    "verse_number": 59,
    "arabic": "إِلَّا مَوْتَتَنَا ٱلْأُولَىٰ وَمَا نَحْنُ بِمُعَذَّبِينَ",
    "transcription": "Illā mautatunal-ūlā wa mā naḥnu bimu'ażżabīn(a)."
  },
  {
    "verse_number": 60,
    "arabic": "إِنَّ هَـٰذَا لَهُوَ ٱلْفَوْزُ ٱلْعَظِيمُ",
    "transcription": "Inna hāżā lahuwal-fauzul-'aẓīm(u)."
  },
  {
    "verse_number": 61,
    "arabic": "لِمِثْلِ هَـٰذَا فَلْيَعْمَلِ ٱلْعَـٰمِلُونَ",
    "transcription": "Limiṡli hāżā falya'malil-'āmilūn(a)."
  },
  {
    "verse_number": 62,
    "arabic": "أَذَٰلِكَ خَيْرٌ نُّزُلًا أَمْ شَجَرَةُ ٱلزَّقُّومِ",
    "transcription": "Ażālika khairun nuzulan am syajaratuz-zaqqūm(i)."
  },
  {
    "verse_number": 63,
    "arabic": "إِنَّا جَعَلْنَـٰهَا فِتْنَةً لِّلظَّـٰلِمِينَ",
    "transcription": "Innā ja'alnāhā fitnatal liẓ-ẓālimīn(a)."
  },
  {
    "verse_number": 64,
    "arabic": "إِنَّهَا شَجَرَةٌ تَخْرُجُ فِىٓ أَصْلِ ٱلْجَحِيمِ",
    "transcription": "Innahā syajaratun takhruju fī aṣlil-jaḥīm(i)."
  },
  {
    "verse_number": 65,
    "arabic": "طَلْعُهَا كَأَنَّهُۥ رُءُوسُ ٱلشَّيَـٰطِينِ",
    "transcription": "Ṭal'uhā ka'annahū ru'ūsusy-syayāṭīn(i)."
  },
  {
    "verse_number": 66,
    "arabic": "فَإِنَّهُمْ لَـَٔاكِلُونَ مِنْهَا فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ",
    "transcription": "Fa innahum la'ākilūna minhā fa māli'ūna minhal-buṭūn(a)."
  },
  {
    "verse_number": 67,
    "arabic": "ثُمَّ إِنَّ لَهُمْ عَلَيْهَا لَشَوْبًا مِّنْ حَمِيمٍ",
    "transcription": "Ṡumma inna lahum 'alaihā lasyaubam min ḥamīm(in)."
  },
  {
    "verse_number": 68,
    "arabic": "ثُمَّ إِنَّ مَرْجِعَهُمْ لَإِلَى ٱلْجَحِيمِ",
    "transcription": "Ṡumma inna marji'ahum la'ilal-jaḥīm(i)."
  },
  {
    "verse_number": 69,
    "arabic": "إِنَّهُمْ أَلْفَوْا۟ ءَابَآءَهُمْ ضَآلِّينَ",
    "transcription": "Innahum alfau ābā'ahum ḍāllīn(a)."
  },
  {
    "verse_number": 70,
    "arabic": "فَهُمْ عَلَىٰٓ ءَاثَـٰرِهِمْ يُهْرَعُونَ",
    "transcription": "Fahum 'alā āṡārihim yuhra'ūn(a)."
  },
  {
    "verse_number": 71,
    "arabic": "وَلَقَدْ ضَلَّ قَبْلَهُمْ أَكْثَرُ ٱلْأَوَّلِينَ",
    "transcription": "Wa laqad ḍalla qablahum akṡarul-awwalīn(a)."
  },
  {
    "verse_number": 72,
    "arabic": "وَلَقَدْ أَرْسَلْنَا فِيهِم مُّنذِرِينَ",
    "transcription": "Wa laqad arsalnā fīhim munżirīn(a)."
  },
  {
    "verse_number": 73,
    "arabic": "فَٱنظُرْ كَيْفَ كَانَ عَـٰقِبَةُ ٱلْمُنذَرِينَ",
    "transcription": "Fanẓur kaifa kāna 'āqibatul-munżarīn(a)."
  },
  {
    "verse_number": 74,
    "arabic": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "transcription": "Illā 'ibādallāhil-mukhlaṣīn(a)."
  },
  {
    "verse_number": 75,
    "arabic": "وَلَقَدْ نَادَىٰنَا نُوحٌ فَلَنِعْمَ ٱلْمُجِيبُونَ",
    "transcription": "Wa laqad nādānā nūḥun falani'mal-mujībūn(a)."
  },
  {
    "verse_number": 76,
    "arabic": "وَنَجَّيْنَـٰهُ وَأَهْلَهُۥ مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ",
    "transcription": "Wa najjaināhu wa ahlahū minal-karbil-'aẓīm(i)."
  },
  {
    "verse_number": 77,
    "arabic": "وَجَعَلْنَا ذُرِّيَّتَهُۥ هُمُ ٱلْبَاقِينَ",
    "transcription": "Wa ja'alnā żurriyyatahū humul-bāqīn(a)."
  },
  {
    "verse_number": 78,
    "arabic": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ",
    "transcription": "Wa taraknā 'alaihi fil-ākhirīn(a)."
  },
  {
    "verse_number": 79,
    "arabic": "سَلَـٰمٌ عَلَىٰ نُوحٍ فِى ٱلْعَـٰلَمِينَ",
    "transcription": "Salāmun 'alā nūḥin fil-'ālamīn(a)."
  },
  {
    "verse_number": 80,
    "arabic": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "transcription": "Innā każālika najzil-muḥsinīn(a)."
  },
  {
    "verse_number": 81,
    "arabic": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
    "transcription": "Innahū min 'ibādinal-mu'minīn(a)."
  },
  {
    "verse_number": 82,
    "arabic": "ثُمَّ أَغْرَقْنَا ٱلْـَٔاخَرِينَ",
    "transcription": "Ṡumma agraqnal-ākharīn(a)."
  },
  {
    "verse_number": 83,
    "arabic": "۞ وَإِنَّ مِن شِيعَتِهِۦ لَإِبْرَٰهِيمَ",
    "transcription": "Wa inna min syī'atihī la'ibrāhīm(a)."
  },
  {
    "verse_number": 84,
    "arabic": "إِذْ جَآءَ رَبَّهُۥ بِقَلْبٍ سَلِيمٍ",
    "transcription": "Iż jā'a rabbahū biqalbin salīm(in)."
  },
  {
    "verse_number": 85,
    "arabic": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَاذَا تَعْبُدُونَ",
    "transcription": "Iż qāla li'abīhi wa qaumihī māżā ta'budūn(a)."
  },
  {
    "verse_number": 86,
    "arabic": "أَئِفْكًا ءَالِهَةً دُونَ ٱللَّهِ تُرِيدُونَ",
    "transcription": "A'ifkan ālihatan dūnallāhi turīdūn(a)."
  },
  {
    "verse_number": 87,
    "arabic": "فَمَا ظَنُّكُم بِرَبِّ ٱلْعَـٰلَمِينَ",
    "transcription": "Famā ẓannukum birabbil-'ālamīn(a)."
  },
  {
    "verse_number": 88,
    "arabic": "فَنَظَرَ نَظْرَةً فِى ٱلنُّجُومِ",
    "transcription": "Fa naẓara naẓratan fin-nujūm(i)."
  },
  {
    "verse_number": 89,
    "arabic": "فَقَالَ إِنِّى سَقِيمٌ",
    "transcription": "Fa qāla innī saqīm(un)."
  },
  {
    "verse_number": 90,
    "arabic": "فَتَوَلَّوْا۟ عَنْهُ مُدْبِرِينَ",
    "transcription": "Fa tawallau 'anhu mudbirīn(a)."
  },
  {
    "verse_number": 91,
    "arabic": "فَرَاغَ إِلَىٰٓ ءَالِهَتِهِمْ فَقَالَ أَلَا تَأْكُلُونَ",
    "transcription": "Farāga ilā ālihatihim faqāla alā ta'kulūn(a)."
  },
  {
    "verse_number": 92,
    "arabic": "مَا لَكُمْ لَا تَنطِقُونَ",
    "transcription": "Mā lakum lā tanṭiqūn(a)."
  },
  {
    "verse_number": 93,
    "arabic": "فَرَاغَ عَلَيْهِمْ ضَرْبًۢا بِٱلْيَمِينِ",
    "transcription": "Farāga 'alaihim ḍarbam bil-yamīn(i)."
  },
  {
    "verse_number": 94,
    "arabic": "فَأَقْبَلُوٓا۟ إِلَيْهِ يَزِفُّونَ",
    "transcription": "Fa aqbalū ilaihi yaziffūn(a)."
  },
  {
    "verse_number": 95,
    "arabic": "قَالَ أَتَعْبُدُونَ مَا تَنْحِتُونَ",
    "transcription": "Qāla ata'budūna mā tanḥitūn(a)."
  },
  {
    "verse_number": 96,
    "arabic": "وَٱللَّهُ خَلَقَكُمْ وَمَا تَعْمَلُونَ",
    "transcription": "Wallāhu khalaqakum wa mā ta'malūn(a)."
  },
  {
    "verse_number": 97,
    "arabic": "قَالُوا۟ ٱبْنُوا۟ لَهُۥ بُنْيَـٰنًا فَأَلْقُوهُ فِى ٱلْجَحِيمِ",
    "transcription": "Qālubnū lahū bun-yānan fa'alqūhu fil-jaḥīm(i)."
  },
  {
    "verse_number": 98,
    "arabic": "فَأَرَادُوا۟ بِهِۦ كَيْدًا فَجَعَلْنَـٰهُمُ ٱلْأَسْفَلِينَ",
    "transcription": "Fa arādū bihī kaidan faja'alnāhumul-asfalīn(a)."
  },
  {
    "verse_number": 99,
    "arabic": "وَقَالَ إِنِّى ذَاهِبٌ إِلَىٰ رَبِّى سَيَهْدِينِ",
    "transcription": "Wa qāla innī żāhibun ilā rabbī sayahdīn(i)."
  },
  {
    "verse_number": 100,
    "arabic": "رَبِّ هَبْ لِى مِنَ ٱلصَّـٰلِحِينَ",
    "transcription": "Rabbi hab lī minaṣ-ṣāliḥīn(a)."
  },
  {
    "verse_number": 101,
    "arabic": "فَبَشَّرْنَـٰهُ بِغُلَـٰمٍ حَلِيمٍ",
    "transcription": "Fa basysyarnāhu bigulāmin ḥalīm(in)."
  },
  {
    "verse_number": 102,
    "arabic": "فَلَمَّا بَلَغَ مَعَهُ ٱلسَّعْىَ قَالَ يَـٰبُنَىَّ إِنِّىٓ أَرَىٰ فِى ٱلْمَنَامِ أَنِّىٓ أَذْبَحُكَ فَٱنظُرْ مَاذَا تَرَىٰ ۚ قَالَ يَـٰٓأَبَتِ ٱفْعَلْ مَا تُؤْمَرُ ۖ سَتَجِدُنِىٓ إِن شَآءَ ٱللَّهُ مِنَ ٱلصَّـٰبِرِينَ",
    "transcription": "Falammā balaga ma'ahus-sa'ya qāla yā bunayya innī arā fil-manāmi annī ażbaḥuka fanẓur māżā tarā, qāla yā abatif'al mā tu'mar(u), satajidunī in syā'allāhu minaṣ-ṣābirīn(a)."
  },
  {
    "verse_number": 103,
    "arabic": "فَلَمَّآ أَسْلَمَا وَتَلَّهُۥ لِلْجَبِينِ",
    "transcription": "Falammā aslamā wa tallahū lil-jabīn(i)."
  },
  {
    "verse_number": 104,
    "arabic": "وَنَـٰدَيْنَـٰهُ أَن يَـٰٓإِبْرَٰهِيمُ",
    "transcription": "Wa nādaināhu ay yā ibrāhīm(u)."
  },
  {
    "verse_number": 105,
    "arabic": "قَدْ صَدَّقْتَ ٱلرُّءْيَآ ۚ إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "transcription": "Qad ṣaddaqtar-ru'yā, innā każālika najzil-muḥsinīn(a)."
  },
  {
    "verse_number": 106,
    "arabic": "إِنَّ هَـٰذَا لَهُوَ ٱلْبَلَـٰٓؤُا۟ ٱلْمُبِينُ",
    "transcription": "Inna hāżā lahuwal-balā'ul-mubīn(u)."
  },
  {
    "verse_number": 107,
    "arabic": "وَفَدَيْنَـٰهُ بِذِبْحٍ عَظِيمٍ",
    "transcription": "Wa fadaināhu biżibḥin 'aẓīm(in)."
  },
  {
    "verse_number": 108,
    "arabic": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ",
    "transcription": "Wa taraknā 'alaihi fil-ākhirīn(a)."
  },
  {
    "verse_number": 109,
    "arabic": "سَلَـٰمٌ عَلَىٰٓ إِبْرَٰهِيمَ",
    "transcription": "Salāmun 'alā ibrāhīm(a)."
  },
  {
    "verse_number": 110,
    "arabic": "كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "transcription": "Każālika najzil-muḥsinīn(a)."
  },
  {
    "verse_number": 111,
    "arabic": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
    "transcription": "Innahū min 'ibādinal-mu'minīn(a)."
  },
  {
    "verse_number": 112,
    "arabic": "وَبَشَّرْنَـٰهُ بِإِسْحَـٰقَ نَبِيًّا مِّنَ ٱلصَّـٰلِحِينَ",
    "transcription": "Wa basysyarnāhu bi'isḥāqa nabiyyam minaṣ-ṣāliḥīn(a)."
  },
  {
    "verse_number": 113,
    "arabic": "وَبَـٰرَكْنَا عَلَيْهِ وَعَلَىٰٓ إِسْحَـٰقَ ۚ وَمِن ذُرِّيَّتِهِمَا مُحْسِنٌ وَظَالِمٌ لِّنَفْسِهِۦ مُبِينٌ",
    "transcription": "Wa bāraknā 'alaihi wa 'alā isḥāq(a), wa min żurriyyatihimā muḥsinuw wa ẓālimul linafsihī mubīn(un)."
  },
  {
    "verse_number": 114,
    "arabic": "وَلَقَدْ مَنَنَّا عَلَىٰ مُوسَىٰ وَهَـٰرُونَ",
    "transcription": "Wa laqad manannā 'alā mūsā wa hārūn(a)."
  },
  {
    "verse_number": 115,
    "arabic": "وَنَجَّيْنَـٰهُمَا وَقَوْمَهُمَا مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ",
    "transcription": "Wa najjaināhumā wa qaumahumā minal-karbil-'aẓīm(i)."
  },
  {
    "verse_number": 116,
    "arabic": "وَنَصَرْنَـٰهُمْ فَكَانُوا۟ هُمُ ٱلْغَـٰلِبِينَ",
    "transcription": "Wa naṣarnāhum fakānū humul-gālibīn(a)."
  },
  {
    "verse_number": 117,
    "arabic": "وَءَاتَيْنَـٰهُمَا ٱلْكِتَـٰبَ ٱلْمُسْتَبِينَ",
    "transcription": "Wa ātaināhumal-kitābal-mustabīn(a)."
  },
  {
    "verse_number": 118,
    "arabic": "وَهَدَيْنَـٰهُمَا ٱلصِّرَٰطَ ٱلْمُسْتَقِيمَ",
    "transcription": "Wa hadaināhumaṣ-ṣirāṭal-mustaqīm(a)."
  },
  {
    "verse_number": 119,
    "arabic": "وَتَرَكْنَا عَلَيْهِمَا فِى ٱلْـَٔاخِرِينَ",
    "transcription": "Wa taraknā 'alaihimā fil-ākhirīn(a)."
  },
  {
    "verse_number": 120,
    "arabic": "سَلَـٰمٌ عَلَىٰ مُوسَىٰ وَهَـٰرُونَ",
    "transcription": "Salāmun 'alā mūsā wa hārūn(a)."
  },
  {
    "verse_number": 121,
    "arabic": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "transcription": "Innā każālika najzil-muḥsinīn(a)."
  },
  {
    "verse_number": 122,
    "arabic": "إِنَّهُمَا مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
    "transcription": "Innahumā min 'ibādinal-mu'minīn(a)."
  },
  {
    "verse_number": 123,
    "arabic": "وَإِنَّ إِلْيَاسَ لَمِنَ ٱلْمُرْسَلِينَ",
    "transcription": "Wa inna ilyāsa laminal-mursalīn(a)."
  },
  {
    "verse_number": 124,
    "arabic": "إِذْ قَالَ لِقَوْمِهِۦٓ أَلَا تَتَّقُونَ",
    "transcription": "Iż qāla liqaumihī alā tattaqūn(a)."
  },
  {
    "verse_number": 125,
    "arabic": "أَتَدْعُونَ بَعْلًا وَتَذَرُونَ أَحْسَنَ ٱلْخَـٰلِقِينَ",
    "transcription": "Atad'ūna ba'law wa tażarūna aḥsanal-khāliqīn(a)."
  },
  {
    "verse_number": 126,
    "arabic": "ٱللَّهَ رَبَّكُمْ وَرَبَّ ءَابَآئِكُمُ ٱلْأَوَّلِينَ",
    "transcription": "Allāha rabbakum wa rabba ābā'ikumul-awwalīn(a)."
  },
  {
    "verse_number": 127,
    "arabic": "فَكَذَّبُوهُ فَإِنَّهُمْ لَمُحْضَرُونَ",
    "transcription": "Fakażżabūhu fa'innahum lamuḥḍarūn(a)."
  },
  {
    "verse_number": 128,
    "arabic": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "transcription": "Illā 'ibādallāhil-mukhlaṣīn(a)."
  },
  {
    "verse_number": 129,
    "arabic": "وَتَرَكْنَا عَلَيْهِ فِى ٱلْـَٔاخِرِينَ",
    "transcription": "Wa taraknā 'alaihi fil-ākhirīn(a)."
  },
  {
    "verse_number": 130,
    "arabic": "سَلَـٰمٌ عَلَىٰٓ إِلْ يَاسِينَ",
    "transcription": "Salāmun 'alā ilyāsīn(a)."
  },
  {
    "verse_number": 131,
    "arabic": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "transcription": "Innā każālika najzil-muḥsinīn(a)."
  },
  {
    "verse_number": 132,
    "arabic": "إِنَّهُۥ مِنْ عِبَادِنَا ٱلْمُؤْمِنِينَ",
    "transcription": "Innahū min 'ibādinal-mu'minīn(a)."
  },
  {
    "verse_number": 133,
    "arabic": "وَإِنَّ لُوطًا لَّمِنَ ٱلْمُرْسَلِينَ",
    "transcription": "Wa inna lūṭal laminal-mursalīn(a)."
  },
  {
    "verse_number": 134,
    "arabic": "إِذْ نَجَّيْنَـٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ",
    "transcription": "Iż najjaināhu wa ahlahū ajma'īn(a)."
  },
  {
    "verse_number": 135,
    "arabic": "إِلَّا عَجُوزًا فِى ٱلْغَـٰبِرِينَ",
    "transcription": "Illā 'ajūzan fil-gābirīn(a)."
  },
  {
    "verse_number": 136,
    "arabic": "ثُمَّ دَمَّرْنَا ٱلْـَٔاخَرِينَ",
    "transcription": "Ṡumma dammarnal-ākharīn(a)."
  },
  {
    "verse_number": 137,
    "arabic": "وَإِنَّكُمْ لَتَمُرُّونَ عَلَيْهِم مُّصْبِحِينَ",
    "transcription": "Wa innakum latamurrūna 'alaihim muṣbiḥīn(a)."
  },
  {
    "verse_number": 138,
    "arabic": "وَبِٱلَّيْلِ ۗ أَفَلَا تَعْقِلُونَ",
    "transcription": "Wa bil-lail(i), afalā ta'qilūn(a)."
  },
  {
    "verse_number": 139,
    "arabic": "وَإِنَّ يُونُسَ لَمِنَ ٱلْمُرْسَلِينَ",
    "transcription": "Wa inna yūnusa laminal-mursalīn(a)."
  },
  {
    "verse_number": 140,
    "arabic": "إِذْ أَبَقَ إِلَى ٱلْفُلْكِ ٱلْمَشْحُونِ",
    "transcription": "Iż abaqa ilal-fulkil-masyḥūn(i)."
  },
  {
    "verse_number": 141,
    "arabic": "فَسَاهَمَ فَكَانَ مِنَ ٱلْمُدْحَضِينَ",
    "transcription": "Fa sāhama fakāna minal-mudḥaḍīn(a)."
  },
  {
    "verse_number": 142,
    "arabic": "فَٱلْتَقَمَهُ ٱلْحُوتُ وَهُوَ مُلِيمٌ",
    "transcription": "Faltaqamahul-ḥūtu wa huwa mulīm(un)."
  },
  {
    "verse_number": 143,
    "arabic": "فَلَوْلَآ أَنَّهُۥ كَانَ مِنَ ٱلْمُسَبِّحِينَ",
    "transcription": "Falau lā annahū kāna minal-musabbiḥīn(a)."
  },
  {
    "verse_number": 144,
    "arabic": "لَلَبِثَ فِى بَطْنِهِۦٓ إِلَىٰ يَوْمِ يُبْعَثُونَ",
    "transcription": "Lalabiṡa fī baṭnihī ilā yaumi yub'aṡūn(a)."
  },
  {
    "verse_number": 145,
    "arabic": "۞ فَنَبَذْنَـٰهُ بِٱلْعَرَآءِ وَهُوَ سَقِيمٌ",
    "transcription": "Fa nabażnāhu bil-'arā'i wa huwa saqīm(un)."
  },
  {
    "verse_number": 146,
    "arabic": "وَأَنۢبَتْنَا عَلَيْهِ شَجَرَةً مِّن يَقْطِينٍ",
    "transcription": "Wa ambatnā 'alaihi syajaratam miy yaqṭīn(in)."
  },
  {
    "verse_number": 147,
    "arabic": "وَأَرْسَلْنَـٰهُ إِلَىٰ مِا۟ئَةِ أَلْفٍ أَوْ يَزِيدُونَ",
    "transcription": "Wa arsalnāhu ilā mi'ati alfin au yazīdūn(a)."
  },
  {
    "verse_number": 148,
    "arabic": "فَـَٔامَنُوا۟ فَمَتَّعْنَـٰهُمْ إِلَىٰ حِينٍ",
    "transcription": "Fa'āmanū famatta'nāhum ilā ḥīn(in)."
  },
  {
    "verse_number": 149,
    "arabic": "فَٱسْتَفْتِهِمْ أَلِرَبِّكَ ٱلْبَنَاتُ وَلَهُمُ ٱلْبَنُونَ",
    "transcription": "Fastaftihim alirabbikal-banātu wa lahumul-banūn(a)."
  },
  {
    "verse_number": 150,
    "arabic": "أَمْ خَلَقْنَا ٱلْمَلَـٰٓئِكَةَ إِنَـٰثًا وَهُمْ شَـٰهِدُونَ",
    "transcription": "Am khalaqnal-malā'ikata ināṡaw wa hum syāhidūn(a)."
  },
  {
    "verse_number": 151,
    "arabic": "أَلَآ إِنَّهُم مِّنْ إِفْكِهِمْ لَيَقُولُونَ",
    "transcription": "Alā innahum min ifkihim layaqūlūn(a)."
  },
  {
    "verse_number": 152,
    "arabic": "وَلَدَ ٱللَّهُ وَإِنَّهُمْ لَكَـٰذِبُونَ",
    "transcription": "Waladallāh(u), wa innahum lakāżibūn(a)."
  },
  {
    "verse_number": 153,
    "arabic": "أَصْطَفَى ٱلْبَنَاتِ عَلَى ٱلْبَنِينَ",
    "transcription": "Aṣṭafal-banāti 'alal-banīn(a)."
  },
  {
    "verse_number": 154,
    "arabic": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ",
    "transcription": "Mā lakum, kaifa taḥkumūn(a)."
  },
  {
    "verse_number": 155,
    "arabic": "أَفَلَا تَذَكَّرُونَ",
    "transcription": "Afalā tażakkarūn(a)."
  },
  {
    "verse_number": 156,
    "arabic": "أَمْ لَكُمْ سُلْطَـٰنٌ مُّبِينٌ",
    "transcription": "Am lakum sulṭānum mubīn(un)."
  },
  {
    "verse_number": 157,
    "arabic": "فَأْتُوا۟ بِكِتَـٰبِكُمْ إِن كُنتُمْ صَـٰدِقِينَ",
    "transcription": "Fa'tū bikitābikum in kuntum ṣādiqīn(a)."
  },
  {
    "verse_number": 158,
    "arabic": "وَجَعَلُوا۟ بَيْنَهُۥ وَبَيْنَ ٱلْجِنَّةِ نَسَبًا ۚ وَلَقَدْ عَلِمَتِ ٱلْجِنَّةُ إِنَّهُمْ لَمُحْضَرُونَ",
    "transcription": "Wa ja'alū bainahū wa bainal-jinnati nasabā(n), wa laqad 'alimatil-jinnatu innahum lamuḥḍarūn(a)."
  },
  {
    "verse_number": 159,
    "arabic": "سُبْحَـٰنَ ٱللَّهِ عَمَّا يَصِفُونَ",
    "transcription": "Subḥānallāhi 'ammā yaṣifūn(a)."
  },
  {
    "verse_number": 160,
    "arabic": "إِلَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "transcription": "Illā 'ibādallāhil-mukhlaṣīn(a)."
  },
  {
    "verse_number": 161,
    "arabic": "فَإِنَّكُمْ وَمَا تَعْبُدُونَ",
    "transcription": "Fa'innakum wa mā ta'budūn(a)."
  },
  {
    "verse_number": 162,
    "arabic": "مَآ أَنتُمْ عَلَيْهِ بِفَـٰتِنِينَ",
    "transcription": "Mā antum 'alaihi bifātinīn(a)."
  },
  {
    "verse_number": 163,
    "arabic": "إِلَّا مَنْ هُوَ صَالِ ٱلْجَحِيمِ",
    "transcription": "Illā man huwa ṣālil-jaḥīm(i)."
  },
  {
    "verse_number": 164,
    "arabic": "وَمَا مِنَّآ إِلَّا لَهُۥ مَقَامٌ مَّعْلُومٌ",
    "transcription": "Wa mā minnā illā lahū maqāmum ma'lūm(un)."
  },
  {
    "verse_number": 165,
    "arabic": "وَإِنَّا لَنَحْنُ ٱلصَّآفُّونَ",
    "transcription": "Wa innā lanaḥnuṣ-ṣāffūn(a)."
  },
  {
    "verse_number": 166,
    "arabic": "وَإِنَّا لَنَحْنُ ٱلْمُسَبِّحُونَ",
    "transcription": "Wa innā lanaḥnul-musabbiḥūn(a)."
  },
  {
    "verse_number": 167,
    "arabic": "وَإِن كَانُوا۟ لَيَقُولُونَ",
    "transcription": "Wa in kānū layaqūlūn(a)."
  },
  {
    "verse_number": 168,
    "arabic": "لَوْ أَنَّ عِندَنَا ذِكْرًا مِّنَ ٱلْأَوَّلِينَ",
    "transcription": "Lau anna 'indanā żikram minal-awwalīn(a)."
  },
  {
    "verse_number": 169,
    "arabic": "لَكُنَّا عِبَادَ ٱللَّهِ ٱلْمُخْلَصِينَ",
    "transcription": "Lakunnā 'ibādallāhil-mukhlaṣīn(a)."
  },
  {
    "verse_number": 170,
    "arabic": "فَكَفَرُوا۟ بِهِۦ ۖ فَسَوْفَ يَعْلَمُونَ",
    "transcription": "Fa kafarū bih(ī), fasaufa ya'lamūn(a)."
  },
  {
    "verse_number": 171,
    "arabic": "وَلَقَدْ سَبَقَتْ كَلِمَتُنَا لِعِبَادِنَا ٱلْمُرْسَلِينَ",
    "transcription": "Wa laqad sabaqat kalimatunā li'ibādinal-mursalīn(a)."
  },
  {
    "verse_number": 172,
    "arabic": "إِنَّهُمْ لَهُمُ ٱلْمَنصُورُونَ",
    "transcription": "Innahum lahumul-manṣūrūn(a)."
  },
  {
    "verse_number": 173,
    "arabic": "وَإِنَّ جُندَنَا لَهُمُ ٱلْغَـٰلِبُونَ",
    "transcription": "Wa inna jundanā lahumul-gālibūn(a)."
  },
  {
    "verse_number": 174,
    "arabic": "فَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ",
    "transcription": "Fatawalla 'anhum ḥattā ḥīn(in)."
  },
  {
    "verse_number": 175,
    "arabic": "وَأَبْصِرْهُمْ فَسَوْفَ يُبْصِرُونَ",
    "transcription": "Wa abṣirhum, fa saufa yubṣirūn(a)."
  },
  {
    "verse_number": 176,
    "arabic": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ",
    "transcription": "Afabi'ażābinā yasta'jilūn(a)."
  },
  {
    "verse_number": 177,
    "arabic": "فَإِذَا نَزَلَ بِسَاحَتِهِمْ فَسَآءَ صَبَاحُ ٱلْمُنذَرِينَ",
    "transcription": "Fa iżā nazala bisāḥatihim fa sā'a ṣabāḥul-munżarīn(a)."
  },
  {
    "verse_number": 178,
    "arabic": "وَتَوَلَّ عَنْهُمْ حَتَّىٰ حِينٍ",
    "transcription": "Wa tawalla 'anhum ḥattā ḥīn(in)."
  },
  {
    "verse_number": 179,
    "arabic": "وَأَبْصِرْ فَسَوْفَ يُبْصِرُونَ",
    "transcription": "Wa abṣir, fasaufa yubṣirūn(a)."
  },
  {
    "verse_number": 180,
    "arabic": "سُبْحَـٰنَ رَبِّكَ رَبِّ ٱلْعِزَّةِ عَمَّا يَصِفُونَ",
    "transcription": "Subḥāna rabbika rabbil-'izzati 'ammā yaṣifūn(a)."
  },
  {
    "verse_number": 181,
    "arabic": "وَسَلَـٰمٌ عَلَى ٱلْمُرْسَلِينَ",
    "transcription": "Wa salāmun 'alal-mursalīn(a)."
  },
  {
    "verse_number": 182,
    "arabic": "وَٱلْحَمْدُ لِلَّهِ رَبِّ ٱلْعَـٰلَمِينَ",
    "transcription": "Wal-ḥamdu lillāhi rabbil-'ālamīn(a)."
  }
]