[
  {
    "verse_number": 1,
    "text_uthmani": " وَٱلْمُرْسَلَـٰتِ عُرْفًا",
    "text_uthmani_transcribed": "Wal-mursalāti 'urfā(n)."
  },
  {
    "verse_number": 2,
    "text_uthmani": "فَٱلْعَـٰصِفَـٰتِ عَصْفًا",
    "text_uthmani_transcribed": "Fal-'āṣifāti 'aṣfā(n)."
  },
  {
    "verse_number": 3,
    "text_uthmani": "وَٱلنَّـٰشِرَٰتِ نَشْرًا",
    "text_uthmani_transcribed": "Wan-nāsyirāti nasyrā(n)."
  },
  {
    "verse_number": 4,
    "text_uthmani": "فَٱلْفَـٰرِقَـٰتِ فَرْقًا",
    "text_uthmani_transcribed": "Fal-fāriqāti farqā(n)."
  },
  {
    "verse_number": 5,
    "text_uthmani": "فَٱلْمُلْقِيَـٰتِ ذِكْرًا",
    "text_uthmani_transcribed": "Fal-mulqiyāti żikrā(n)."
  },
  {
    "verse_number": 6,
    "text_uthmani": "عُذْرًا أَوْ نُذْرًا",
    "text_uthmani_transcribed": "'Użran au nużrā(n)."
  },
  {
    "verse_number": 7,
    "text_uthmani": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌ",
    "text_uthmani_transcribed": "Innamā tū'adūna lawāqi'(un)."
  },
  {
    "verse_number": 8,
    "text_uthmani": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ",
    "text_uthmani_transcribed": "Fa iżan-nujūmu ṭumisat."
  },
  {
    "verse_number": 9,
    "text_uthmani": "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ",
    "text_uthmani_transcribed": "Wa iżas-samā'u furijat."
  },
  {
    "verse_number": 10,
    "text_uthmani": "وَإِذَا ٱلْجِبَالُ نُسِفَتْ",
    "text_uthmani_transcribed": "Wa iżal-jibālu nusifat."
  },
  {
    "verse_number": 11,
    "text_uthmani": "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ",
    "text_uthmani_transcribed": "Wa iżar-rusulu uqqitat."
  },
  {
    "verse_number": 12,
    "text_uthmani": "لِأَىِّ يَوْمٍ أُجِّلَتْ",
    "text_uthmani_transcribed": "Li'ayyi yaumin ujjilat."
  },
  {
    "verse_number": 13,
    "text_uthmani": "لِيَوْمِ ٱلْفَصْلِ",
    "text_uthmani_transcribed": "Liyaumil-faṣl(i)."
  },
  {
    "verse_number": 14,
    "text_uthmani": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ",
    "text_uthmani_transcribed": "Wa mā adrāka mā yaumul-faṣl(i)."
  },
  {
    "verse_number": 15,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 16,
    "text_uthmani": "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ",
    "text_uthmani_transcribed": "Alam nuhlikil-awwalīn(a)."
  },
  {
    "verse_number": 17,
    "text_uthmani": "ثُمَّ نُتْبِعُهُمُ ٱلْـَٔاخِرِينَ",
    "text_uthmani_transcribed": "Ṡumma nutbi'uhumul-ākhirīn(a)."
  },
  {
    "verse_number": 18,
    "text_uthmani": "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
    "text_uthmani_transcribed": "Każālika naf'alu bil-mujrimīn(a)."
  },
  {
    "verse_number": 19,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 20,
    "text_uthmani": "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍ مَّهِينٍ",
    "text_uthmani_transcribed": "Alam nakhluqkum mim mā'im mahīn(in)."
  },
  {
    "verse_number": 21,
    "text_uthmani": "فَجَعَلْنَـٰهُ فِى قَرَارٍ مَّكِينٍ",
    "text_uthmani_transcribed": "Fa ja'alnāhu fī qarārim makīn(in)."
  },
  {
    "verse_number": 22,
    "text_uthmani": "إِلَىٰ قَدَرٍ مَّعْلُومٍ",
    "text_uthmani_transcribed": "Ilā qadarim ma'lūm(in)."
  },
  {
    "verse_number": 23,
    "text_uthmani": "فَقَدَرْنَا فَنِعْمَ ٱلْقَـٰدِرُونَ",
    "text_uthmani_transcribed": "Fa qadarnā, fani'mal-qādirūn(a)."
  },
  {
    "verse_number": 24,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 25,
    "text_uthmani": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا",
    "text_uthmani_transcribed": "Alam naj'alil-arḍa kifātā(n)."
  },
  {
    "verse_number": 26,
    "text_uthmani": "أَحْيَآءً وَأَمْوَٰتًا",
    "text_uthmani_transcribed": "Aḥyā'aw wa amwātā(n)."
  },
  {
    "verse_number": 27,
    "text_uthmani": "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَـٰمِخَـٰتٍ وَأَسْقَيْنَـٰكُم مَّآءً فُرَاتًا",
    "text_uthmani_transcribed": "Wa ja'alnā fīhā rawāsiya syāmikhātiw wa asqainākum mā'an furātā(n)."
  },
  {
    "verse_number": 28,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 29,
    "text_uthmani": "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
    "text_uthmani_transcribed": "Inṭaliqū ilā mā kuntum bihī tukażżibūn(a)."
  },
  {
    "verse_number": 30,
    "text_uthmani": "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّ ذِى ثَلَـٰثِ شُعَبٍ",
    "text_uthmani_transcribed": "Inṭaliqū ilā ẓillin żī ṡalāṡi syu'ab(in)."
  },
  {
    "verse_number": 31,
    "text_uthmani": "لَّا ظَلِيلٍ وَلَا يُغْنِى مِنَ ٱللَّهَبِ",
    "text_uthmani_transcribed": "Lā ẓalīliw wa lā yugnī minal-lahab(i)."
  },
  {
    "verse_number": 32,
    "text_uthmani": "إِنَّهَا تَرْمِى بِشَرَرٍ كَٱلْقَصْرِ",
    "text_uthmani_transcribed": "Innahā tarmī bisyararin kal-qaṣr(i)."
  },
  {
    "verse_number": 33,
    "text_uthmani": "كَأَنَّهُۥ جِمَـٰلَتٌ صُفْرٌ",
    "text_uthmani_transcribed": "Ka'annahū jimālatun ṣufr(un)."
  },
  {
    "verse_number": 34,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 35,
    "text_uthmani": "هَـٰذَا يَوْمُ لَا يَنطِقُونَ",
    "text_uthmani_transcribed": "Hāżā yaumu lā yanṭiqūn(a)."
  },
  {
    "verse_number": 36,
    "text_uthmani": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
    "text_uthmani_transcribed": "Wa lā yu'żanu lahum fa ya'tażirūn(a)."
  },
  {
    "verse_number": 37,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 38,
    "text_uthmani": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَـٰكُمْ وَٱلْأَوَّلِينَ",
    "text_uthmani_transcribed": "Hāżā yaumul-faṣli jama'nākum wal-awwalīn(a)."
  },
  {
    "verse_number": 39,
    "text_uthmani": "فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ",
    "text_uthmani_transcribed": "Fa'in kāna lakum kaidun fakīdūn(i)."
  },
  {
    "verse_number": 40,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 41,
    "text_uthmani": "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَـٰلٍ وَعُيُونٍ",
    "text_uthmani_transcribed": "Innal-muttaqīna fī ẓilāliw wa 'uyūn(in)."
  },
  {
    "verse_number": 42,
    "text_uthmani": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ",
    "text_uthmani_transcribed": "Wa fawākiha mimmā yasytahūn(a)."
  },
  {
    "verse_number": 43,
    "text_uthmani": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
    "text_uthmani_transcribed": "Kulū wasyrabū hanī'am bimā kuntum ta'malūn(a)."
  },
  {
    "verse_number": 44,
    "text_uthmani": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
    "text_uthmani_transcribed": "Innā każālika najzil-muḥsinīn(a)."
  },
  {
    "verse_number": 45,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 46,
    "text_uthmani": "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ",
    "text_uthmani_transcribed": "Kulū wa tamatta'ū qalīlan innakum mujrimūn(a)."
  },
  {
    "verse_number": 47,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 48,
    "text_uthmani": "وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ",
    "text_uthmani_transcribed": "Wa iżā qīla lahumurka'ū lā yarka'ūn(a)."
  },
  {
    "verse_number": 49,
    "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
    "text_uthmani_transcribed": "Wailuy yauma'iżil lil-mukażżibīn(a)."
  },
  {
    "verse_number": 50,
    "text_uthmani": "فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ",
    "text_uthmani_transcribed": "Fa bi'ayyi ḥadīṡim ba'dahū yu'minūn(a)."
  }
]