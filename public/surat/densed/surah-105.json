[
  {
    "verse_number": 1,
    "text_uthmani": " أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَـٰبِ ٱلْفِيلِ",
    "text_uthmani_transcribed": "Alam tara kaifa fa'ala rabbuka bi'aṣḥābil-fīl(i)."
  },
  {
    "verse_number": 2,
    "text_uthmani": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِى تَضْلِيلٍ",
    "text_uthmani_transcribed": "Alam yaj'al kaidahum fī taḍlīl(in)."
  },
  {
    "verse_number": 3,
    "text_uthmani": "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ",
    "text_uthmani_transcribed": "Wa arsala 'alaihim ṭairan abābīl(a)."
  },
  {
    "verse_number": 4,
    "text_uthmani": "تَرْمِيهِم بِحِجَارَةٍ مِّن سِجِّيلٍ",
    "text_uthmani_transcribed": "Tarmīhim biḥijāratim min sijjīl(in)."
  },
  {
    "verse_number": 5,
    "text_uthmani": "فَجَعَلَهُمْ كَعَصْفٍ مَّأْكُولٍۭ",
    "text_uthmani_transcribed": "Fa ja'alahum ka'aṣfim ma'kūl(in)."
  }
]