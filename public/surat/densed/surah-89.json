[
  {
    "verse_number": 1,
    "arabic": " وَٱلْفَجْرِ",
    "transcription": "Wal-fajr(i)."
  },
  {
    "verse_number": 2,
    "arabic": "وَلَيَالٍ عَشْرٍ",
    "transcription": "Wa layālin 'asyr(in)."
  },
  {
    "verse_number": 3,
    "arabic": "وَٱلشَّفْعِ وَٱلْوَتْرِ",
    "transcription": "Wasy-syaf'i wal-watr(i)."
  },
  {
    "verse_number": 4,
    "arabic": "وَٱلَّيْلِ إِذَا يَسْرِ",
    "transcription": "Wal-laili iżā yasr(i)."
  },
  {
    "verse_number": 5,
    "arabic": "هَلْ فِى ذَٰلِكَ قَسَمٌ لِّذِى حِجْرٍ",
    "transcription": "Hal fī żālika qasamul liżī ḥijr(in)."
  },
  {
    "verse_number": 6,
    "arabic": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
    "transcription": "Alam tara kaifa fa'ala rabbuka bi'ād(in)."
  },
  {
    "verse_number": 7,
    "arabic": "إِرَمَ ذَاتِ ٱلْعِمَادِ",
    "transcription": "Irama żātil-'imād(i)."
  },
  {
    "verse_number": 8,
    "arabic": "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَـٰدِ",
    "transcription": "Allatī lam yukhlaq miṡluhā fil-bilād(i)."
  },
  {
    "verse_number": 9,
    "arabic": "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ",
    "transcription": "Wa ṡamūdal-lażīna jābuṣ-ṣakhra bil-wād(i)."
  },
  {
    "verse_number": 10,
    "arabic": "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ",
    "transcription": "Wa fir'auna żil-autād(i)."
  },
  {
    "verse_number": 11,
    "arabic": "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَـٰدِ",
    "transcription": "Allażīna ṭagau fil-bilād(i)."
  },
  {
    "verse_number": 12,
    "arabic": "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ",
    "transcription": "Fa akṡarū fīhal-fasād(a)."
  },
  {
    "verse_number": 13,
    "arabic": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
    "transcription": "Fa ṣabba 'alaihim rabbuka sauṭa 'ażāb(in)."
  },
  {
    "verse_number": 14,
    "arabic": "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ",
    "transcription": "Inna rabbaka labil-mirṣād(i)."
  },
  {
    "verse_number": 15,
    "arabic": "فَأَمَّا ٱلْإِنسَـٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ",
    "transcription": "Fa ammal-insānu iżā mabtalāhu rabbuhū fa akramahū wa na''amah(ū), fa yaqūlu rabbī akraman(i)."
  },
  {
    "verse_number": 16,
    "arabic": "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَـٰنَنِ",
    "transcription": "Wa ammā iżā mabtalāhu fa qadara 'alaihi rizqah(ū), fa yaqūlu rabbī ahānan(i)."
  },
  {
    "verse_number": 17,
    "arabic": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ",
    "transcription": "Kallā bal lā tukrimūnal-yatīm(a)."
  },
  {
    "verse_number": 18,
    "arabic": "وَلَا تَحَـٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
    "transcription": "Wa lā taḥāḍḍūna 'alā ṭa'āmil-miskīn(i)."
  },
  {
    "verse_number": 19,
    "arabic": "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًا لَّمًّا",
    "transcription": "Wa ta'kulūnat-turāṡa aklal lammā(n)."
  },
  {
    "verse_number": 20,
    "arabic": "وَتُحِبُّونَ ٱلْمَالَ حُبًّا جَمًّا",
    "transcription": "Wa tuḥibbūnal-māla ḥubban jammā(n)."
  },
  {
    "verse_number": 21,
    "arabic": "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّا دَكًّا",
    "transcription": "Kallā iżā dukkatil-arḍu dakkan dakkā(n)."
  },
  {
    "verse_number": 22,
    "arabic": "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّا صَفًّا",
    "transcription": "Wa jā'a rabbuka wal-malaku ṣaffan ṣaffā(n)."
  },
  {
    "verse_number": 23,
    "arabic": "وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ ٱلْإِنسَـٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ",
    "transcription": "Wa jī'a yauma'iżim bijahannam(a), yauma'iżiy yatażakkarul-insānu wa annā lahuż-żikrā."
  },
  {
    "verse_number": 24,
    "arabic": "يَقُولُ يَـٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى",
    "transcription": "Yaqūlu yā laitanī qaddamtu liḥayātī."
  },
  {
    "verse_number": 25,
    "arabic": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌ",
    "transcription": "Fa yauma'iżil lā yu'ażżibu 'ażābahū aḥad(un)."
  },
  {
    "verse_number": 26,
    "arabic": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌ",
    "transcription": "Wa lā yūṡiqu waṡāqahū aḥad(un)."
  },
  {
    "verse_number": 27,
    "arabic": "يَـٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ",
    "transcription": "Yā ayyatuhan-nafsul-muṭma'innah(tu)."
  },
  {
    "verse_number": 28,
    "arabic": "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً",
    "transcription": "Irji'ī ilā rabbiki rāḍiyatam marḍiyyah(tan)."
  },
  {
    "verse_number": 29,
    "arabic": "فَٱدْخُلِى فِى عِبَـٰدِى",
    "transcription": "Fadkhuli fī 'ibādī."
  },
  {
    "verse_number": 30,
    "arabic": "وَٱدْخُلِى جَنَّتِى",
    "transcription": "Wadkhulī jannatī."
  }
]