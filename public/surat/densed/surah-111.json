[
  {
    "verse_number": 1,
    "arabic": " تَبَّتْ يَدَآ أَبِى لَهَبٍ وَتَبَّ",
    "transcription": "Tabbat yadā abī lahabiw wa tabb(a)."
  },
  {
    "verse_number": 2,
    "arabic": "مَآ أَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ",
    "transcription": "Mā agnā 'anhu māluhū wa mā kasab(a)."
  },
  {
    "verse_number": 3,
    "arabic": "سَيَصْلَىٰ نَارًا ذَاتَ لَهَبٍ",
    "transcription": "Sayaṣlā nāran żāta lahab(in)."
  },
  {
    "verse_number": 4,
    "arabic": "وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ",
    "transcription": "Wamra'atuh(ū), ḥammālatal-ḥaṭab(i)."
  },
  {
    "verse_number": 5,
    "arabic": "فِى جِيدِهَا حَبْلٌ مِّن مَّسَدٍۭ",
    "transcription": "Fī jīdihā ḥablum mim masad(in)."
  }
