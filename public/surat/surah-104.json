      "sajdah_number": null,
      "text_uthmani": "فِى عَمَدٍ مُّمَدَّدَةٍۭ",
      "text_uthmani_tajweed": "فِى عَمَ[a:14642[دٍ م]ُّمَدَّدَةِۭ",
      "text_uthmani_tajweed_parsed": "فِى عَمَ<tajweed class='idgh_ghn' data-type='idgham-without-ghunnah' data-description='Idgham - With Ghunnah' data-tajweed=':14642'>دٍ م</tajweed>ُّمَدَّدَةِۭ",
      "text_uthmani_transcribed": "Fī 'amadim mumaddadah(tin).",
      "translations": [
        {
          "id": 90026,
          "resource_id": 19,
          "text": "In outstretched columns."
        },
        {
          "id": 370475,
          "resource_id": 79,
          "text": "(будучи) на колоннах вытянутых (которые поддерживают слои огня, которые покрывают обитателей Ада)."
        },
        {
          "id": 252088,
          "resource_id": 45,
          "text": "высокими столбами (мученики будут прикованы к этим столбам или врата Ада будут заперты и закреплены этими столбами)."
        },
        {
          "id": 145335,
          "resource_id": 27,
          "text": "in langgestreckten Säulen."
        }
