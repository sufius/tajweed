      "text_uthmani": "نَارُ ٱللَّهِ ٱلْمُوقَدَةُ",
      "text_uthmani_tajweed": "نَارُ [h:14638[ٱ]للَّهِ [h:14639[ٱ]لْمُوقَدَةُ",
      "text_uthmani_tajweed_parsed": "نَارُ <tajweed class='ham_wasl' data-type='hamza-wasl' data-description='Hamzat ul Wasl' data-tajweed=':14638'>ٱ</tajweed>للَّهِ <tajweed class='ham_wasl' data-type='hamza-wasl' data-description='Hamzat ul Wasl' data-tajweed=':14639'>ٱ</tajweed>لْمُوقَدَةُ",
      "text_uthmani_transcribed": "Nārullāhil-mūqadah(tu).",
      "translations": [
        {
          "id": 90020,
          "resource_id": 19,
          "text": "(It is) the fire of Allah, kindled,"
        },
        {
          "id": 370472,
          "resource_id": 79,
          "text": "Огонь Аллаха разожженный (по Его повелению) [Ад],"
        },
        {
          "id": 257335,
          "resource_id": 45,
          "text": "Это - разожженный Огонь Аллаха,"
        },
        {
          "id": 145356,
          "resource_id": 27,
          "text": "(Sie ist) Allahs entfachtes Feuer,"
        }
      ],
