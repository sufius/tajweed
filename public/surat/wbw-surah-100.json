      ],
      "translations": [
        {
          "id": 89935,
          "resource_id": 19,
          "text": "And scouring to the raid at dawn,"
        },
        {
          "id": 370436,
          "resource_id": 79,
          "text": "и нападающими на заре (на врагов)..."
        },
        {
          "id": 257603,
          "resource_id": 45,
          "text": "Клянусь нападающими на заре!"
        },
        {
          "id": 141819,
          "resource_id": 27,
          "text": "den am Morgen Angreifenden,"
        }
      ],
