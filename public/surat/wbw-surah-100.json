        {
          "id": 7119,
          "position": 3,
          "audio_url": "wbw/100_006_003.mp3",
          "char_type_name": "word",
          "code_v1": "ﮨ",
          "page_number": 599,
          "line_number": 14,
          "text": "ﮨ",
          "translation": {
