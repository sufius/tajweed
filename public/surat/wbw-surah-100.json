            "language_name": "english"
          },
          "transliteration": {
            "text": "inna",
            "language_name": "english"
          },
          "text_uthmani": "إِنَّ"
        },
        {
          "id": 7118,
          "position": 2,
          "audio_url": "wbw/100_006_002.mp3",
          "char_type_name": "word",
          "code_v1": "ﮧ",
          "page_number": 599,
          "line_number": 13,
          "text": "ﮧ",
          "translation": {
            "text": "mankind",
            "language_name": "english"
          },
          "transliteration": {
            "text": "l-insāna",
            "language_name": "english"
          },
          "text_uthmani": "ٱلْإِنسَـٰنَ"
