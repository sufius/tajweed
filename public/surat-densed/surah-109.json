[
  {
    "verse_number": 1,
    "text_uthmani": " قُلْ يَـٰٓأَيُّهَا ٱلْكَـٰفِرُونَ",
    "text_uthmani_transcribed": "Qul yā ayyuhal-kāfirūn(a)."
  },
  {
    "verse_number": 2,
    "text_uthmani": "لَآ أَعْبُدُ مَا تَعْبُدُونَ",
    "text_uthmani_transcribed": "Lā a'budu mā ta'budūn(a)."
  },
  {
    "verse_number": 3,
    "text_uthmani": "وَلَآ أَنتُمْ عَـٰبِدُونَ مَآ أَعْبُدُ",
    "text_uthmani_transcribed": "Wa lā antum 'ābidūna mā a'bud(u)."
  },
  {
    "verse_number": 4,
    "text_uthmani": "وَلَآ أَنَا۠ عَابِدٌ مَّا عَبَدتُّمْ",
    "text_uthmani_transcribed": "Wa lā ana 'ābidum mā 'abattum."
  },
  {
    "verse_number": 5,
    "text_uthmani": "وَلَآ أَنتُمْ عَـٰبِدُونَ مَآ أَعْبُدُ",
    "text_uthmani_transcribed": "Wa lā antum 'ābidūna mā a'bud(u)."
  },
  {
    "verse_number": 6,
    "text_uthmani": "لَكُمْ دِينُكُمْ وَلِىَ دِينِ",
    "text_uthmani_transcribed": "Lakum dīnukum wa liya dīn(i)."
  }
]