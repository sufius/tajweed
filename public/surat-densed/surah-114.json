[
  {
    "verse_number": 1,
    "text_uthmani": " قُلْ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
    "text_uthmani_transcribed": "Qul a'ūżu birabbin-nās(i)."
  },
  {
    "verse_number": 2,
    "text_uthmani": "مَلِكِ ٱلنَّاسِ",
    "text_uthmani_transcribed": "Malikin-nās(i)."
  },
  {
    "verse_number": 3,
    "text_uthmani": "إِلَـٰهِ ٱلنَّاسِ",
    "text_uthmani_transcribed": "Ilāhin-nās(i)."
  },
  {
    "verse_number": 4,
    "text_uthmani": "مِن شَرِّ ٱلْوَسْوَاسِ ٱلْخَنَّاسِ",
    "text_uthmani_transcribed": "Min syarril-waswāsil-khannās(i)."
  },
  {
    "verse_number": 5,
    "text_uthmani": "ٱلَّذِى يُوَسْوِسُ فِى صُدُورِ ٱلنَّاسِ",
    "text_uthmani_transcribed": "Allażī yuwaswisu fī ṣudūrin-nās(i)."
  },
  {
    "verse_number": 6,
    "text_uthmani": "مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ",
    "text_uthmani_transcribed": "Minal jinnati wan-nās(i)."
  }
]