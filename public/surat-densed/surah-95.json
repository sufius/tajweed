[
  {
    "verse_number": 1,
    "text_uthmani": "وَٱلتِّينِ وَٱلزَّيْتُونِ",
    "text_uthmani_transcribed": "Wat-tīni waz-zaitūn(i)."
  },
  {
    "verse_number": 2,
    "text_uthmani": "وَطُورِ سِينِينَ",
    "text_uthmani_transcribed": "Wa ṭūri sīnīn(a)."
  },
  {
    "verse_number": 3,
    "text_uthmani": "وَهَـٰذَا ٱلْبَلَدِ ٱلْأَمِينِ",
    "text_uthmani_transcribed": "Wa hāżal-baladil-amīn(i)."
  },
  {
    "verse_number": 4,
    "text_uthmani": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِىٓ أَحْسَنِ تَقْوِيمٍ",
    "text_uthmani_transcribed": "Laqad khalaqnal-insāna fī aḥsani taqwīm(in)."
  },
  {
    "verse_number": 5,
    "text_uthmani": "ثُمَّ رَدَدْنَـٰهُ أَسْفَلَ سَـٰفِلِينَ",
    "text_uthmani_transcribed": "Ṡumma radadnāhu asfala sāfilīn(a)."
  },
  {
    "verse_number": 6,
    "text_uthmani": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ",
    "text_uthmani_transcribed": "Illal-lażīna āmanū wa 'amiluṣ-ṣāliḥāti falahum ajrun gairu mamnūn(in)."
  },
  {
    "verse_number": 7,
    "text_uthmani": "فَمَا يُكَذِّبُكَ بَعْدُ بِٱلدِّينِ",
    "text_uthmani_transcribed": "Famā yukażżibuka ba'du bid-dīn(i)."
  },
  {
    "verse_number": 8,
    "text_uthmani": "أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَـٰكِمِينَ",
    "text_uthmani_transcribed": "Alaisallāhu bi'aḥkamil-ḥākimīn(a)."
  }
]