      "sajdah_number": null,
      "text_uthmani": " أَلْهَىٰكُمُ ٱلتَّكَاثُرُ",
      "text_uthmani_tajweed": "أَلْهَ[n[ـٰ]كُمُ [h:14627[ٱ][l[ل]تَّكَاثُرُ",
      "text_uthmani_tajweed_parsed": "أَلْهَ<tajweed class='madda_normal' data-type='madda-normal' data-description='Normal Prolongation: 2 Vowels'>ـٰ</tajweed>كُمُ <tajweed class='ham_wasl' data-type='hamza-wasl' data-description='Hamzat ul Wasl' data-tajweed=':14627'>ٱ</tajweed><tajweed class='slnt' data-type='laam-shamsiyah' data-description='Lam Shamsiyyah'>ل</tajweed>تَّكَاثُرُ",
      "text_uthmani_transcribed": "Alhākumut-takāṡur(u).",
      "translations": [
        {
          "id": 89980,
          "resource_id": 19,
          "text": "Rivalry in worldly increase distracteth you"
        },
        {
          "id": 370456,
          "resource_id": 79,
          "text": "Увлекла [отвлекла от повиновения Аллаху] вас (о, люди) страсть к приумножению (благ этого мира),"
        },
        {
          "id": 257405,
          "resource_id": 45,
          "text": "Страсть к приумножению (похвальба богатством и детьми) увлекает вас"
        },
        {
          "id": 140014,
          "resource_id": 27,
          "text": "Die Vermehrung lenkt euch ab,"
        },
        {
          "id": 10216288,
          "resource_id": 445,
          "text": "Страсть к накоплению добра Вас отвлекает от деяний должных"
        }
      ],
