      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "sajdah_number": null,
      "text_uthmani": "أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَـٰكِمِينَ",
      "text_uthmani_tajweed": "أَلَيْسَ [h:4087[ٱ]للَّهُ بِأَحْكَمِ [h:14558[ٱ]لْحَ[n[ـٰ]كِم[p[ِي]نَ",
      "text_uthmani_tajweed_parsed": "أَلَيْسَ <tajweed class='ham_wasl' data-type='hamza-wasl' data-description='Hamzat ul Wasl' data-tajweed=':4087'>ٱ</tajweed>للَّهُ بِأَحْكَمِ <tajweed class='ham_wasl' data-type='hamza-wasl' data-description='Hamzat ul Wasl' data-tajweed=':14558'>ٱ</tajweed>لْحَ<tajweed class='madda_normal' data-type='madda-normal' data-description='Normal Prolongation: 2 Vowels'>ـٰ</tajweed>كِم<tajweed class='madda_permissible' data-type='madda-permissible' data-description='Permissible Prolongation: 2, 4, 6 Vowels'>ِي</tajweed>نَ",
      "text_uthmani_transcribed": "Alaisallāhu bi'aḥkamil-ḥākimīn(a).",
      "translations": [
        {
          "id": 96078,
          "resource_id": 19,
          "text": "Is not Allah the most conclusive of all judges?"
        },
        {
          "id": 370393,
          "resource_id": 79,
          "text": "Разве Аллах (Который установил День Суда для расчета и воздаяния за все деяния творений) не является Самым справедливым из судей?"
        },
        {
          "id": 256361,
          "resource_id": 45,
          "text": "Разве Аллах не является Наимудрейшим Судьей?"
        },
        {
          "id": 145205,
          "resource_id": 27,
          "text": "Ist nicht Allah der Weiseste derjenigen, die richten?"
        },
        {
          "id": 9581883,
          "resource_id": 445,
          "text": "Ужель Аллах - не лучший из судей"
        }
      ],
      "verse_key": "95:8",
      "verse_number": 8,
      "words": [
